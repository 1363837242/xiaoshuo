\r\nimport \"./index.less\";\r\nimport Home1 from \"./home/index1\";\r\nimport Home from \"./home\";\r\n// const Home = React.lazy(() => import(\"./home\"));\r\n\r\nconst tabs: Array<ITabBarItem<string>> = [\r\n  {name: \"书架\", icon: bookshelfIcon, activeIcon: bookshelfActiveIcon, _key: \"书架\", path: \"/book-shelf.html\"},\r\n  {name: \"首页\", icon: homeIcon, activeIcon: homeActiveIcon, _key: \"首页\", path: \"/index.html\"},\r\n  {name: \"充值\", icon: payIcon, activeIcon: payActiveIcon, _key: \"充值\", path: \"/pay.html\"},\r\n  {name: \"我的\", icon: mineIcon, activeIcon: mineActiveIcon, _key: \"我的\", path: \"/mine.html\"},\r\n];\r\n\r\nexport const Index: React.FC<RouteChildrenProps> = (props) => {\r\n  const [homeData, setHomeData] = useHomeData();\r\n\r\n  const handleTabClick = (key: string) => {\r\n    switch (key) {\r\n      case \"书架\":\r\n        goToPageWithCustomerId(props.history, {pathname: \"/book-shelf.html\"}, props.location.pathname.indexOf(\"/index.html\") === -1);\r\n        break;\r\n      case \"首页\":\r\n        getHomeData()\r\n          .then((data) => {\r\n            setHomeData(data.data);\r\n          });\r\n        goToPageWithCustomerId(props.history, {\r\n          pathname: \"/index.html\",\r\n          state: {reload: true}\r\n        }, props.location.pathname.indexOf(\"/index.html\") === -1);\r\n        // goToPageWithCustomerId(props.history, {pathname: \"/index.html\", state: {reload: true}});\r\n        break;\r\n      case \"充值\":\r\n        goToPageWithCustomerId(props.history, {pathname: \"/pay.html\"}, props.location.pathname.indexOf(\"/index.html\") === -1);\r\n        break;\r\n      case \"我的\":\r\n        goToPageWithCustomerId(props.history, {\r\n          pathname: \"/mine.html\",\r\n          state: {reload: true}\r\n        }, props.location.pathname.indexOf(\"/index.html\") === -1);\r\n        break;\r\n    }\r\n  };\r\n\r\n  const tabsProp = {\r\n    onClick: handleTabClick,\r\n    tabs\r\n  };\r\n\r\n  return (\r\n    <div className=\"container\">\r\n      <Switch>\r\n        <Route path={\"/index.html\"} children={(router) => (\r\n          <NeedLogin {...router}>\r\n            <PersistentComp {...router} component={Home}/>\r\n          </NeedLogin>\r\n        )}/>\r\n\r\n        <Route path={\"/index1.html\"} children={(router) => (\r\n          <NeedLogin {...router}>\r\n            <PersistentComp {...router} component={Home1}/>\r\n          </NeedLogin>\r\n        )}/>\r\n\r\n        <Route path={\"/pay.html\"} children={\r\n          (router) => (\r\n            <NeedLogin {...router} component={Pay}/>\r\n          )\r\n        }/>\r\n        <Route path={\"/book-:type.html\"} children={\r\n          (router) => (\r\n            <NeedLogin {...router} component={BookShelf}/>\r\n          )\r\n        }/>\r\n        <Route path={\"/mine.html\"} children={\r\n          (router) => (\r\n            <NeedLogin {...router} component={Mine}/>\r\n          )\r\n        }/>\r\n        <Redirect path={\"\"} to={\"/index.html\"}/>\r\n      </Switch>\r\n      <MyBookManage/>\r\n      <ShouldShowTabBar {...props}>\r\n        <TabBar tabs={tabsProp.tabs} onClick={handleTabClick}/>\r\n      </ShouldShowTabBar>\r\n    </div>\r\n  )\r\n};\r\n\r\nconst alwaysShowRoute = [\r\n  \"/index.html\"\r\n];\r\n\r\nconst ShouldShowTabBar: React.FC<RouteChildrenProps> = (props) => {\r\n  const isLogin = useIsLogin()[0];\r\n\r\n  if (isLogin === true || alwaysShowRoute.indexOf(props.history.location.pathname) !== -1) {\r\n    return props.children as JSX.Element;\r\n  }\r\n\r\n  return null;\r\n};\r\n","import * as React from \"react\";\r\nimport {Fragment, useCallback, useEffect, useRef, useState} from \"react\";\r\nimport {useDocumentTitle, usePayCallbackUrl, usePrizeInfo, useTheme, useUserInfo} from \"../../hooks/Hooks\";\r\nimport {doPrize, getUserInfo, IPrizedUser, IPrizeResult} from \"../../api/UserApi\";\r\nimport {Button, Icon, Toast} from \"antd-mobile\";\r\nimport dialogIcon from \"./icons/dialog_wrapper.png\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport prizeDialogBg from \"./icons/bg.png\";\r\nimport prizeBtn from \"./icons/btn.png\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport \"./Sweepstakes.less\";\r\nimport {SERVER_ROOT} from \"../../constants/Server\";\r\n\r\nlet doing = false;\r\n\r\nexport const Sweepstakes: React.FC<RouteChildrenProps> = (props) => {\r\n  useDocumentTitle(\"幸运大转盘\");\r\n  const [prizeInfo,] = usePrizeInfo();\r\n  const [userInfo, setUserInfo] = useUserInfo();\r\n  const [currentDeg, setCurrentDeg] = useState<number>(0);\r\n  const theme = useTheme()[0] as Theme;\r\n  const [dialogOpen, setDialogOpen] = useState(false);\r\n  const [prizeAlertOpen, setPrizeAlertOpen] = useState(false);\r\n  const [doPrizeResult, setDoPrizeResult] = useState<IPrizeResult | null>(null);\r\n\r\n  const [users, setUsers] = useState<Array<IPrizedUser>>([]);\r\n  const [payCallbackUrl, setPayCallbackUrl] = usePayCallbackUrl();\r\n\r\n  const wheelRef: React.RefObject<HTMLImageElement> = useRef(null);\r\n  const listRef: React.RefObject<HTMLDivElement> = useRef(null);\r\n\r\n  useEffect(() => {\r\n    getUserInfo()\r\n      .then((data) => {\r\n        setUserInfo(data.data);\r\n      });\r\n  }, []);\r\n\r\n  useEffect(() => {\r\n    if (prizeInfo !== null) {\r\n      console.log(prizeInfo);\r\n      setUsers(prizeInfo.prized_list);\r\n    }\r\n  }, [prizeInfo]);\r\n\r\n  useEffect(() => {\r\n    if (listRef.current === null || users.length === 0) {\r\n      return;\r\n    }\r\n\r\n    const ref = listRef.current as HTMLDivElement;\r\n    const runner = setTimeout(() => {\r\n      ref.style.transition = \"all 1s\";\r\n      ref.style.marginTop = \"-.22rem\";\r\n      setTimeout(() => {\r\n        ref.style.transition = \"none\";\r\n        ref.style.marginTop = \"0\";\r\n        setTimeout(() => {\r\n          const newList = [...users];\r\n          newList.push(newList.shift() as IPrizedUser);\r\n          setUsers(newList);\r\n        }, 0);\r\n      }, 1000);\r\n    }, 1000);\r\n\r\n    return () => {\r\n      clearTimeout(runner);\r\n    }\r\n\r\n  }, [listRef, users]);\r\n\r\n  const doAction = useCallback((() => {\r\n    return () => {\r\n      if (wheelRef.current === null || prizeInfo === null || doing || userInfo === null) {\r\n        return;\r\n      }\r\n\r\n      if (userInfo.balance < 25) {\r\n        setDialogOpen(true);\r\n        return\r\n      }\r\n\r\n      doing = true;\r\n      doPrize()\r\n        .then((data) => {\r\n          setDoPrizeResult(data.data);\r\n          const prizeIndex = data.data.index;\r\n          const degPerItem = 360 / prizeInfo.prize_info.length;\r\n          const randomDeg = Math.round(Math.random() * degPerItem);\r\n          const finalDeg = 360 * 11 - prizeIndex * degPerItem - randomDeg;\r\n          const ref = wheelRef.current as HTMLImageElement;\r\n          ref.style.transition = \"all 5s cubic-bezier(0.1, 0.1, 0.1, 1)\";\r\n          ref.style.transform = `rotateZ(${finalDeg}deg)`;\r\n\r\n          setTimeout(() => {\r\n            ref.style.transition = \"none\";\r\n            ref.style.transform = `rotateZ(${finalDeg % 360}deg)`;\r\n            setCurrentDeg(finalDeg % 360);\r\n\r\n            getUserInfo()\r\n              .then((data) => {\r\n                setUserInfo(data.data);\r\n              });\r\n            if (data.data.coupon_id === 0 && data.data.coin === 0) {\r\n              Toast.info(data.data.name, 1.5, undefined, false);\r\n              setTimeout(() => {\r\n                doing = false;\r\n              }, 100);\r\n            } else {\r\n              setPrizeAlertOpen(true);\r\n              setTimeout(() => {\r\n                doing = false;\r\n              }, 100);\r\n            }\r\n          }, 5000);\r\n        });\r\n    }\r\n  })(), [wheelRef, prizeInfo, currentDeg, setDialogOpen, userInfo, setDoPrizeResult]);\r\n\r\n  return (\r\n    <div className=\"sweepstakes-container\">\r\n      {\r\n        prizeAlertOpen && doPrizeResult !== null\r\n          ? (\r\n            <div className=\"prize-dialog-wrapper\">\r\n              <div className=\"prize-dialog\">\r\n                <div onClick={() => setPrizeAlertOpen(false)} className=\"close-btn\"/>\r\n                <img src={prizeDialogBg} alt=\"\"/>\r\n                <p style={{color: \"#fff\"}}>恭喜您</p>\r\n                <p>抽中 <span style={{fontWeight: \"bold\"}}>{doPrizeResult.name}</span></p>\r\n                <div onClick={() => {\r\n                  setPrizeAlertOpen(false);\r\n                }} className=\"get-prize-btn\">\r\n                  <p>我收下了</p>\r\n                  <img src={prizeBtn} alt=\"\"/>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          )\r\n          : null\r\n      }\r\n      {dialogOpen\r\n        ? <div className=\"dialog-wrapper\">\r\n          <div className=\"dialog\">\r\n            <img className={\"pointer\"} src=\"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/24121575429180_.pic.jpg\"\r\n                 alt=\"\"/>\r\n            <img className={\"bg\"} src={dialogIcon} alt=\"\"/>\r\n            <p className=\"title\">幸运女神温馨提示</p>\r\n            <p className={\"tip\"}>书币余额不足</p>\r\n            <div className=\"buttons\">\r\n              <Button onClick={() => setDialogOpen(false)} activeStyle={{background: \"#e5e5e5\"}}\r\n                      className={\"cancel-btn\"}>离开</Button>\r\n              <Button onClick={() => {\r\n                setPayCallbackUrl(SERVER_ROOT + \"front/\" + window.location.hash);\r\n                goToPageWithCustomerId(props.history, {pathname: \"/pay.html\"})\r\n              }}\r\n                      activeStyle={{opacity: \".6\", color: \"#fff\"}} style={{background: theme.mainColor, border: \"none\"}}\r\n                      className={\"charge-btn\"} type={\"primary\"}>立刻充值</Button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        : null}\r\n      <div className=\"barrier\"></div>\r\n      <div className=\"content\">\r\n        <img alt=\"\" className=\"dress\" src={\"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/dress.png\"}/>\r\n        <div onClick={() => props.history.go(-1)} className={\"back more-touch\"}><Icon type={\"left\"}/></div>\r\n        <div className=\"my-prizes-wrapper\">\r\n          <div onClick={() => goToPageWithCustomerId(props.history, {pathname: \"/my-prizes.html\"})}\r\n               className=\"my-prizes\">\r\n            <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/gift_turn_icon.png\" alt=\"\"/>\r\n            我的奖品\r\n          </div>\r\n        </div>\r\n        <p className=\"title\">充值福利好运抽</p>\r\n        <p className=\"subtitle\">365天包年VIP等你拿</p>\r\n\r\n        <div className=\"wheel\">\r\n          {prizeInfo === null ? null : (\r\n            <Fragment>\r\n              <img ref={wheelRef} className={\"wheel-img\"} src={\"//novelsys.oss-cn-shenzhen.aliyuncs.com/front/turn.png\"}\r\n                   alt=\"\"/>\r\n              <img onClick={doAction} className={\"pointer\"}\r\n                   src=\"//qcdn-legacy.zhangzhongyun.com/static/1521708642882/img/mall/indicator.png\" alt=\"\"/>\r\n              <img onClick={doAction} className={\"pointer-outer\"}\r\n                   src=\"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/24121575429180_.pic.jpg\" alt=\"\"/>\r\n            </Fragment>\r\n          )}\r\n        </div>\r\n        <div className=\"hint\">每次抽奖消耗25书币，书币不得进行退换</div>\r\n        <Button activeStyle={{background: \"#3688ef\", color: \"#fff\"}}\r\n                style={{height: \".47rem\", fontSize: \".18rem\", background: \"#3688eff\", opacity: doing ? \".5\" : \"1\"}}\r\n                onClick={() => doAction()} className={\"do-btn\"} type={\"primary\"}>消耗25书币，开始抽奖</Button>\r\n\r\n        <div className=\"prized-user-wrapper\">\r\n          {\r\n            users !== null && users.length > 0\r\n              ? <div ref={listRef} className=\"prized-user\">\r\n                {\r\n                  users.map((user, index) => {\r\n                    return (\r\n                      <p key={index} className=\"user-item\">{user.username} 刚刚中了 {user.prize_name}</p>\r\n                    )\r\n                  })\r\n                }\r\n              </div>\r\n              : null\r\n          }\r\n        </div>\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n","import {LoadingState} from \"../pages/common/LoadMoreBottomContent\";\r\n\r\nexport const AutoMaticSetLoadingState = <T>(promise: Promise<T>, setState: (state: LoadingState) => void) => {\r\n\r\n  setState(LoadingState.LOADING);\r\n  promise.then(() => {\r\n    setState(LoadingState.LOADED);\r\n  })\r\n\r\n};\r\n","import * as React from \"react\";\r\nimport {useCallback, useEffect, useState} from \"react\";\r\nimport \"./FirstRecharge.less\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport {getFirstRechargeMoneyBtns, IMoneyFirstRechargeBtn} from \"../../api/UserApi\";\r\nimport {FutureElement} from \"../common/FutrueElement\";\r\nimport {LoadingState} from \"../common/LoadMoreBottomContent\";\r\nimport {AutoMaticSetLoadingState} from \"../../HOC/AutoMaticSetLoadingState\";\r\nimport {usePayCallbackUrl, usePayDomain, useSearchParamsProvider, useUserInfo} from \"../../hooks/Hooks\";\r\nimport {Toast} from \"antd-mobile\";\r\nimport {SERVER_ROOT} from \"../../constants/Server\";\r\n\r\nexport const FirstRecharge: React.FC<RouteChildrenProps> = (props) => {\r\n  const [btn, setBtn] = useState<IMoneyFirstRechargeBtn | null>(null);\r\n  const [loadingState, setLoadingState] = useState(LoadingState.LOADING);\r\n  const [payCallbackUrl, setPayCallbackUrl] = usePayCallbackUrl();\r\n  const [payDomain] = usePayDomain();\r\n  const [_userInfo] = useUserInfo();\r\n  const [cid] = useSearchParamsProvider();\r\n\r\n\r\n  useEffect(() => {\r\n    window.document.title