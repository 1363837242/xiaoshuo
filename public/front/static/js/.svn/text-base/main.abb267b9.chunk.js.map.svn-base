ories !== null) {\r\n        setSelectedIds(readHistories.data.map((book) => book.novel_id))\r\n      }\r\n    }\r\n  }, [bookManageState, setSelectedIds, shelf, readHistories]);\r\n\r\n  useEffect(() => {\r\n    const subId = PubSub.subscribe(BookManageEvent.BOOK_MANAGE_DELETE, (msg: BookManageEvent) => {\r\n      if (currentChose === 0) {\r\n        delNovelFromShelf(selectedIds)\r\n          .then((data) => {\r\n            setBookManageState({...bookManageState, isOpen: false, isSelectedAll: false});\r\n            if (data.err_code === ResponseCode.SUCCESS) {\r\n              Toast.success(\"删除成功\", 1.5, undefined, false);\r\n              getBookShelfList()\r\n                .then((data) => {\r\n                  setShelf(data.data);\r\n                })\r\n            }\r\n          })\r\n      } else {\r\n        delReadHistories(selectedIds)\r\n          .then((data) => {\r\n            setBookManageState({...bookManageState, isOpen: false, isSelectedAll: false});\r\n            if (data.err_code === ResponseCode.SUCCESS) {\r\n              Toast.success(\"删除成功\", 1.5, undefined, false);\r\n              getReadHistories()\r\n                .then((data) => {\r\n                  setReadHistories(data.data);\r\n                })\r\n            }\r\n          })\r\n      }\r\n    });\r\n\r\n    return () => {\r\n      PubSub.unsubscribe(subId);\r\n    }\r\n  }, [selectedIds]);\r\n\r\n  useEffect(() => {\r\n    if (!bookManageState.isSelectedAll) {\r\n      setSelectedIds([]);\r\n    }\r\n  }, [bookManageState, setSelectedIds]);\r\n\r\n\r\n  const buildItem = (index: number) => {\r\n    if (shelf === null || shelf.data.recommend[index] === undefined) {\r\n      return <Fragment></Fragment>;\r\n    }\r\n\r\n    return (\r\n      <BookItem\r\n        onClick={() => {\r\n          goToPageWithCustomerId(props.history, {pathname: `/detail/novel-${shelf.data.recommend[index].novel_id}.html`, state: {reload: true}});\r\n        }}\r\n        img={shelf.data.recommend[index].img}\r\n        name={shelf.data.recommend[index].title}\r\n      />\r\n    )\r\n  };\r\n\r\n  const handleClick = useCallback((id: number) => {\r\n    const index = selectedIds.findIndex((_id) => _id === id);\r\n    if (index === -1) {\r\n      setSelectedIds([...selectedIds, id])\r\n    } else {\r\n      const newOne = [...selectedIds];\r\n      newOne.splice(index, 1);\r\n      setSelectedIds(newOne);\r\n    }\r\n  }, [shelf, readHistories, selectedIds, setSelectedIds]);\r\n\r\n  const buildMyStoryItem = (index: number) => {\r\n    if (shelf === null || shelf.data.book_store[index] === undefined) {\r\n      return <Fragment></Fragment>;\r\n    }\r\n\r\n    const elem = (\r\n      <BookItem\r\n        extra={\r\n          bookManageState.isOpen\r\n            ? <div className=\"checked-wrapper\">\r\n              <Check onClick={() => handleClick(shelf.data.book_store[index].novel_id)} active={\r\n                selectedIds.findIndex((id) => id === shelf.data.book_store[index].novel_id) !== -1\r\n              }/></div>\r\n            : <Fragment></Fragment>\r\n        }\r\n        img={shelf.data.book_store[index].novel_img}\r\n        name={shelf.data.book_store[index].novel_title}\r\n      />\r\n    );\r\n\r\n    return elem;\r\n  };\r\n\r\n  const buildHistoryItem = (index: number) => {\r\n    if (readHistories === null) {\r\n      return <Fragment></Fragment>;\r\n    }\r\n\r\n    const data = readHistories.data;\r\n\r\n    return <BookHistoryItem\r\n      extra={\r\n        bookManageState.isOpen\r\n          ? <div className=\"checked-wrapper\">\r\n            <Check onClick={() => handleClick(readHistories.data[index].novel_id)} active={\r\n              selectedIds.findIndex((id) => id === readHistories.data[index].novel_id) !== -1\r\n            }/></div>\r\n          : <Fragment></Fragment>\r\n      }\r\n      date={data[index].updated_at}\r\n      id={data[index].novel_id}\r\n      chapter={data[index].title + data[index].name}\r\n      name={data[index].name}\r\n      img={data[index].novel === null ? \"\" : data[index].novel.img}\r\n    />\r\n  };\r\n\r\n  const toggleManageOpen = (active?: boolean) => {\r\n    if (undefined !== active) {\r\n      bookManageState.isOpen = active;\r\n    } else {\r\n      bookManageState.isOpen = !bookManageState.isOpen;\r\n    }\r\n    setBookManageState({...bookManageState});\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (!_bookManageState.isOpen) {\r\n      setBookManageState({..._bookManageState, isSelectedAll: false, isOpen: false});\r\n    }\r\n  }, [_bookManageState.isOpen]);\r\n\r\n  /**\r\n   *  初始化书籍状态操作\r\n   */\r\n  useEffect(() => {\r\n    setBookManageState({...bookManageState, isSelectedAll: false, isOpen: false});\r\n    const evtId = PubSub.subscribe(BookManageEvent.BOOK_MANAGE_DELETE, () => {\r\n      // TODO 删除事件\r\n    });\r\n\r\n    return () => {\r\n      PubSub.unsubscribe(evtId);\r\n    }\r\n  }, [currentChose]);\r\n\r\n  /**\r\n   *  设置documentTitle\r\n   */\r\n  useEffect(() => {\r\n    toggleManageOpen(false);\r\n    if (currentChose === 0) {\r\n      goToPageWithCustomerId(props.history, {pathname: \"/book-shelf.html\", search: props.location.search}, true);\r\n      window.document.title = \"我的书架\";\r\n    } else {\r\n      goToPageWithCustomerId(props.history, {pathname: \"/book-history.html\", search: props.location.search}, true);\r\n      window.document.title = \"阅读历史\";\r\n    }\r\n  }, [currentChose]);\r\n\r\n  const buildBookItem = useCallback((index: number) => {\r\n\r\n  }, [shelf]);\r\n\r\n  return (\r\n    <div className=\"book-shelf-container\">\r\n      <ServiceButton />\r\n      <Tabs\r\n        initialPage={currentChose}\r\n        onChange={(current, index) => {\r\n          setCurrentChose(index);\r\n        }}\r\n        tabs={tabs}\r\n        swipeable={false}\r\n        tabBarUnderlineStyle={{transform: \"scaleX(.4)\", borderColor: (theme as Theme).mainColor}}\r\n        tabBarActiveTextColor={(theme as Theme).mainColor}\r\n      >\r\n        <div className=\"my-shelf\">\r\n          <HomeChunk\r\n            itemPerLine={3}\r\n            itemsCount={shelf === null ? 0 : shelf.data.recommend.length}\r\n            title={\"书架推荐\"}\r\n            axis={Axis.horizontal}\r\n            itemBuilder={buildItem}\r\n            btn={\"\"}/>\r\n\r\n          <HomeChunk\r\n            extra={\r\n              bookManageState.isOpen\r\n                ? undefined\r\n                : (\r\n                  <div className=\"item-wrapper\">\r\n                    <div onClick={() => goToPageWithCustomerId(props.history, {pathname: \"/index.html\"})}\r\n                         className=\"add-more\">\r\n                      <img src={addIcon} alt=\"\"/>\r\n                    </div>\r\n                  </div>\r\n                )\r\n            }\r\n            itemPerLine={3}\r\n            itemsCount={shelf === null ? 0 : shelf.data.book_store.length}\r\n            title={\"我的小说\"}\r\n            onClick={(index) => {\r\n              if (shelf === null) {\r\n                return;\r\n              }\r\n              if (bookManageState.isOpen) {\r\n                handleClick(shelf.data.book_store[index].novel_id);\r\n                return;\r\n              }\r\n              goToPageWithCustomerId(props.history, {pathname: `/detail/novel-${shelf.data.book_store[index].novel_id}.html`, state: {reload: true}});\r\n            }}\r\n            axis={Axis.horizontal}\r\n            itemBuilder={buildMyStoryItem}\r\n            onBtnClick={() => toggleManageOpen()}\r\n            btn={bookManageState.isOpen ? \"完成\" : \"管理\"}/>\r\n\r\n        </div>\r\n        <div className=\"read-history\">\r\n          <HomeChunk\r\n            onClick={(index) => {\r\n              if (readHistories === null) {\r\n                return;\r\n              }\r\n              if (bookManageState.isOpen) {\r\n                handleClick(readHistories.data[index].novel_id);\r\n                return;\r\n              }\r\n              goToPageWithCustomerId(props.history, {\r\n                pathname: `/detail/novel-${readHistories.data[index].novel_id}.html`,\r\n                state: {reload: true}\r\n              })\r\n            }}\r\n            itemsCount={readHistories === null ? 0 : readHistories.data.length}\r\n            title={\"阅读历史\"}\r\n            axis={Axis.vertical}\r\n            itemBuilder={buildHistoryItem}\r\n            onBtnClick={() => toggleManageOpen()}\r\n            btn={bookManageState.isOpen ? \"完成\" : \"管理\"}/>\r\n\r\n        </div>\r\n      </Tabs>\r\n    </div>\r\n  )\r\n};\r\n","export interface IPersistentRouteData {\r\n  route: string;\r\n  data: any;\r\n}\r\n\r\nexport const persistentRouteData: Array<IPersistentRouteData> = [\r\n\r\n];\r\n","import {IPersistentRouteData, persistentRouteData} from \"./bus/PersitentDateBus\";\n\nexport const clearData = (route: string) => {\n  const index = persistentRouteData.findIndex(\n    (_item) => _item.route === route\n  );\n\n  if (index > -1) {\n    persistentRouteData.splice(index, 1);\n  }\n};\n\nexport const getData = (route: string) => {\n  const data = persistentRouteData.find(\n    (item) => item.route === route\n  );\n  return data === undefined ? null : data.data;\n};\n\nexport interface ISetData<T> {\n  name: string;\n  value: T\n}\n\nexport const setData = <T>(route: string, data: T) => {\n  const old = persistentRouteData.find(\n    (item) => item.route === route\n  );\n\n  if (old === undefined) {\n    persistentRouteData.push({route, data})\n  } else {\n    (old as IPersistentRouteData).data = data;\n  }\n};\n","import * as React from \"react\";\r\nimport {match, RouteChildrenProps, RouteComponentProps} from \"react-router\";\r\nimport {clearData, getData, setData} from \"./SetPersitentDataUtil\";\r\nimport {goToPageWithCustomerId} from \"../utils/RouteUtil\";\r\n\r\nexport interface IPersistentCompProps {\r\n  component?: React.ComponentType<RouteComponentProps<any> & IPersistentProps> | React.ComponentType<any>;\r\n}\r\n\r\nexport interface IPersistentProps {\r\n  persistentData?: any;\r\n  persistData: (data: any) => void;\r\n}\r\n\r\ninterface IRouteReloadState {\r\n  reload?: boolean;\r\n}\r\n\r\nexport const PersistentComp: React.FC<RouteChildrenProps & IPersistentCompProps> = (props) => {\r\n  const url = (props.match as match).url;\r\n  const {reload} = props.location.state === undefined ? {reload: false} : props.location.state as IRouteReloadState;\r\n  const Child = props.component;\r\n\r\n  if (reload) {\r\n    clearData(url);\r\n    goToPageWithCustomerId(\r\n      props.history,\r\n      {...props.location, state: {...props.location.state, reload: undefined}},\r\n      true\r\n    );\r\n    return null;\r\n  }\r\n\r\n  return Child === undefined\r\n    ? props.children as JSX.Element\r\n    : <Child\r\n      persistData={(data) => setData(url, data)}\r\n      persistentData={getData(url)}\r\n      history={props.history}\r\n      location={props.location}\r\n      match={props.match as match}/>;\r\n};\r\n","import { PostForm } from \"../utils/RequestShorcuts\";\r\n\r\nexport interface IHomeCarouselItem {\r\n  suitable_sex: number;\r\n  novel_id: number;\r\n  img: string;\r\n}\r\n\r\nexport interface IHomeChunkItemData {\r\n  author: string;\r\n  img: string;\r\n  title: string;\r\n  desc: string;\r\n  tags: string | null;\r\n  suitable_sex: number;\r\n  novel_id: number;\r\n  recommend_num?: number;\r\n}\r\n\r\nexport interface IHomeDataItem {\r\n  carousel: Array<IHomeCarouselItem>;\r\n  hot: Array<IHomeChunkItemData>;\r\n  recommend: Array<IHomeChunkItemData>;\r\n  weeknews: Array<IHomeChunkItemData>;\r\n  published: Array<IHomeChunkItemData>;\r\n  selection: Array<IHomeChunkItemData>;\r\n}\r\n\r\nexport interface IHomeData {\r\n  man: IHomeDataItem;\r\n  woman: IHomeDataItem;\r\n}\r\n\r\nexport interface IHomeMoreListItem {\r\n  novel_id: number;\r\n  img: string;\r\n  title: string;\r\n  desc: string;\r\n  tags: string | null;\r\n  suitable_sex: number;\r\n}\r\n\r\nexport interface IHomeMoreList {\r\n  last_page: number | null;\r\n  data: Array<IHomeMoreListItem>;\r\n}\r\n\r\nconst getRandomNum = () => {\r\n  let num: number | null;\r\n  return () => {\r\n    if (num !== null) {\r\n      num = Math.floor(Math.random() * (200 - 10 + 10) + 10) / 10;\r\n    }\r\n    return num;\r\n  };\r\n};\r\n\r\nexport const getHomeData = async () => {\r\n  const data = await PostForm<IHomeData>(\"/novel/indexdata\");\r\n  data.data.man.weeknews = data.data.man.weeknews.map((item) => ({\r\n    ...item,\r\n    recommend_num: getRandomNum()() as number\r\n  }));\r\n\r\n  data.data.woman.weeknews = data.data.woman.weeknews.map((item) => ({\r\n    ...item,\r\n    recommend_num: getRandomNum()() as number\r\n  }));\r\n  return data;\r\n};\r\n\r\n// 首页更多查询条件的类型\r\nexport type HomeMoreData = \"hot\" | \"recommend\" | \"weeknews\" | \"selection\" | \"published\";\r\n\r\nexport type Gender = \"man\" | \"woman\";\r\n\r\nexport const getHomeMoreData = (type: HomeMoreData, gender: Gender, page?: number) => {\r\n  return PostForm<IHomeMoreList>(\"/novel/indexdatamore\", undefined, { type, sex: gender.toString(), page });\r\n};\r\n","import * as React from \"react\";\r\nimport {goToPageWithCustomerId} from \"../../../utils/RouteUtil\";\r\nimport {useHistory} from \"react-router\";\r\nimport \"./HomeNavBar1.less\";\r\n\r\nexport interface IHomeNavBarItem<T> {\r\n  name: string;\r\n  key: T\r\n}\r\n\r\nexport interface IHomeNavBarProps<T> {\r\n  currentNav: T;\r\n  items: Array<IHomeNavBarItem<T>>;\r\n  activeColor: string | null,\r\n  onClick: (key: T) => void;\r\n}\r\n\r\nexport const HomeNavBar: React.FC<IHomeNavBarProps<string>> = (props) => {\r\n  const history = useHistory();\r\n\r\n  const handleOnClick = (key: string) => {\r\n    props.onClick(key);\r\n  };\r\n\r\n  return (\r\n    <div className=\"home-nav-bar1\">\r\n      {\r\n        props.items.map((item) => {\r\n          return (\r\n            <div key={item.key} onClick={() => handleOnClick(item.key)} className=\"home-nav-bar-item\">\r\n              <p style={{color: item.key === props.currentNav ? props.activeColor || \"#3688ff\" : \"#333333\"}}\r\n                 className={item.key === props.currentNav ? \"name-active\" : \"name\"}>{item.name}</p>\r\n            </div>\r\n          )\r\n        })\r\n      }\r\n      <div onClick={() => goToPageWithCustomerId(history, {pathname: \"/search.html\"})} className=\"search\">\r\n        <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/v2.0_search_icon.png\" alt=\"\"/>\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {Carousel} from \"antd-mobile\";\r\nimport \"./HomeCarousel.less\";\r\nimport {IHomeCarouselItem} from \"../../../api/HomeApi\";\r\nimport {useHistory} from \"react-router\";\r\nimport {goToPageWithCustomerId} from \"../../../utils/RouteUtil\";\r\n\r\nexport interface IHomeCarouselProps {\r\n  items: Array<IHomeCarouselItem>\r\n}\r\n\r\nexport const HomeCarousel: React.FC<IHomeCarouselProps> = (props) => {\r\n  const history = useHistory();\r\n\r\n  return (\r\n    <div className=\"home-carousel-container\">\r\n      <Carousel className=\"my-carousel\"\r\n                dots\r\n                autoplay\r\n                infinite\r\n      >\r\n        {props.items.map(item => (\r\n          <div\r\n            onClick={() => goToPageWithCustomerId(history, {pathname: `/detail/novel-${item.novel_id}.html`, state: {reload: true}})}\r\n            className=\"carousel-item\" key={item.novel_id}>\r\n            <img src={item.img} alt=\"\"/>\r\n          </div>\r\n        ))}\r\n      </Carousel>\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport  \"./HomeCategories1.less\";\r\n\r\nexport interface HomeCategoryItem {\r\n  key: string;\r\n  icon: string | null,\r\n  name: string,\r\n}\r\n\r\nexport interface IHomeCategoriesProps {\r\n  items: Array<HomeCategoryItem>;\r\n  onClick: (key: string) => void;\r\n}\r\n\r\nexport const HomeCategories: React.FC<IHomeCategoriesProps> = (props) => {\r\n  return (\r\n    <div className=\"home-categories1\">\r\n      {\r\n        props.items.map((item) => {\r\n          return (\r\n            <div onClick={() => props.onClick(item.key)} key={item.name} className=\"home-category-item\">\r\n              <div className=\"icon\">\r\n                <img src={item.icon || \"\"} alt=\"\"/>\r\n              </div>\r\n              <div className=\"name\">{item.name}</div>\r\n            </div>\r\n          )\r\n        })\r\n      }\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport \"./HomeBottomCategories.less\";\r\nimport {useCallback} from \"react\";\r\nimport {IBookRepositoryState} from \"../../BookRepository\";\r\n\r\ninterface IHomeBottomCategoryItem<T> {\r\n  key: T;\r\n  img: string;\r\n}\r\n\r\nexport interface IHomeBottomCategoriesProps<T> {\r\n  items: Array<IHomeBottomCategoryItem<T>>;\r\n  onClick: (key: T) => void;\r\n}\r\n\r\nexport const HomeBottomCategories: React.FC<IHomeBottomCategoriesProps<IBookRepositoryState>> = (props) => {\r\n\r\n  const handleClick = useCallback((key: IBookRepositoryState) => {\r\n    if (props.onClick !== undefined) {\r\n      props.onClick(key);\r\n    }\r\n  }, [props]);\r\n\r\n  return (\r\n    <div className=\"home-bottom-categories\">\r\n      {\r\n        props.items.map((item) => {\r\n          return (\r\n            <div onClick={() => handleClick(item.key)} key={item.key.typeId} className=\"home-bottom-category-item\">\r\n              <div className=\"img\">\r\n                <img src={item.img} alt=\"\"/>\r\n              </div>\r\n            </div>\r\n          )\r\n        })\r\n      }\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {Fragment, useCallback, useEffect, useRef, useState} from \"react\";\r\nimport {HomeNavBar} from \"./HomeNavBar1\";\r\nimport {HomeCarousel} from \"./HomeCarousel\";\r\nimport {HomeCategories, HomeCategoryItem} from \"./HomeCategories1\";\r\nimport {HomeChunk, HomeChunk1} from \"../../common/HomeChunk\";\r\nimport {Axis} from \"../../../entities/Axis\";\r\nimport {WhiteSpace, WingBlank} from \"antd-mobile\";\r\nimport {BookItem, BookItem1, BookItemVertical, BookItemVertical1} from \"../../common/BookItem\";\r\nimport {useDocumentTitle, useHomeData, useTheme, useWechatInfo} from \"../../../hooks/Hooks\";\r\nimport {Theme} from \"../../../entities/Theme\";\r\nimport {HomeBottomCategories} from \"./HomeBottomCategories\";\r\nimport {RouterProps} from \"react-router\";\r\nimport {IHomeDataItem} from \"../../../api/HomeApi\";\r\nimport {LoadingState} from \"../../common/LoadMoreBottomContent\";\r\nimport {FutureElement} from \"../../common/FutrueElement\";\r\nimport {IPersistentProps} from \"../../../HOC/PersistentComp\";\r\nimport {goToPageWithCustomerId} from \"../../../utils/RouteUtil\";\r\nimport \"./index1.less\";\r\n\r\nconst navs = {\r\n  items: [\r\n    {name: \"男生\", key: \"man\"},\r\n    {name: \"女生\", key: \"woman\"},\r\n  ],\r\n  onClick: (key: string) => {\r\n    console.log(\"点击了nav\");\r\n  }\r\n};\r\n\r\nconst carouselItem: Array<string> = [\r\n  \"https://qcdn.zhangzhongyun.com/novel_banners/15744269264748.jpg?imageView2/0/w/570/q/75\",\r\n  \"https://qcdn.zhangzhongyun.com/novel_banners/15744269264748.jpg?imageView2/0/w/570/q/75\",\r\n  \"https://qcdn.zhangzhongyun.com/novel_banners/15744269264748.jpg?imageView2/0/w/570/q/75\",\r\n  \"https://qcdn.zhangzhongyun.com/novel_banners/15744269264748.jpg?imageView2/0/w/570/q/75\",\r\n  \"https://qcdn.zhangzhongyun.com/novel_banners/15744269264748.jpg?imageView2/0/w/570/q/75\",\r\n  \"https://qcdn.zhangzhongyun.com/novel_banners/15744269264748.jpg?imageView2/0/w/570/q/75\",\r\n  \"https://qcdn.zhangzhongyun.com/novel_banners/15744269264748.jpg?imageView2/0/w/570/q/75\",\r\n  \"https://qcdn.zhangzhongyun.com/novel_banners/15744269264748.jpg?imageView2/0/w/570/q/75\",\r\n];\r\n\r\nconst homeCategories: Array<HomeCategoryItem> = [\r\n  {key: \"书库\", name: \"书库\", icon: \"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/v2.0_bookshelf_icon.png\"},\r\n  {key: \"榜单\", name: \"榜单\", icon: \"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/v2.0_top_icon.png\"},\r\n  {key: \"出版\", name: \"出版\", icon: \"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/v2.0_push_icon.png\"},\r\n  {key: \"活动\", name: \"活动\", icon: \"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/v2.0_activity_icon.png\"},\r\n  {key: \"限免\", name: \"限免\", icon: \"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/v2.0_limit_icon.png\"},\r\n];\r\n\r\nconst getRandomNum = () => {\r\n  let num: number | null;\r\n  return () => {\r\n    if (num !== null) {\r\n      num = Math.floor(Math.random() * (200 - 10 + 10) + 10) / 10;\r\n    }\r\n    return num;\r\n  };\r\n};\r\n\r\nconst buildVerticalItem = (index: number): JSX.Element => (\r\n  <BookItemVertical\r\n    id={0}\r\n    tags={[\"狂拽\", \"酷炫\", \"吊炸天\"]}\r\n    desc={\"这一世本非常普通小说，不建议看。这一世本非常普通小说，不建议看。这一世本非常普通小说，不建议看。\"}\r\n    img={\"https://qcdn.zhangzhongyun.com/covers/15416408304622.jpg?imageView2/0/w/300/q/75\"}\r\n    name={\"普通的小说\" + index}/>\r\n);\r\n\r\nexport interface IHomePersitentData {\r\n  scrollTop: number;\r\n  gender: string;\r\n}\r\n\r\nconst filterChars = (str: string) => {\r\n  return str.length > 40 ? str.slice(0, 37) + \"...\" : str;\r\n};\r\n\r\nconst Home1: React.FC<RouterProps & IPersistentProps> = (props) => {\r\n  useDocumentTitle(\"首页\");\r\n  const [homeData] = useHomeData();\r\n  const [currentGender, setCurrentGender] = useState(\"man\");\r\n  const [theme] = useTheme();\r\n  const [currentNav] = useState<string>(\"男生\");\r\n  const [loadingState, setLoadingState] = useState(LoadingState.LOADING);\r\n  const scrollRef = useRef<HTMLDivElement>(null);\r\n\r\n  const scrollData = props.persistentData as IHomePersitentData;\r\n  const [wechatQrcode] = useWechatInfo();\r\n\r\n  const getGender = useCallback(() => {\r\n    return currentNav === \"男生\" ? \"man\" : \"woman\";\r\n  }, [currentNav]);\r\n\r\n  const getSuiteData = useCallback(() => {\r\n    if (homeData === null) {\r\n      return null;\r\n    } else {\r\n      switch (currentGender) {\r\n        case \"man\":\r\n          return homeData.man;\r\n        case \"woman\":\r\n          return homeData.woman;\r\n      }\r\n\r\n      return null;\r\n    }\r\n  }, [currentGender, homeData]);\r\n\r\n  useEffect(() => {\r\n    if (scrollData !== null) {\r\n      if (scrollRef.current !== null) {\r\n        setTimeout(() => {\r\n          (scrollRef.current as HTMLDivElement).scrollTop = scrollData.scrollTop;\r\n        }, 0);\r\n      }\r\n\r\n      setCurrentGender(scrollData.gender);\r\n    }\r\n  }, [scrollRef, setCurrentGender]);\r\n\r\n  useEffect(() => {\r\n    if (homeData === null) {\r\n      setLoadingState(LoadingState.LOADING);\r\n    } else {\r\n      setLoadingState(LoadingState.LOADED);\r\n    }\r\n  }, [homeData]);\r\n\r\n  // 性别改变后回到顶部\r\n  useEffect(() => {\r\n    if (scrollRef.current !== null) {\r\n      (scrollRef.current as HTMLDivElement).scrollTop = 0;\r\n    }\r\n  }, [currentGender, scrollRef]);\r\n\r\n  const persisData = useCallback(() => {\r\n    props.persistData({\r\n      scrollTop: scrollRef.current === null ? 0 : (scrollRef.current as HTMLDivElement).scrollTop,\r\n      gender: currentGender\r\n    })\r\n  }, [props, scrollRef, currentGender]);\r\n\r\n  const handleScroll = useCallback((evt: React.UIEvent<HTMLDivElement>) => {\r\n    persisData();\r\n  }, [props, scrollRef, currentGender, getGender]);\r\n\r\n  useEffect(() => {\r\n    persisData();\r\n  }, [scrollRef, currentGender, getGender, persisData]);\r\n\r\n  const getItemCount = useCallback((itemName: string) => {\r\n    const data = getSuiteData() as IHomeDataItem;\r\n    if (data === null) {\r\n      return 0;\r\n    }\r\n\r\n    switch (itemName) {\r\n      case \"hot\":\r\n        return