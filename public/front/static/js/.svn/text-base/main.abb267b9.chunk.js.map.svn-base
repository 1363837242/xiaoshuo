nstants/Server\";\r\nimport axios, {AxiosInstance, AxiosRequestConfig, AxiosResponse} from \"axios\";\r\n\r\nexport interface IQuery {\r\n  [propName: string]: any;\r\n}\r\n\r\nexport interface IParams {\r\n  [propName: string]: any;\r\n}\r\n\r\nexport enum RequestMethod {\r\n  POST = \"POST\", GET = \"GET\", POST_FORM = \"POST\"\r\n}\r\n\r\nexport enum ContentType {\r\n  json = \"application/json;charset=utf-8\",\r\n  form = \"application/x-www-form-urlencoded;charset=utf-8\",\r\n  textPlain = \"text/plain;charset=utf-8\",\r\n  textHtml = \"text/html;charset=utf-8\",\r\n}\r\n\r\nexport interface IBeforeInterceptor {\r\n  (config: AxiosRequestConfig): AxiosRequestConfig;\r\n}\r\n\r\nexport interface IAfterInterceptor {\r\n  (response: AxiosResponse): AxiosResponse;\r\n}\r\n\r\nconst filterUndefined = <T extends {}>(t: T) => {\r\n  for (const key in t) {\r\n    if (t.hasOwnProperty(key)) {\r\n      if (t[key] === undefined) {\r\n        delete t[key];\r\n      }\r\n    }\r\n  }\r\n};\r\n\r\nexport const request = <T>(method: RequestMethod) => {\r\n  let defaultContentType: ContentType;\r\n  switch (method) {\r\n    case RequestMethod.GET:\r\n      defaultContentType = ContentType.textPlain;\r\n      break;\r\n    case RequestMethod.POST:\r\n      defaultContentType = ContentType.form;\r\n      break;\r\n    case RequestMethod.POST_FORM:\r\n      defaultContentType = ContentType.json;\r\n      break;\r\n  }\r\n\r\n  return (beforeInterceptors: Array<IBeforeInterceptor>, afterInterceptors: Array<IAfterInterceptor>) => {\r\n\r\n    return (url: string, query?: IQuery, params?: IParams, contentType: ContentType = defaultContentType) : Promise<T> => {\r\n\r\n      if (!url || !method) {\r\n        return Promise.reject(\"参数错误\");\r\n      }\r\n\r\n      if (query !== undefined) {\r\n        filterUndefined(query);\r\n      }\r\n\r\n      if (params !== undefined) {\r\n        filterUndefined(params);\r\n      }\r\n\r\n      const http: AxiosInstance = axios.create({\r\n        baseURL: SERVER_ROOT,\r\n        withCredentials: true,\r\n        headers: {\r\n          \"Content-Type\": contentType\r\n        }\r\n      });\r\n\r\n      http.interceptors.request.use(config => {\r\n        if (beforeInterceptors && beforeInterceptors.length > 0) {\r\n          beforeInterceptors.forEach(fn => {\r\n            if (typeof fn === \"function\") {\r\n              fn(config);\r\n            }\r\n          });\r\n        }\r\n        return config;\r\n      });\r\n\r\n      http.interceptors.response.use(response => {\r\n        if (afterInterceptors && afterInterceptors.length > 0) {\r\n          afterInterceptors.forEach(fn => {\r\n            if (typeof fn === \"function\") {\r\n              fn(response);\r\n            }\r\n          });\r\n        }\r\n        return response;\r\n      });\r\n\r\n      let paramsStr: Array<string> = [];\r\n      for (const key in params) {\r\n        if (params.hasOwnProperty(key)) {\r\n          paramsStr.push(key + \"=\" + params[key]);\r\n        }\r\n      }\r\n      const realParamsStr = paramsStr.reduce((str, curr, index) => {\r\n        if (index === 0) {\r\n          return curr;\r\n        }\r\n        return str + \"&\" + curr;\r\n      }, \"\");\r\n\r\n      return http.request<T>({\r\n        url, data: contentType === ContentType.form ? realParamsStr : params, params: query, method\r\n      }).then((data) => data.data);\r\n\r\n    }\r\n  }\r\n};\r\n","export const addToCache = (name: string, value: any) => {\r\n  window.localStorage.setItem(name, JSON.stringify(value));\r\n};\r\n\r\nexport const getCache = <R>(name: string): R | null => {\r\n  const item: string | null = window.localStorage.getItem(name);\r\n  if (null === item) {\r\n    return item;\r\n  }\r\n\r\n  return JSON.parse(item) as R;\r\n};\r\n\r\ninterface bind<R> {\r\n  (item: R): R;\r\n}\r\n\r\nexport const mixWithCache = <R>(name: string, handler: bind<R | null>) => {\r\n  const item: string | null = window.localStorage.getItem(name);\r\n\r\n  const result = handler((item === null ? null : JSON.parse(item) as R));\r\n  window.localStorage.removeItem(name);\r\n  addToCache(name, result);\r\n};\r\n","import {useDebugValue, useEffect, useState} from \"react\";\r\nimport {ChangeNotifier} from \"./Provider\";\r\nimport {addToCache} from \"../utils/LocalStorageUtil\";\r\n\r\nexport const createHook = <E>(notifier: ChangeNotifier<E>, debugValue?: string) => {\r\n  return () => {\r\n    const [val, setVal] = useState<E>(notifier.getValue());\r\n    useDebugValue(debugValue);\r\n\r\n\r\n    useEffect(() => {\r\n      const subscribeId: number = notifier.addListener(() => {\r\n        setVal(notifier.getValue());\r\n      });\r\n\r\n      return () => {\r\n        notifier.removeListener(subscribeId);\r\n      }\r\n    }, []);\r\n\r\n    const tuple: [E, (val: E) => void] = [val, (value: E) => {\r\n      notifier.setValue(value)\r\n    }];\r\n    return tuple;\r\n  }\r\n};\r\n\r\nexport const createHookWithCache = <E>(notifier: ChangeNotifier<E>, cache: string) => {\r\n  return () => {\r\n    const [val, setVal] = useState<E>(notifier.getValue());\r\n\r\n    useEffect(() => {\r\n      const subscribeId: number = notifier.addListener(() => {\r\n        setVal(notifier.getValue());\r\n      });\r\n\r\n      return () => {\r\n        notifier.removeListener(subscribeId);\r\n      }\r\n    }, []);\r\n\r\n    const tuple: [E, (val: E) => void] = [val, (value: E) => {\r\n      notifier.setValue(value);\r\n      addToCache(cache, value);\r\n    }];\r\n    return tuple;\r\n  }\r\n};\r\n","interface VoidChangeNotifierHandler {\r\n  (): void\r\n}\r\n\r\nexport interface ChangeNotifier<T> {\r\n\r\n  addListener(handler: VoidChangeNotifierHandler): number;\r\n\r\n  notifyAll(): void;\r\n\r\n  removeListener(subscribeId: number): void;\r\n\r\n  setValue(t: T): void;\r\n\r\n  getValue(): T;\r\n\r\n}\r\n\r\nexport class Provider<T> implements ChangeNotifier<T> {\r\n  protected _observerHandlers: {[key: number]: VoidChangeNotifierHandler} = {};\r\n  private _subscribeIdIota: number = 0;\r\n  private _val: T;\r\n\r\n  constructor(val: T) {\r\n    this._val = val;\r\n  }\r\n\r\n  addListener(handler: VoidChangeNotifierHandler): number {\r\n    this._observerHandlers[++this._subscribeIdIota] = handler;\r\n    return this._subscribeIdIota;\r\n  }\r\n\r\n  notifyAll(): void {\r\n    for (const subscribeId in this._observerHandlers) {\r\n      if (this._observerHandlers.hasOwnProperty(subscribeId)) {\r\n        this._observerHandlers[subscribeId]();\r\n      }\r\n    }\r\n  }\r\n\r\n  removeListener(subscribeId: number): void {\r\n    delete this._observerHandlers[subscribeId];\r\n  }\r\n\r\n  getValue(): T {\r\n    return this._val;\r\n  }\r\n\r\n  setValue(t: T): void {\r\n    this._val = t;\r\n    this.notifyAll();\r\n  }\r\n\r\n}\r\n","export class GlobalVariables {\r\n  private _screenHeight: number | null;\r\n  private _screenWidth: number | null;\r\n  private _realWidth: number | null;\r\n  private _realHeight: number | null;\r\n\r\n  get screenHeight(): number | null {\r\n    return this._screenHeight;\r\n  }\r\n\r\n  set screenHeight(value: number | null) {\r\n    this._screenHeight = value;\r\n  }\r\n\r\n  get screenWidth(): number | null {\r\n    return this._screenWidth;\r\n  }\r\n\r\n  set screenWidth(value: number | null) {\r\n    this._screenWidth = value;\r\n  }\r\n\r\n  get realWidth(): number | null {\r\n    return this._realWidth;\r\n  }\r\n\r\n  set realWidth(value: number | null) {\r\n    this._realWidth = value;\r\n  }\r\n\r\n  get realHeight(): number | null {\r\n    return this._realHeight;\r\n  }\r\n\r\n  set realHeight(value: number | null) {\r\n    this._realHeight = value;\r\n  }\r\n\r\n\r\n  constructor(screenHeight: number | null, screenWidth: number | null, realWidth: number | null, realHeight: number | null) {\r\n    this._screenHeight = screenHeight;\r\n    this._screenWidth = screenWidth;\r\n    this._realWidth = realWidth;\r\n    this._realHeight = realHeight;\r\n  }\r\n}\r\n","export class Theme {\r\n  private _mainColor: string;\r\n  private _secondColor: string;\r\n\r\n  constructor(mainColor: string, secondColor: string) {\r\n    this._mainColor = mainColor;\r\n    this._secondColor = secondColor;\r\n  }\r\n\r\n  get mainColor(): string {\r\n    return this._mainColor;\r\n  }\r\n\r\n  set mainColor(value: string) {\r\n    this._mainColor = value;\r\n  }\r\n\r\n  get secondColor(): string {\r\n    return this._secondColor;\r\n  }\r\n\r\n  set secondColor(value: string) {\r\n    this._secondColor = value;\r\n  }\r\n}\r\n","import {Provider} from \"./Provider\";\r\nimport {GlobalVariables} from \"../entities/GlobalVariables\";\r\nimport {Theme} from \"../entities/Theme\";\r\nimport {BookManageState} from \"../entities/BookManageState\";\r\nimport {IReadState} from \"../entities/ReadState\";\r\nimport {\r\n  IBookShelfList, ICostLog,\r\n  ICouponData,\r\n  IMoneyBtn, IMyPrizes,\r\n  IPrizeInfo,\r\n  IReadHistories, IRecharge2Info, IRechargeLog,\r\n  IUserInfo,\r\n  IUserSignedRecord\r\n} from \"../api/UserApi\";\r\nimport {IRewardItem} from \"../api/RewardApi\";\r\nimport {IHomeData} from \"../api/HomeApi\";\r\nimport {ISearchResults} from \"../api/SearchApi\";\r\nimport {ICommonSet, IWechatInfo} from \"../api/OtherApi\";\r\nimport {getCache} from \"../utils/LocalStorageUtil\";\r\nimport {LocationDescriptorObject} from \"history\";\r\n\r\nconst cachedReadTheme = getCache<IReadState>(\"readTheme\");\r\nconst cachedCallback = getCache<string>(\"payCallbackUrl\");\r\n\r\n/**\r\n * 全局变量provider\r\n */\r\nexport const GlobalVariablesProvider\r\n  = new Provider<GlobalVariables>(new GlobalVariables(null, null, null, null));\r\n\r\n/**\r\n * 主题provider\r\n */\r\nexport const themeProvider = new Provider<Theme>(new Theme(\"#f78146\", \"\"));\r\n\r\n/**\r\n * 当前路由provider\r\n */\r\nexport const routePathProvider = new Provider<string | null>(null);\r\n\r\n/**\r\n * 我的书籍管理状态\r\n */\r\nexport const bookManageState = new Provider<BookManageState>({isOpen: false, isSelectedAll: false});\r\n\r\n/**\r\n *  阅读主题状态\r\n */\r\nexport const readState = new Provider<IReadState>(\r\n  cachedReadTheme === null\r\n  ? {fontSize: 21, theme: \"普通\", lineHeight: () => 30}\r\n  : cachedReadTheme\r\n);\r\n\r\n/**\r\n * 登录状态类型\r\n */\r\nexport type IsLoginState = true | false | null | Promise<any>;\r\n\r\n/**\r\n * 登录状态\r\n */\r\nexport const isLoginProvider = new Provider<IsLoginState>(null);\r\n\r\nexport const userInfoProvider = new Provider<IUserInfo | null>(null);\r\n\r\nexport const moneyBtnsProvider = new Provider<Array<IMoneyBtn> | null>(null);\r\n\r\nexport const vipMoneyBtnsProvider = new Provider<Array<IMoneyBtn> | null>(null);\r\n\r\nexport const rechargeMoneyBtnsProvider = new Provider<IMoneyBtn | null>(null);\r\n\r\nexport const rewardPopupProvider = new Provider(false);\r\n\r\nexport const readThemePopupProvider = new Provider(false);\r\n\r\nexport const rewardItemsProvider = new Provider<Array<IRewardItem>>([]);\r\n\r\nexport const homeDataProvider = new Provider<IHomeData | null>(null);\r\n\r\nexport const prizeInfoProvider = new Provider<IPrizeInfo | null>(null);\r\n\r\nexport const userSignedRecordProvider = new Provider<IUserSignedRecord | null>(null);\r\n\r\nexport const defaultSearchArgumentsProvider = new Provider<ISearchResults | null>(null);\r\n\r\nexport const userBookShelfDataProvider = new Provider<IBookShelfList | null>(null);\r\n\r\nexport const userReadHistoriesProvider = new Provider<IReadHistories | null>(null);\r\n\r\nexport const userValidCouponsProvider = new Provider<Array<ICouponData> | null>(null);\r\n\r\nexport const userInValidCouponsProvider = new Provider<Array<ICouponData> | null>(null);\r\n\r\nexport const userRechargeLogsProvider = new Provider<Array<IRechargeLog> | null>(null);\r\n\r\nexport const userCostLogsProvider = new Provider<Array<ICostLog> | null>(null);\r\n\r\nexport const userPrizesProvider = new Provider<IMyPrizes | null>(null);\r\n\r\nexport const searchParamsProvider = new Provider<{[propName: string]: any}>({});\r\n\r\nexport const userWechatInfoProvider = new Provider<IWechatInfo | null>(null);\r\n\r\nexport const payDomainProvider = new Provider(\"/\");\r\n\r\nexport const payCallbackUrlProvider = new Provider<string | null>(cachedCallback || null);\r\n\r\nexport const commonSetProvider = new Provider<number>(0);\r\n\r\nexport const recharge2InfoProvider = new Provider<IRecharge2Info | null>(null);\r\n\r\nexport const signSuccessProvider = new Provider<boolean>(false);\r\n","export enum ResponseCode {\r\n  SUCCESS = 0,\r\n  NOW_FOLLOW = 804,\r\n  NOT_LOGIN = 800,\r\n  NOT_ENOUGH_MONEY = 801,\r\n  TO_RECHARGE = 802,\r\n  TO_LOGIN = 803,\r\n}\r\n","// export const SERVER_ROOT = \"http://dev.zmr029.com/\";\n//\n// export const SERVER_ROOT = \"http://wd.zhaojinger.cn/\";\n//\nexport const SERVER_ROOT = \"/\";\n\n","import {IAfterInterceptor, IBeforeInterceptor} from \"./HttpUtils\";\r\nimport {AxiosRequestConfig, AxiosResponse} from \"axios\";\r\nimport {Toast} from \"antd-mobile\";\r\n\r\ninterface IRequestMiddlewares {\r\n  beforeInterceptors?: Array<IBeforeInterceptor>;\r\n  afterInterceptors?: Array<IAfterInterceptor>;\r\n}\r\n\r\nexport const interceptors: IRequestMiddlewares = {\r\n  beforeInterceptors: [\r\n    (config: AxiosRequestConfig) => {\r\n      return config;\r\n    }\r\n  ],\r\n\r\n  afterInterceptors: [\r\n    (response: AxiosResponse) => {\r\n      if (response.status === 500) {\r\n        Toast.fail(\"服务器内部错误\", 1.5, undefined, false);\r\n        return response;\r\n      }\r\n\r\n      return response;\r\n    }\r\n  ]\r\n};\r\n","import {ContentType, IAfterInterceptor, IBeforeInterceptor, IParams, IQuery, request, RequestMethod} from \"./HttpUtils\";\r\nimport {interceptors} from \"./RequestMiddlewares\";\r\nimport {ResponseCode} from \"./ResponseCode\";\r\n\r\nconst commonRequest = <T>(method: RequestMethod) => {\r\n  return request<T>(method)(\r\n    interceptors.beforeInterceptors as Array<IBeforeInterceptor>,\r\n    interceptors.afterInterceptors as Array<IAfterInterceptor>\r\n  )\r\n};\r\n\r\nexport interface IResponseData<T = {[propName: string]: string}> {\r\n  data: T;\r\n  err_code?: ResponseCode;\r\n  err_msg?: string\r\n}\r\n\r\nexport const Get = <T = any>(url: string, query?: IQuery, params?: IParams) => {\r\n  return commonRequest<IResponseData<T>>(RequestMethod.GET)(url, query, params);\r\n};\r\n\r\nexport const GetData = <T = any>(url: string, query?: IQuery, params?: IParams) => {\r\n  return commonRequest<IResponseData<T>>(RequestMethod.GET)(url, query, params, ContentType.json);\r\n};\r\n\r\nexport const PostForm = <T = any>(url: string, query?: IQuery, params?: IParams) => {\r\n  return commonRequest<IResponseData<T>>(RequestMethod.POST_FORM)(url, query, params);\r\n};\r\n\r\nexport const PostData = <T = any>(url: string, query?: IQuery, params?: IParams) => {\r\n  return commonRequest<IResponseData<T>>(RequestMethod.POST)(url, query, params, ContentType.json);\r\n};\r\n","// 金额按钮\nimport {IResponseData, PostData, PostForm} from \"../utils/RequestShorcuts\";\nimport {request, RequestMethod} from \"../utils/HttpUtils\";\n\nexport interface IMoneyBtn {\n  //优惠描述\n  red_desc:string;\n  // id\n  id: number;\n  //标签\n  tag:string;\n  // 价格\n  price: number;\n  status?: number;\n  // 描述\n  desc: string | null;\n  // 是否hot，并默认显示\n  default: number;\n  // 金额\n  coin: number;\n  title: string;\n  // 满减金额\n  reduction: number;\n}\n\nexport interface IMoneyBtns {\n  btns: Array<IMoneyBtn>;\n  pay_domain: string;\n}\n\nexport interface IMoneyVipBtn {\n  btns: IMoneyBtn\n  pay_domain: string;\n}\n\nexport interface IMoneyFirstRechargeBtn {\n  btns: IMoneyBtn;\n  pay_domain: string;\n}\n\nexport const getFirstRechargeMoneyBtns = () => {\n  return PostForm<IMoneyFirstRechargeBtn>(\"/payment/money_btns\", undefined, {type: \"first\"});\n};\n\nexport const getRecharge3MoneyBtns = (btn_id?: number | string) => {\n  return PostForm<IMoneyFirstRechargeBtn>(\"/payment/money_btns\", undefined, {type: \"act\", btn_id});\n};\n\nexport const getMoneyBtns = () => {\n  return PostForm<IMoneyBtns>(\"/payment/money_btns\");\n};\n\nexport const getVipMoneyBtns = () => {\n  return PostForm<IMoneyBtns>(\"/payment/money_btns\", undefined, {type: \"vip\"});\n};\n\nexport const getActivityRechargeMoneyBtn = () => {\n  return PostForm<IMoneyFirstRechargeBtn>('/payment/money_btns', undefined, {type: 'page_act'});\n};\n\nexport interface IUserInfo {\n  name: string;\n  email: string;\n  subscribe: number;\n  sign_day: number;\n  img: string;\n  id: number;\n  sex: number;\n  recharge_money: number;\n  balance: number;\n  vip_end_at: number;\n  extend_link_id: number;\n}\n\nexport interface IPrizeResult {\n  id: number;\n  name: string;\n  img: string;\n  count: number;\n  send_num: number;\n  type: number;\n  coin: number;\n  coupon_id: number;\n  coupon: number;\n  status: number;\n  updated_at: number;\n  // 奖品索引\n  index: number;\n}\n\nexport interface IBookShelf {\n\n}\n\nexport interface IPrizeInfoItem {\n  id: number;\n  img: string;\n  name: string;\n  send_num: number;\n}\n\nexport interface IPrizedUser {\n  customer_id: number\n  desc: string\n  id: number\n  platform_wechat_id: number\n  prize_id: number\n  prize_name: string;\n  status: number\n  user_id: 3\n  username: string;\n}\n\nexport interface IPrizeInfo {\n  prize_bgimg: string;\n  prize_info: Array<IPrizeInfoItem>;\n  prized_list: Array<IPrizedUser>;\n}\n\nexport interface IDaysCoin {\n  [propName: string]: number;\n}\n\nexport interface IUserSignedRecord {\n  id: number;\n  user_id: number;\n  continue_day: number;\n  created_at: number;\n  coin: number;\n  day_coins: IDaysCoin;\n  signed: boolean;\n}\n\nexport interface IBookShelfListDataItem {\n  id: number;\n  user_id: number;\n  novel_id: number;\n  novel_img: string;\n  novel_title: string;\n  section_num: number;\n  status: number;\n}\n\nexport interface IBookShelfRecommendItem {\n  img: string;\n  title: string;\n  desc: string;\n  tags: null | string;\n  suitable_sex: number;\n  novel_id: number;\n}\n\nexport interface IBookShelfListData {\n  book_store: Array<IBookShelfListDataItem>;\n  recommend: Array<IBookShelfRecommendItem>;\n}\n\nexport interface IBookShelfList {\n  data: IBookShelfListData;\n  last_page: null | number;\n}\n\nexport interface IReadHistoryItemDetail {\n  id: number;\n  img: string;\n}\n\nexport interface IReadHistoryItem {\n  novel: IReadHistoryItemDetail;\n  updated_at: number;\n  customer_id: number;\n  id: number;\n  name: string;\n  novel_id: number;\n  novel_section_id: number;\n  platform_wechat_id: number;\n  status: number;\n  title: string;\n  user_id: number;\n}\n\nexport interface IReadHistories {\n  data: Array<IReadHistoryItem>;\n  last_page: null | number;\n}\n\nexport interface ICouponDataItem {\n  id: number;\n  name: string;\n  img: string;\n  type: number;\n  novel_info: number;\n  count: number;\n  send_num: number;\n  use_num: number;\n  status: number;\n  start_at: number;\n  end_at: number;\n}\n\nexport interface ICouponData {\n  id: number;\n  customer_id: number;\n  platform_wechat_id: number;\n  user_id: number;\n  coupon_id: number;\n  status: number;\n  coupon: ICouponDataItem;\n}\n\nexport interface IRechargeLog {\n  type: \"coin\" | \"vip\";\n  coin: number | null;\n  money: number;\n  balance: number;\n  status: number;\n  out_trade_no: string;\n  updated_at: number;\n  desc: string;\n}\n\nexport interface IRechargeLogs {\n  data: Array<IRechargeLog>;\n  last_page: null | number;\n}\n\n/**\n *  1 打赏\n *  3 购买阅读\n *  5 抽奖\n *  6 其他\n */\nexport type CostType = 1 | 3 | 5 | 6;\n\nexport type CostStatus = 0 | 1 | 2;\n\nexport interface ICostLog {\n  type: CostType;\n  coin: number;\n  balance: number;\n  status: CostStatus;\n  updated_at: number;\n  title: string;\n  desc: string;\n}\n\nexport interface ICostLogs {\n  data: Array<ICostLog>;\n  last_page: null | number;\n}\n\nexport interface IMyPrizeDetail {\n  id: number;\n  img: string;\n}\n\nexport interface IMyPrize {\n  prize_id: number;\n  user_id: number;\n  username: null | string;\n  prize_name: string;\n  desc: string;\n  updated_at: number;\n  type: string;\n  num: number;\n  prize: IMyPrizeDetail;\n}\n\nexport interface IMyPrizes {\n  data: Array<IMyPrize>;\n  last_page: null | number;\n}\n\nexport interface IPayWxInfo {\n  out_trade_no: string;\n  order: IPayWxInfoDetail;\n}\n\nexport interface IPayWxInfoDetail {\n  appId: string;\n  timeStamp: number;\n  nonceStr: string;\n  package: string;\n  signType: string;\n  paySign: string;\n  [propName: string]: any;\n}\n\nexport interface IRecharge2Info {\n  id: number;\n  name: string;\n  coin: number;\n  start_at: number;\n  end_at: number;\n  limit: number;\n  customer_id: number;\n  status: number;\n}\n\nexport interface IPayResult {\n  balance: number;\n}\n\nexport interface INovelWechatQrcode {\n  qrcode: string;\n}\n\nexport const getPayInfo = (money_btn_id: number) => {\n  return PostForm<IPayWxInfo>(\"/payment/topay/wechat\", undefined, {money_btn_id});\n};\n\nexport const getUserInfo = () => {\n  return PostForm<IUserInfo>(\"/user/userinfo\")\n};\n\n/**\n * 检查是否登录\n */\nexport const checkLogin = () => {\n  return request<IResponseData<IUserInfo | null>>(RequestMethod.GET)([], [])(\"/user/userinfo\")\n};\n\nexport const getBookShelf = () => {\n  return PostForm(\"/novel/bookstore\");\n};\n\nexport const getPrizeInfo = () => {\n  return PostForm<IPrizeInfo>(\"/operate/prizelist\");\n};\n\nexport const doPrize = () => {\n  return PostForm<IPrizeResult>(\"/operate/doprize\");\n};\n\nexport const getUserSignedRecord = () => {\n return PostForm<IUserSignedRecord>(\"/operate/signinfo\");\n};\n\nexport const doUserSign = (otherPageSign: boolean = false) => {\n  return PostForm<null>(\"/operate/dosing\", undefined, {refer: otherPageSign ? \"notsignpage\" : undefined});\n};\n\nexport const getBookShelfList = () => {\n  return PostForm<IBookShelfList>(\"/novel/bookstore\");\n};\n\nexport const addNovelToBookShelf = (novel_id: number, section?: number) => {\n  return PostForm<null>(\"/operate/addbookstore\", undefined, {novel_id, section});\n};\n\nexport const getReadHistories = () => {\n  return PostForm<IReadHistories>(\"/novel/readlist\");\n};\n\nexport const getRechargeRecors = (page?: number) => {\n  return PostForm<IRechargeLogs>(\"/operate/rechargelogs\", undefined, {page});\n};\n\nexport const getCostRecors = (page?: number) => {\n  return PostForm<ICostLogs>(\"/operate/coinlogs\", undefined, {page});\n};\n\nexport type CouponValid = \"valid\" | \"invalid\";\n\nexport const getCoupons = (valid: CouponValid) => {\n  return PostForm<Array<ICouponData>>(\"/operate/usercoupons\", undefined, {valid});\n};\n\nexport const getMyPrizes = (page?: number) => {\n  return PostForm<IMyPrizes>(\"/operate/userprizelog\", undefined, {page});\n};\n\nexport const delNovelFromShelf = (ids: Array<number>) => {\n  return PostForm<null>(\"/novel/delbookstore\", undefined, {ids: JSON.stringify(ids)});\n};\n\nexport const delReadHistories = (ids: Array<number>) => {\n  return PostForm<null>(\"/novel/delreadlog\", undefined, {ids: JSON.stringify(ids)});\n};\n\nexport const doFeedBack = (content: string, desc: string, tel?: string) => {\n  return PostForm<null>(\"/operate/dofeedback\", undefined, {content, desc, tel});\n};\n\nexport const getNovelWechatQrcode = (novel_id: number, section: number) => {\n  return PostForm<INovelWechatQrcode>(\"/common/productqrcode\", undefined, {novel_id, section});\n};\n\nexport const getPayResult = (out_trade_no: string) => {\n  return PostForm<null>(\"/payment/outtradenocheck\", undefined, {out_trade_no});\n};\n\nexport const getRecharge2Info = (id: number, customer_id: number) => {\n  return PostForm<IRecharge2Info>(\"/operate/coinactinfo\", undefined, {id, customer_id});\n};\n\nexport const doRecharge2 = (id: number, customer_id: number) => {\n  return PostForm<{coin: number, balance: number}>(\"/operate/coinact\", undefined, {id, customer_id});\n};\n","import {createHook, createHookWithCache} from \"./CreateHook\";\r\nimport {\r\n  bookManageState,\r\n  commonSetProvider,\r\n  defaultSearchArgumentsProvider,\r\n  homeDataProvider,\r\n  isLoginProvider,\r\n  moneyBtnsProvider,\r\n  payCallbackUrlProvider,\r\n  payDomainProvider,\r\n  prizeInfoProvider,\r\n  readState,\r\n  readThemePopupProvider, recharge2InfoProvider, rechargeMoneyBtnsProvider,\r\n  rewardItemsProvider,\r\n  rewardPopupProvider,\r\n  searchParamsProvider, signSuccessProvider,\r\n  themeProvider,\r\n  userBookShelfDataProvider,\r\n  userCostLogsProvider,\r\n  userInfoProvider,\r\n  userInValidCouponsProvider,\r\n  userPrizesProvider,\r\n  userReadHistoriesProvider,\r\n  userRechargeLogsProvider,\r\n  userSignedRecordProvider,\r\n  userValidCouponsProvider,\r\n  userWechatInfoProvider,\r\n  vipMoneyBtnsProvider\r\n} from \"./Providers\";\r\nimport {useEffect} from \"react\";\r\nimport {checkLogin, doUserSign, IRecharge2Info, IUserInfo} from \"../api/UserApi\";\r\nimport {IResponseData} from \"../utils/RequestShorcuts\";\r\nimport qs from \"qs\"\r\nimport {ResponseCode} from \"../utils/ResponseCode\";\r\nimport {Toast} from \"antd-mobile\";\r\nimport {SERVER_ROOT} from \"../constants/Server\";\r\n\r\n/**\r\n * 主题hook\r\n */\r\nexport const useTheme = createHook(themeProvider, \"全局主题\");\r\n\r\n/**\r\n * 书籍管理hook\r\n */\r\nexport const useBookManage = createHook(bookManageState, \"书籍管理\");\r\n\r\n/**\r\n * 阅读状态hook\r\n *\r\n */\r\nexport const useReadState = createHook(readState, \"阅读主题\");\r\n\r\n/**\r\n *  是否已经登录 true 一登录 false 未登录 null 未执行登录 promise<any> 正在登录\r\n */\r\nexport const useIsLogin = createHook(isLoginProvider, \"是否登录\");\r\n\r\n/**\r\n * 充值按钮金额列表hook\r\n */\r\nexport const useMoneyBtns = createHook(moneyBtnsProvider, \"充值金额列表\");\r\n\r\n/**\r\n *  vip充值按钮列表hook\r\n */\r\nexport const useVipMoneyBtns = createHook(vipMoneyBtnsProvider, \"vip充值按钮列表\");\r\n\r\n/**\r\n *  活动充值按钮列表hook\r\n */\r\nexport const useActivityMoneyBtns = createHook(rechargeMoneyBtnsProvider, '首充按钮列表');\r\n\r\n/**\r\n * 打赏弹窗是否带开hook\r\n */\r\nexport const useRewardPopup = createHook(rewardPopupProvider, \"打赏弹窗打开状态\");\r\n\r\n/**\r\n * 悬赏商品列表hook\r\n */\r\nexport const useRewardItems = createHook(rewardItemsProvider, \"悬赏物品列表\");\r\n\r\n/**\r\n * 阅读弹窗hook\r\n */\r\nexport const useReadThemePopup = createHook(readThemePopupProvider, \"阅读主题弹窗状态\");\r\n\r\n/**\r\n *  用户信息hook\r\n */\r\nexport const useUserInfo = createHook(userInfoProvider, \"用户信息\");\r\n\r\n/**\r\n *  奖品信息hook\r\n */\r\nexport const usePrizeInfo = createHook(prizeInfoProvider, \"奖品信息\");\r\n\r\n/**\r\n *  用户签到信息hook\r\n */\r\nexport const useUserSignedRecord = createHook(userSignedRecordProvider, \"用户签到记录\");\r\n\r\n/**\r\n *  搜索页面默认数据hook\r\n */\r\nexport const useDefaultSearchArgument = createHook(defaultSearchArgumentsProvider, \"搜索页默认数据\");\r\n\r\n/**\r\n *  用户书架信息hook\r\n */\r\nexport const useUserBookShelfData = createHook(userBookShelfDataProvider, \"用户书架信息\");\r\n\r\n/**\r\n *  用户阅读历史记录hook\r\n */\r\nexport const useUserReadHistories = createHook(userReadHistoriesProvider, \"用户阅读历史\");\r\n\r\n\r\n/**\r\n *  用户有效的优惠券hook\r\n */\r\nexport const useUserValidCoupons = createHook(userValidCouponsProvider, \"用户有效优惠券\");\r\n\r\n/**\r\n *  用户无法使用的优惠券hook\r\n */\r\nexport const useUserInValidCoupons = createHook(userInValidCouponsProvider, \"用户无效优惠券\");\r\n\r\n/**\r\n *  用户获得的奖品列表\r\n */\r\nexport const useUserPrized = createHook(userPrizesProvider, \"用户奖品列表\");\r\n\r\n/**\r\n *  主页数据hook\r\n */\r\nexport const useHomeData = createHook(homeDataProvider, \"首页数据\");\r\n\r\nexport const useUserCostLogs = createHook(userCostLogsProvider, \"用户消费记录\");\r\n\r\nexport const useUserRechargeLogs = createHook(userRechargeLogsProvider, \"用户充值记录\");\r\n\r\nexport const useSearchParamsProvider = createHook(searchParamsProvider, \"url参数\");\r\n\r\nexport const useWechatInfo = createHook(userWechatInfoProvider, \"微信信息\");\r\n\r\nexport const usePayDomain = createHook(payDomainProvider, \"全局支付域名\");\r\n\r\nexport const usePayCallbackUrl = createHookWithCache(payCallbackUrlProvider, \"payCallbackUrl\");\r\n\r\nexport const useCommonSet = createHook(commonSetProvider, \"全局配置\");\r\n\r\nexport const useRecharge2Info = createHook(recharge2InfoProvider, \"充值活动2的信息\");\r\n\r\nexport const useSignSuccess = createHook(signSuccessProvider, \"签到成功窗口是否打开\");\r\n\r\nexport interface ICustomerId {\r\n  cid?: number;\r\n}\r\n\r\nexport const useCheckLogin = () => {\r\n  const params = qs.parse(window.location.hash.split(\"?\")[1]) as ICustomerId;\r\n  const [isLogin, setIsLogin] = useIsLogin();\r\n  const [userInfo, setUserInfo] = useUserInfo();\r\n  const [, setSignSuccess] = useSignSuccess();\r\n\r\n  useEffect(() => {\r\n    if (isLogin === null) {\r\n      const promise: Promise<IResponseData<IUserInfo | null>> = checkLogin();\r\n      setIsLogin(promise);\r\n\r\n      promise.then((data) => {\r\n        // if (data.err_code === ResponseCode.NOT_LOGIN) {\r\n        //   const url = encodeURIComponent(window.location.pathname + window.location.hash);\r\n        //   window.location.href = (SERVER_ROOT + `user/login/wechat?backurl=${url}&customer_id=` + (params.cid === undefined ? \"\" : params.cid + \"\"));\r\n        //   return;\r\n        // }\r\n\r\n        if (data.err_code === ResponseCode.NOT_LOGIN) {\r\n          setIsLogin(false);\r\n        } else if (data.err_code === ResponseCode.SUCCESS) {\r\n          doUserSign(true)\r\n            .then((data) => {\r\n              if (data.err_code === ResponseCode.SUCCESS) {\r\n                // Toast.success(\"签到成功\", 1.5, undefined, false);\r\n                setTimeout(() => {\r\n                  setSignSuccess(true);\r\n                }, 1000);\r\n              }\r\n            });\r\n          setUserInfo(data.data);\r\n          setIsLogin(true);\r\n        }\r\n      });\r\n    }\r\n  }, [setIsLogin, setUserInfo, isLogin]);\r\n\r\n  useEffect(() => {\r\n    if (userInfo !== null) {\r\n      setIsLogin(true);\r\n    }\r\n  }, [userInfo]);\r\n};\r\n\r\n/**\r\n * 动态设置标题hook\r\n * @param title\r\n */\r\nexport const useDocumentTitle = (title: string | undefined | null) => {\r\n  const prevTitle: string = window.document.title;\r\n\r\n  useEffect(() => {\r\n    if (title !== undefined && title !== null) {\r\n      window.document.title = title;\r\n    }\r\n  }, []);\r\n};\r\n","import * as React from \"react\";\r\nimport \"./LoadMoreBottomContent.less\";\r\nimport {Icon} from \"antd-mobile\";\r\nimport {Fragment} from \"react\";\r\n\r\nexport enum LoadingState {\r\n  FINISHED = -1, LOADING = 0, LOADED = 1,\r\n}\r\n\r\nexport interface IloadMoreBottomContentProps {\r\n  status: LoadingState | null,\r\n  bgColor?: string;\r\n}\r\n\r\nexport const LoadMoreBottomContent: React.FC<IloadMoreBottomContentProps> = (props) => {\r\n  const buildContent = (status: LoadingState | null): JSX.Element => {\r\n    switch (status) {\r\n      case null:\r\n        return <Fragment></Fragment>\r\n      case LoadingState.LOADED:\r\n        return <div style={{background: props.bgColor}} className=\"loading\">玩命加载中<Icon type={\"loading\"} /></div>;\r\n      case LoadingState.FINISHED:\r\n        return (\r\n          <p style={{background: props.bgColor}} className=\"finished\">暂无更多数据</p>\r\n        );\r\n      case LoadingState.LOADING:\r\n        return (\r\n          <div style={{background: props.bgColor}} className=\"loading\">玩命加载中<Icon type={\"loading\"} /></div>\r\n        );\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"load-more-bottom-content\">\r\n      { buildContent(props.status) }\r\n    </div>\r\n  )\r\n};\r\n\r\nLoadMoreBottomContent.defaultProps = {\r\n  bgColor: \"transparent\",\r\n};\r\n","import * as React from \"react\";\r\nimport {useCallback} from \"react\";\r\nimport {useHomeData, useTheme} from \"../../hooks/Hooks\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport \"./TabBar.less\";\r\nimport {Route} from \"react-router-dom\";\r\nimport {RouterProps} from \"react-router\";\r\n\r\nexport interface ITabBarItem<T> {\r\n  path: string;\r\n  name: string,\r\n  icon: string | null,\r\n  activeIcon: string | null;\r\n  _key: T\r\n}\r\n\r\nexport interface ITabBarItemProps<T> extends ITabBarItem<T> {\r\n  onClick: (key: T) => void;\r\n}\r\n\r\nexport interface ITabBarProps<T> {\r\n  tabs: Array<ITabBarItem<T>>,\r\n  onClick: (key: T) => void;\r\n}\r\n\r\nexport const TabBarItem: React.FC<ITabBarItemProps<any> & RouterProps> = (props) => {\r\n  const [theme] = useTheme();\r\n\r\n  const isActive = useCallback((path: string, currentPath: string) => {\r\n    if (\r\n      [\"/book-shelf.html\", \"/book-history.html\"].indexOf(currentPath) !== -1 &&\r\n      [\"/book-shelf.html\", \"/book-history.html\"].indexOf(path) !== -1\r\n    ) {\r\n      return true;\r\n    }\r\n\r\n    return path === currentPath;\r\n  }, [props]);\r\n\r\n  return (\r\n    <div onClick={() => props.onClick(props._key)} className=\"tab-bar-item\">\r\n      <div className=\"tab-bar-icon\">\r\n        <img\r\n          src={isActive(props.path, props.history.location.pathname) ? props.activeIcon as string : props.icon as string}\r\n          alt=\"\"/>\r\n      </div>\r\n      <div className=\"tab-bar-title\">\r\n        <p style={{color: isActive(props.path, props.history.location.pathname) ? (theme as Theme).mainColor : \"#999\"}}\r\n           className=\"title\">{props.name}</p>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport const TabBar: React.FC<ITabBarProps<any>> = (props) => {\r\n\r\n  return <div className=\"tab-bar\">\r\n    {\r\n      props.tabs.map((item) => {\r\n        return <Route key={item._key} children={\r\n          (router) => (\r\n            <TabBarItem\r\n              {...router}\r\n              activeIcon={item.activeIcon}\r\n              onClick={props.onClick}\r\n              path={item.path}\r\n              _key={item._key}\r\n              icon={item.icon}\r\n              name={item.name}/>\r\n          )\r\n        }/>\r\n      })\r\n    }\r\n  </div>\r\n};\r\n","const dic: {[propName: string]: string} = {\r\n  \"\\\\=\": \"0v11v0\",\r\n  \"\\\\#\": \"0v12v0\",\r\n  \"\\\\/\": \"0v13v0\",\r\n  \"\\\\?\": \"0v14v0\",\r\n  \"\\\\-\": \"0v15v0\",\r\n  \"\\\\_\": \"0v16v0\",\r\n  \"\\\\%\": \"0v17v0\",\r\n  \"\\\\.\": \"0v18v0\",\r\n  \"\\\\:\": \"0v19v0\",\r\n};\r\n\r\nexport const urlEscapeChar = (url: string, decode: boolean = false) => {\r\n  if (!decode) {\r\n    for (const key in dic) {\r\n      url = url.replace(new RegExp(key, \"g\"), dic[key]);\r\n    }\r\n  } else {\r\n    for (const key in dic) {\r\n      url = url.replace(new RegExp(dic[key], \"g\"), key.replace(/\\\\/g, \"\"));\r\n    }\r\n  }\r\n\r\n  return url;\r\n};\r\n","import {History, LocationDescriptorObject} from \"history\";\r\nimport qs from \"qs\";\r\nimport {LoadingState} from \"../pages/common/LoadMoreBottomContent\";\r\nimport {searchParamsProvider} from \"../hooks/Providers\";\r\n\r\nexport const goToPageWithCustomerId = (\r\n  history: History<any>,\r\n  args: LocationDescriptorObject<any>, replace: boolean = false\r\n) => {\r\n  if (searchParamsProvider.getValue() !== null) {\r\n\r\n    if (args.search === undefined) {\r\n      args.search = \"\";\r\n    }\r\n    let _query = qs.parse(args.search.split(\"?\")[1]) as { [propName: string]: any };\r\n    _query = {...searchParamsProvider.getValue(), ..._query};\r\n    delete _query[\"jumpto\"];\r\n    args.search = qs.stringify(_query);\r\n  }\r\n\r\n  if (replace) {\r\n    history.replace(args);\r\n  } else {\r\n    history.push(args);\r\n  }\r\n};\r\n\r\nexport const goToReadPage2 = (\r\n  history: History<any>,\r\n  id: number,\r\n  chapter: number,\r\n  reload: boolean = false,\r\n  replace: boolean = false,\r\n  setLoadingState?: (state: LoadingState) => void\r\n) => {\r\n  if (setLoadingState !== undefined) {\r\n    setLoadingState(LoadingState.LOADING);\r\n  }\r\n  goToPageWithCustomerId(history, {pathname: `/read2/${id}-${chapter}.html`, state: {reload}}, replace);\r\n};\r\n\r\n\r\nexport const goToReadPage = (\r\n  history: History<any>,\r\n  id: number,\r\n  chapter: number,\r\n  reload: boolean = false,\r\n  replace: boolean = false,\r\n  setLoadingState?: (state: LoadingState) => void\r\n) => {\r\n  if (setLoadingState !== undefined) {\r\n    setLoadingState(LoadingState.LOADING);\r\n  }\r\n  goToPageWithCustomerId(history, {pathname: `/read/${id}-${chapter}.html`, state: {reload}}, replace);\r\n};\r\n\r\nexport const goBack = (history: History<any>) => {\r\n  if (window.history.length <= 1) {\r\n    goToPageWithCustomerId(history, {pathname: \"/index.html\"})\r\n  } else {\r\n    history.go(-1);\r\n  }\r\n};\r\n","import React, {useCallback, useEffect, useRef, useState} from \"react\"\r\nimport \"./ServiceButton.less\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport {useHistory} from 'react-router';\r\n\r\nconst ServiceButton: React.FC = (props) => {\r\n  const [available, setAvailable] = useState(true);\r\n  const ref = useRef<HTMLDivElement>(null);\r\n  const history = useHistory();\r\n\r\n  const touchMoveHandler = useCallback((() => {\r\n    let runner: NodeJS.Timeout | null = null;\r\n\r\n    return () => {\r\n      if (ref.current === null) {\r\n        return;\r\n      }\r\n\r\n      if (runner !== null) {\r\n        clearTimeout(runner);\r\n        runner = null;\r\n      }\r\n\r\n      const elem = ref.current as HTMLDivElement;\r\n      elem.style.left = \"-.7rem\";\r\n\r\n      runner = setTimeout(() => {\r\n        elem.style.left = \".15rem\";\r\n      }, 1000);\r\n    };\r\n  })(), [ref]);\r\n\r\n  useEffect(() => {\r\n    window.addEventListener(\"touchmove\", touchMoveHandler);\r\n\r\n    return () => {\r\n      window.removeEventListener(\"touchmove\", touchMoveHandler);\r\n    }\r\n  }, []);\r\n\r\n  return (\r\n    <div onClick={() => goToPageWithCustomerId(history