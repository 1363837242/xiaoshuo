r\n            <div className=\"inner-container\">\r\n              <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/recharge4_bg.png\" alt=\"\"\r\n                   className=\"inner-container-bg\"/>\r\n              <div className=\"tip-container\">\r\n                <p className=\"tip1\" dangerouslySetInnerHTML={{__html: btn === null ? '' : btn.btns.title}} />\r\n                <p className=\"tip2\">共得{btn === null ? null : btn.btns.coin}书币 </p>\r\n              </div>\r\n              <div className=\"activity-time\">\r\n                <p className=\"time-title\">活动时间</p>\r\n                <p className=\"time-content\">{activityTime[0]} -- {activityTime[1]}</p>\r\n              </div>\r\n              <div className=\"do-recharge\" onClick={handlePay}>立即充值</div>\r\n            </div>\r\n\r\n            <div className=\"bottom-tips\">\r\n              <p className=\"tips-line\">*书币属于虚拟商品，充值后无法退款，请理解</p>\r\n              {\r\n                _userInfo !== null\r\n                  ? <p className=\"tips-line\">*客服： <span\r\n                    onClick={() => goToPageWithCustomerId(props.history, {pathname: '/contact.html'})}\r\n                    className=\"blue\">点击联系</span> (咨询时提供ID.{_userInfo.id})</p>\r\n                  : null\r\n              }\r\n              <p className=\"tips-line\">*工作时间: 9:00 -- 18:00</p>\r\n            </div>\r\n          </div>\r\n        )\r\n        : null\r\n    }}>\r\n\r\n    </FutureElement>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {match, Route, Switch} from \"react-router\";\r\nimport {Sweepstakes} from \"./Sweepstakes\";\r\nimport {NeedLogin} from \"../../HOC/User\";\r\nimport {FirstRecharge} from \"./FirstRecharge\";\r\nimport {Recharge2} from \"./Recharge2\";\r\nimport {Recharge3} from \"./Recharge3\";\r\nimport {Recharge4} from \"./Recharge4\";\r\n\r\ninterface IActivityType {\r\n  name: string;\r\n}\r\n\r\nexport const Activity: React.FC = (props) => {\r\n\r\n  return (\r\n    <Switch>\r\n      <Route path={\"/activity-sweepstakes.html\"} children={(router) => (\r\n        <NeedLogin {...router} component={Sweepstakes} />\r\n      )} />\r\n\r\n      <Route path={\"/activity-recharge.html\"} children={(router) => (\r\n        <NeedLogin {...router} component={FirstRecharge} />\r\n      )} />\r\n\r\n      <Route path={\"/activity-recharge2.html\"} children={(router) => (\r\n        <NeedLogin {...router} component={Recharge2} />\r\n      )} />\r\n\r\n      <Route path={\"/activity-recharge3.html\"} children={(router) => (\r\n        <NeedLogin {...router} component={Recharge4} />\r\n      )} />\r\n    </Switch>\r\n  );\r\n};\r\n","import * as React from \"react\";\r\nimport {Icon, NavBar} from \"antd-mobile\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport \"./PageWithNavBar.less\";\r\n\r\nexport interface IPageWithNavBarProps {\r\n  title?: string | JSX.Element;\r\n  rightContent?: string | JSX.Element;\r\n  onClick?: () => void;\r\n  background?: string;\r\n}\r\n\r\nexport const PageWithNavBar: React.FC<IPageWithNavBarProps & RouteChildrenProps> = (props) => {\r\n  const defaultBackHandler = () => {\r\n    props.history.go(-1);\r\n  };\r\n\r\n  return (\r\n    <div style={{background: props.background}} className=\"page-with-nav-bar-container\">\r\n      <NavBar\r\n        className=\"nav-bar\"\r\n        mode={\"light\"}\r\n        onLeftClick={\r\n          props.onClick === undefined\r\n            ? () => defaultBackHandler()\r\n            : () => (props.onClick as () => void)()\r\n        }\r\n        leftContent={<Icon type={\"left\"}/>}\r\n        rightContent={\r\n          typeof props.rightContent === \"string\"\r\n            ? <div className=\"right-btn\">{props.rightContent}</div>\r\n            : <div className=\"right-btn\">\r\n              {props.rightContent}\r\n            </div>\r\n        }\r\n      >\r\n        {props.title === undefined ? \" \" : props.title}\r\n      </NavBar>\r\n      {props.children}\r\n    </div>\r\n  )\r\n};\r\n\r\nPageWithNavBar.defaultProps = {\r\n  background: \"#fff\"\r\n};\r\n","import * as React from \"react\";\r\nimport {useTheme} from \"../../hooks/Hooks\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport \"./Category.less\";\r\nimport {INovelCategoryItem} from \"../../api/NovelApi\";\r\n\r\nexport interface IBookCategoryProps {\r\n  initialItem?: number;\r\n  withBottomBorder?: boolean;\r\n  items: Array<INovelCategoryItem>;\r\n  onChange: (key: INovelCategoryItem) => void;\r\n}\r\n\r\nexport const BookCategory: React.FC<IBookCategoryProps> = (props) => {\r\n  const theme = useTheme()[0] as Theme;\r\n  let first: INovelCategoryItem | undefined = props.items[0];\r\n\r\n  const handleClick = (item: INovelCategoryItem) => {\r\n    props.onChange(item);\r\n  };\r\n\r\n  return (\r\n    <div style={{borderBottom: props.withBottomBorder ? \"1px solid #f1f1f1\" : \"none\"}} className=\"book-categories\">\r\n      {\r\n        first === undefined\r\n          ? null\r\n          : (\r\n            <div className=\"left\">\r\n              <div onClick={() => handleClick(first as INovelCategoryItem)} className=\"book-category-first book-category\">\r\n                <p style={{color: props.initialItem === (first as INovelCategoryItem).id ? theme.mainColor : \"#333333\"}}\r\n                   className=\"book-category-name\">{(first as INovelCategoryItem).name}</p>\r\n              </div>\r\n            </div>\r\n          )\r\n      }\r\n      <div className=\"right\">\r\n        {\r\n          props.items.slice(1).map((category, index) => {\r\n            return (\r\n              <div key={category.id} onClick={() => handleClick(category)} className={\"book-category\"}>\r\n                <p style={{color: props.initialItem === category.id ? theme.mainColor : \"#333333\"}}\r\n                   className=\"book-category-name\">{category.name}</p>\r\n              </div>\r\n            )\r\n          })\r\n        }\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n\r\nBookCategory.defaultProps = {\r\n  withBottomBorder: true,\r\n};\r\n","import {PostForm} from \"../utils/RequestShorcuts\";\r\n\r\nexport interface INovelDetail {\r\n  id: number,\r\n  is_bookstore: boolean;\r\n  title: string,\r\n  img: string,\r\n  author_id: number,\r\n  author_name: string,\r\n  serial_status: 1,\r\n  word_count: number,\r\n  suitable_sex: number,\r\n  type_ids: number,\r\n  tags: null | string,\r\n  desc: string,\r\n  sections: number,\r\n  status: number,\r\n  read_num: number,\r\n  week_read_num: number,\r\n  reward_coin: number;\r\n  need_buy_section: number;\r\n  section_first2last: ISectionFirstAndLast\r\n}\r\n\r\nexport interface ISectionFirstAndLast {\r\n  first: IChapterShortInfo;\r\n  last: IChapterShortInfo;\r\n}\r\n\r\nexport interface IChapterShortInfo {\r\n  novel_id: number;\r\n  num: number;\r\n  title: string\r\n}\r\n\r\nexport interface IRewardListItem {\r\n  goods_img: string;\r\n  novel_id: number,\r\n  user_id: number,\r\n  username: string\r\n  user_img: string,\r\n  goods_id: number,\r\n  goods_num: number,\r\n  coin_num: number,\r\n  created_at: number\r\n}\r\n\r\nexport interface INovelComment {\r\n  username: string,\r\n  user_img: string,\r\n  content: string,\r\n  created_at: number\r\n}\r\n\r\nexport interface INovelComments {\r\n  data: Array<INovelComment>;\r\n  last_page: number | null;\r\n}\r\n\r\nexport interface INovelRewardList {\r\n  data: Array<IRewardListItem>;\r\n  last_page: number | null;\r\n}\r\n\r\nexport interface INovelChapter {\r\n  novel_id: number;\r\n  num: number;\r\n  title: string;\r\n}\r\n\r\nexport interface INovelChapters {\r\n  data: Array<INovelChapter>;\r\n  section_count: number;\r\n  need_buy_section: number;\r\n  subscribe_section: number;\r\n  last_page: number | null;\r\n}\r\n\r\nexport interface INovelCategoryItem {\r\n  id: number;\r\n  pid: number;\r\n  name: string;\r\n  status: number;\r\n  sort: number;\r\n  category_id: number;\r\n}\r\n\r\nexport interface INovelCategory {\r\n  id: number;\r\n  name: string;\r\n  activeItemId: number;\r\n  categories: Array<INovelCategoryItem>;\r\n}\r\n\r\nexport interface INovelContent {\r\n  id: number;\r\n  novel_id: number;\r\n  num: number;\r\n  is_bookstore: boolean;\r\n  title: string;\r\n  content: string;\r\n  updated_num: number;\r\n  section_count: number;\r\n}\r\n\r\nexport interface INovelRecommend {\r\n  id: number;\r\n  title: string;\r\n  hot_num: number;\r\n  desc: string;\r\n}\r\n\r\nexport interface INovelBasicInfo {\r\n  id: number;\r\n  novel_id: number;\r\n  num: number;\r\n  title: string;\r\n  updated_num: number;\r\n}\r\n\r\nexport const getNovelDetail = (id: number) => {\r\n  return PostForm<INovelDetail>(\"/novel/info\", undefined, {id});\r\n};\r\n\r\nexport const getNovelBasicInfo = (id: number, section: number) => {\r\n  return PostForm<INovelBasicInfo>(\"/novel/section\", undefined, {id, section});\r\n};\r\n\r\nexport const getNovelRewardList = async (novel_id: number, page: number = 1, order_column?: string) => {\r\n  const data = await PostForm<INovelRewardList>(\"/operate/rewardlist\", undefined, {novel_id, page, order_column});\r\n  if (order_column === \"coin_num\") {\r\n    // 映射数据\r\n    const maps: { [propName: number]: Array<IRewardListItem> } = {};\r\n    data.data.data.reduce((obj, curr) => {\r\n      if (obj[curr.user_id] === undefined) {\r\n        obj[curr.user_id] = [curr];\r\n      } else {\r\n        obj[curr.user_id].push(curr);\r\n      }\r\n\r\n      return obj;\r\n    }, maps);\r\n\r\n    const result: Array<IRewardListItem> = [];\r\n    for (const user_id in maps) {\r\n      if (maps.hasOwnProperty(user_id)) {\r\n        result.push({\r\n          ...maps[user_id][0],\r\n          coin_num: maps[user_id]\r\n            .map((_user) => _user.coin_num)\r\n            .reduce((prev, next) => prev + next)\r\n        })\r\n      }\r\n    }\r\n    data.data.data = result;\r\n  }\r\n\r\n  return data;\r\n};\r\n\r\nexport const getNovelCommentsList = (novel_id: number, page: number = 1) => {\r\n  return PostForm<INovelComments>(\"/operate/commentlist\", undefined, {novel_id, page});\r\n};\r\n\r\nexport const getNovelChapters = (id: number, page: number = 1, order?: \"asc\" | \"desc\") => {\r\n  return PostForm<INovelChapters>(\"/novel/sectionlist\", undefined, {id, page, order})\r\n};\r\n\r\nexport const getNovelCategories = () => {\r\n  return PostForm<Array<INovelCategory>>(\"/novel/types\");\r\n};\r\n\r\nexport const getNovelContent = (id: number, section: number, page?: string, params: {[propName: string]: any} = {}) => {\r\n  return PostForm<INovelContent>(\"/novel/sectioninfo\", undefined, {id, section, page, ...params});\r\n};\r\n\r\nexport const doComment = (novel_id: number, content: string) => {\r\n  return PostForm<null>(\"/operate/docomment\", undefined, {novel_id, content});\r\n};\r\n\r\nexport const isBalanceEnoughToRead = (id: number, section: number) => {\r\n  return PostForm<{en_read: boolean, balance: number}>(\"/novel/balanceenough\", undefined, {id, section});\r\n};\r\n\r\nexport const getNovelRecommends = (id: number) => {\r\n  return PostForm<Array<INovelRecommend>>(\"/novel/sectionrecommend\", undefined, {id});\r\n};\r\n","import {PostData, PostForm} from \"../utils/RequestShorcuts\";\r\n\r\nexport interface ISearchResult {\r\n  id: number,\r\n  novel_id: number;\r\n  title: string,\r\n  img: string,\r\n  author_id: number,\r\n  author_name: string,\r\n  serial_status: number,\r\n  word_count: number,\r\n  suitable_sex: number,\r\n  type_ids: 1,\r\n  tags: null | string,\r\n  desc: string,\r\n  sections: number,\r\n  status: number,\r\n  read_num: number,\r\n  week_read_num: number;\r\n}\r\n\r\nexport interface ISearchParams {\r\n  title?: string;\r\n  published?: boolean;\r\n  free?: boolean;\r\n  author_id?: number;\r\n  serial_status?: number;\r\n  suitable_sex?: number;\r\n  type_id?: number;\r\n  order?: string;\r\n  page?: number;\r\n}\r\n\r\ninterface IsearchResultsData {\r\n  novel_list: Array<ISearchResult>;\r\n  title_list: Array<ISearchResult>;\r\n  last_page: number | null;\r\n}\r\n\r\nexport interface ISearchResults {\r\n  last_page: number | null | undefined;\r\n  data: IsearchResultsData;\r\n}\r\n\r\nexport const getSearchResults = (params: ISearchParams) => {\r\n  return PostForm<ISearchResults>(\"/novel/search\", undefined, params);\r\n};\r\n\r\nexport const getSearchResultByDefault = () => {\r\n  return PostForm<ISearchResults>(\"/novel/search\", undefined, {default: true});\r\n};\r\n","import * as React from \"react\";\r\nimport {useEffect, useRef, useState} from \"react\";\r\nimport \"./LoadMoreList.less\";\r\n\r\nexport interface ILoadMoreList {\r\n  itemBuilder: (index: number) => JSX.Element | null;\r\n  itemCount: number;\r\n  onLoadMore?: () => void;\r\n  onScroll?: (pos: number) => void;\r\n  className?: string;\r\n  initPos?: number;\r\n}\r\n\r\nconst LoadMoreListComponent: React.FC<ILoadMoreList> = (props, ref) => {\r\n  const scrollContainer: React.RefObject<HTMLDivElement> = useRef(null);\r\n  const [isOnBottom, setIsOnBottom] = useState(false);\r\n\r\n  useEffect(() => {\r\n    setTimeout(() => {\r\n      if (scrollContainer.current !== null) {\r\n        (scrollContainer.current as HTMLDivElement).scrollTop = props.initPos === undefined\r\n          ? 0\r\n          : props.initPos;\r\n      }\r\n    }, 0);\r\n\r\n  }, [scrollContainer]);\r\n\r\n  const handleScroll = () => {\r\n    const ref = scrollContainer.current as HTMLDivElement;\r\n\r\n    if (props.onScroll !== undefined) {\r\n      handleOnScroll(ref.scrollTop);\r\n    }\r\n\r\n    if (undefined === props.onLoadMore) {\r\n      return;\r\n    }\r\n\r\n    if (isOnBottom) {\r\n      if (ref.scrollTop + ref.clientHeight < ref.scrollHeight - 80) {\r\n        setIsOnBottom(false);\r\n      }\r\n    } else {\r\n      if (ref.scrollTop + ref.clientHeight > ref.scrollHeight - 20) {\r\n        setIsOnBottom(true);\r\n        props.onLoadMore();\r\n      }\r\n    }\r\n  };\r\n\r\n  const handleOnScroll = (offset: number) => {\r\n    if (props.onScroll !== undefined) {\r\n      props.onScroll(offset);\r\n    }\r\n  };\r\n\r\n  const buildItems = () => {\r\n    const items: Array<JSX.Element> = [];\r\n    for (let i = 0; i < props.itemCount; i++) {\r\n      items.push((\r\n        <div key={i} className=\"load-more-item-wrapper\">\r\n          {props.itemBuilder(i)}\r\n        </div>\r\n      ))\r\n    }\r\n    return items;\r\n  };\r\n\r\n  return (\r\n    <div ref={scrollContainer} onScroll={handleScroll}\r\n         className={props.className !== undefined ? props.className + \" load-more-list\" : \"load-more-list\"}>\r\n      {buildItems()}\r\n      {props.children}\r\n    </div>\r\n  )\r\n};\r\n\r\nexport const LoadMoreList = LoadMoreListComponent\r\n","import * as React from \"react\";\r\nimport noContentImg from \"./images/no_content.png\"\r\nimport \"./NoContent.less\";\r\n\r\nexport interface INoContentProps {\r\n  fixed?: boolean;\r\n}\r\n\r\nexport const NoContent: React.FC<INoContentProps> = (props) => {\r\n  return (\r\n    <div className={props.fixed === true ? \"no-content fixed\" : \"no-content\"}>\r\n      <img src={noContentImg} alt=\"\"/>\r\n      <p className=\"content\">啥都没有</p>\r\n    </div>\r\n  )\r\n};\r\n\r\nNoContent.defaultProps = {\r\n  fixed: true,\r\n};\r\n\r\n","import * as React from \"react\";\r\nimport {useCallback, useEffect, useRef, useState} from \"react\";\r\nimport {PageWithNavBar} from \"../common/PageWithNavBar\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport {useDocumentTitle, useTheme} from \"../../hooks/Hooks\";\r\nimport {TopListCategory} from \"./Category\";\r\nimport {BookItemVertical} from \"../common/BookItem\";\r\nimport {WingBlank} from \"antd-mobile\";\r\nimport {LoadingState, LoadMoreBottomContent} from \"../common/LoadMoreBottomContent\";\r\nimport {getSearchResults, ISearchResult} from \"../../api/SearchApi\";\r\nimport {LoadMoreList} from \"../common/LoadMoreList\";\r\nimport {FutureElement} from \"../common/FutrueElement\";\r\nimport {AutoMaticSetLoadingState} from \"../../HOC/AutoMaticSetLoadingState\";\r\nimport {IPersistentProps} from \"../../HOC/PersistentComp\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport \"./index.less\";\r\nimport {Theme} from \"../../entities/Theme\";\r\n\r\nexport enum NavTypes {\r\n  WEEK_READ_NUM = \"week_read_num\",\r\n  READ_NUM = \"read_num\",\r\n  CREATED_AT = \"created_at\",\r\n  IS_ORIGINAL = \"is_original\"\r\n}\r\n\r\nexport interface ITopListPersistentData {\r\n  list: Array<ISearchResult> | null;\r\n  page: number;\r\n  navType: NavTypes;\r\n  scrollTop: number;\r\n  loadMoreState: LoadingState;\r\n}\r\n\r\nlet loading = false;\r\n\r\nexport const TopList: React.FC<RouteChildrenProps & IPersistentProps> = (props) => {\r\n  useDocumentTitle(\"榜单\");\r\n  const [loadingState, setLoadingState] = useState(LoadingState.LOADING);\r\n  const [navType, setNavType] = useState(NavTypes.WEEK_READ_NUM);\r\n  const [list, setList] = useState<Array<ISearchResult> | null>(null);\r\n  const [page, setPage] = useState(1);\r\n  const [shouldLoad, setShouldLoad] = useState(props.persistentData === null);\r\n  const scrollRef = useRef<HTMLDivElement>(null);\r\n  const [loadMoreState, setLoadMoreState] = useState(LoadingState.LOADING);\r\n  const data = props.persistentData as ITopListPersistentData;\r\n  const [shouldSetPos, setShouldSetPos] = useState(true);\r\n  const theme = useTheme()[0] as Theme;\r\n\r\n  useEffect(() => {\r\n    if (props.persistentData !== null) {\r\n      setLoadingState(LoadingState.LOADED);\r\n      setLoadMoreState(data.loadMoreState);\r\n      setList(data.list);\r\n      setPage(data.page);\r\n      setNavType(data.navType);\r\n    }\r\n\r\n  }, [scrollRef, setLoadingState, setNavType]);\r\n\r\n  useEffect(() => {\r\n    const data: ITopListPersistentData = {\r\n      list, page, navType, scrollTop: 0, loadMoreState\r\n    };\r\n\r\n    props.persistData(data);\r\n  }, [list, page, navType, loadMoreState]);\r\n\r\n  const handleScroll = useCallback((pos: number) => {\r\n    const data: ITopListPersistentData = {\r\n      list, page, navType, scrollTop: pos, loadMoreState\r\n    };\r\n    props.persistData(data);\r\n  }, [scrollRef, list, page, navType, loadMoreState]);\r\n\r\n  useEffect(() => {\r\n    if (!shouldLoad) {\r\n      return;\r\n    }\r\n    setPage(1);\r\n  }, [navType, shouldLoad]);\r\n\r\n  useEffect(() => {\r\n    if (shouldLoad) {\r\n      AutoMaticSetLoadingState(\r\n        getSearchResults({order: navType, page: 1})\r\n          .then((data) => {\r\n            (document.querySelector(\".load-more-list\") as HTMLDivElement).scrollTop = 0;\r\n            if (data.data.last_page === null) {\r\n              setLoadMoreState(LoadingState.FINISHED);\r\n              setLoadingState(LoadingState.FINISHED);\r\n            } else {\r\n              loading = false;\r\n              setLoadingState(LoadingState.LOADED);\r\n            }\r\n            setList(data.data.data.novel_list);\r\n          }),\r\n        (state) => setLoadingState(state)\r\n      )\r\n    }\r\n  }, [navType, shouldLoad]);\r\n\r\n  useEffect(() => {\r\n    if (loading) {\r\n      return;\r\n    }\r\n\r\n    loading = true;\r\n\r\n    if (page > 1 && shouldLoad) {\r\n      AutoMaticSetLoadingState(\r\n        getSearchResults({order: navType, page})\r\n          .then((data) => {\r\n            if (data.data.last_page === null) {\r\n              setLoadMoreState(LoadingState.FINISHED);\r\n            } else {\r\n              loading = false;\r\n              setLoadMoreState(LoadingState.LOADED);\r\n            }\r\n            setList([...list === null ? [] : list, ...data.data.data.novel_list]);\r\n          }),\r\n        (state) => {\r\n          if (state !== LoadingState.LOADED) {\r\n            setLoadMoreState(loadMoreState);\r\n          }\r\n        }\r\n      )\r\n    }\r\n  }, [page, navType, shouldLoad]);\r\n\r\n  const buildItem = useCallback((index: number) => {\r\n    if (list === null) {\r\n      return null;\r\n    }\r\n    return (\r\n      <BookItemVertical\r\n        onClick={(id) => goToPageWithCustomerId(props.history, {\r\n          pathname: `/detail/novel-${id}.html`,\r\n          state: {reload: true}\r\n        })}\r\n        id={list[index].id}\r\n        tags={list[index].tags === null ? [] : (list[index].tags as string).split(\"|\")}\r\n        desc={list[index].desc}\r\n        img={list[index].img}\r\n        name={\r\n          <div className={\"title-with-top\"}>\r\n            <span>{list[index].title}</span>\r\n            <span className={`top${index + 1}`}>{index + 1}</span>\r\n          </div>\r\n        }/>\r\n    )\r\n  }, [list]);\r\n\r\n  return (\r\n    <PageWithNavBar {...props} title={\"榜单\"} rightContent={\r\n      <p\r\n        style={{color: theme.mainColor}}\r\n        onClick={() => goToPageWithCustomerId(props.history, {pathname: \"/index.html\", state: {reload: true}})}\r\n        className={\"more-touch\"}\r\n      >\r\n        首页\r\n      </p>\r\n    }>\r\n      <div className=\"top-list\">\r\n        <TopListCategory onChange={(key) => {\r\n          setShouldSetPos(false);\r\n          setShouldLoad(true);\r\n          setPage(1);\r\n          setLoadMoreState(LoadingState.LOADING);\r\n          setNavType(key as NavTypes);\r\n          return true;\r\n        }} initialCategory={navType} items={\r\n          [\r\n            {key: NavTypes.WEEK_READ_NUM, name: \"周读榜\"},\r\n            {key: NavTypes.CREATED_AT, name: \"新书榜\"},\r\n            {key: NavTypes.IS_ORIGINAL, name: \"原创榜\"},\r\n            {key: NavTypes.READ_NUM, name: \"追生榜\"},\r\n          ]\r\n        }/>\r\n        <WingBlank size={\"sm\"}>\r\n\r\n        </WingBlank>\r\n        <FutureElement showLoadingDelay={0} loadingState={loadingState} showBarrier={false} contentBuilder={() => (\r\n          <LoadMoreList initPos={data === null || (!shouldSetPos) ? 0 : data.scrollTop} onScroll={handleScroll} onLoadMore={() => {\r\n            if (loadMoreState === LoadingState.FINISHED) {\r\n              return;\r\n            }\r\n            setShouldLoad(true);\r\n            setPage(page + 1);\r\n            loading = false;\r\n          }} itemCount={list === null ? 0 : list.length}\r\n                        itemBuilder={buildItem}>\r\n            {\r\n              list === null\r\n                ? null\r\n                : <LoadMoreBottomContent status={loadMoreState}/>\r\n            }\r\n          </LoadMoreList>\r\n        )}/>\r\n      </div>\r\n    </PageWithNavBar>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {useCallback, useEffect, useRef, useState} from \"react\";\r\nimport {PageWithNavBar} from \"../common/PageWithNavBar\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport {useDocumentTitle, useTheme} from \"../../hooks/Hooks\";\r\nimport {WingBlank} from \"antd-mobile\";\r\nimport {BookCategory} from \"./Category\";\r\nimport {BookItemVertical} from \"../common/BookItem\";\r\nimport {SearchBar} from \"../common/SearchBar\";\r\nimport {getNovelCategories, INovelCategory, INovelCategoryItem} from \"../../api/NovelApi\";\r\nimport {LoadingState, LoadMoreBottomContent} from \"../common/LoadMoreBottomContent\";\r\nimport {AutoMaticSetLoadingState} from \"../../HOC/AutoMaticSetLoadingState\";\r\nimport {FutureElement} from \"../common/FutrueElement\";\r\nimport {getSearchResults, ISearchResult} from \"../../api/SearchApi\";\r\nimport {IPersistentProps} from \"../../HOC/PersistentComp\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport {LoadMoreList} from \"../common/LoadMoreList\";\r\nimport {NoContent} from \"../common/NoContent\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport \"./index.less\";\r\n\r\nlet loading = false;\r\n\r\nexport interface IBookRepositoryPersistentData {\r\n  novelList: Array<ISear