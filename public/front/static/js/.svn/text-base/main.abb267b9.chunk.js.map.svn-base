r\n        break;\r\n      case \"榜单\":\r\n        goToPageWithCustomerId(props.history, {pathname: \"/top-list.html\", state: {reload: true}});\r\n        break;\r\n      case \"出版\":\r\n        goToPageWithCustomerId(props.history, {\r\n          pathname: `/list/publications-${getGender()}.html`,\r\n          state: {reload: true}\r\n        });\r\n    }\r\n  };\r\n\r\n  const data = getSuiteData();\r\n  return (\r\n    <FutureElement\r\n      showBarrier={false}\r\n      loadingState={loadingState}\r\n      showLoadingDelay={0}\r\n      contentBuilder={() => (\r\n        data === null\r\n          ? null\r\n          : <Fragment>\r\n            <HomeNavBar currentNav={currentGender} activeColor={(theme as Theme).mainColor} items={navs.items}\r\n                        onClick={(key) => setCurrentGender(key)}/>\r\n            <div ref={scrollRef} onScroll={handleScroll} className=\"home\">\r\n              <div className=\"content\" style={{paddingTop: \".15rem\"}}>\r\n                <HomeCarousel items={data.carousel}/>\r\n                <HomeCategories items={homeCategories} onClick={handleClick}/>\r\n                <div onClick={() => {\r\n                  goToPageWithCustomerId(props.history, {pathname: \"/pay.html\", state: {vip: true}});\r\n                }} className=\"vip-ad\">\r\n                  <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/vipadv.png\" alt=\"\"/>\r\n                </div>\r\n                <WhiteSpace style={{background: \"#f1f1f1\"}}/>\r\n                <HomeChunk1\r\n                  head={(\r\n                    <div onClick={() => {\r\n                      goToPageWithCustomerId(props.history, {\r\n                        pathname: `/detail/novel-${(getSuiteData() as IHomeDataItem).hot[0].novel_id}.html`,\r\n                        state: {reload: true}\r\n                      })\r\n                    }} className={\"hot-item\"}>\r\n                      <div className=\"hot-item-content\">\r\n                        <p className={\"title\"}>\r\n                          {(getSuiteData() as IHomeDataItem).hot[0].title}\r\n                          <span className={\"author\"}></span>\r\n                        </p>\r\n                        <p className=\"desc\"><img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/marks_icon.png\" alt=\"\"/>{filterChars((getSuiteData() as IHomeDataItem).hot[0].desc)}</p>\r\n                        <img className={\"img\"} src={(getSuiteData() as IHomeDataItem).hot[0].img} alt=\"\"/>\r\n                      </div>\r\n                    </div>\r\n                  )}\r\n                  type={0}\r\n                  title={\"本周热门\"}\r\n                  itemBuilder={buildHotItem}\r\n                  onBtnClick={() => {\r\n                    goToPageWithCustomerId(props.history, {\r\n                      pathname: `/list/weekly-${currentGender}.html`,\r\n                      state: {reload: true}\r\n                    })\r\n                  }}\r\n                  itemsCount={3}\r\n                  btn={\"查看更多\"}\r\n                />\r\n                <WhiteSpace style={{background: \"#f1f1f1\"}}/>\r\n                <HomeChunk1\r\n                  extra={(\r\n                    <div className=\"extra-data\">\r\n                      {\r\n                        getSuiteData() !== null\r\n                          ? (getSuiteData() as IHomeDataItem).weeknews.slice(4, 8).map((item, i) => {\r\n                            return (\r\n                              <div onClick={() => {\r\n                                goToPageWithCustomerId(props.history, {\r\n                                  pathname: `/detail/novel-${item.novel_id}.html`,\r\n                                  state: {reload: true}\r\n                                })\r\n                              }} key={i} className=\"extra-item\">\r\n                                <img src={item.img} alt=\"\" className=\"img\"/>\r\n                                <div className=\"text-content\">\r\n                                  <p className=\"title\">{item.title}</p>\r\n                                  <p className=\"recommend\">{item.recommend_num}万推荐</p>\r\n                                </div>\r\n                              </div>\r\n                            )\r\n                          })\r\n                          : null\r\n                      }\r\n                    </div>\r\n                  )}\r\n                  title={\"本周新书\"}\r\n                  itemsCount={4}\r\n                  itemBuilder={buildWeekNewsItem}\r\n                  onClick={() => {\r\n                  }}\r\n                  onBtnClick={() => {\r\n                    goToPageWithCustomerId(props.history, {\r\n                      pathname: `/list/news-${currentGender}.html`,\r\n                      state: {reload: true}\r\n                    })\r\n                  }}\r\n                  btn={\"查看更多\"}\r\n                  type={1}\r\n                />\r\n                <WhiteSpace style={{background: \"#f1f1f1\"}} />\r\n                <HomeChunk1\r\n                  type={1}\r\n                  title={\"主编力荐\"}\r\n                  itemBuilder={buildRecommendItem}\r\n                  onBtnClick={() => {\r\n                    goToPageWithCustomerId(props.history, {\r\n                      pathname: `/list/weekly-${currentGender}.html`,\r\n                      state: {reload: true}\r\n                    })\r\n                  }}\r\n                  itemsCount={(getSuiteData() as IHomeDataItem).recommend.slice(0, 8).length}\r\n                  btn={\"查看更多\"}\r\n                />\r\n                {/*<HomeChunk axis={Axis.horizontal}*/}\r\n                {/*           itemWidth={\"35%\"}*/}\r\n                {/*           title={\"主编力荐\"}*/}\r\n                {/*           itemPerLine={0}*/}\r\n                {/*           itemsCount={getItemCount(\"recommend\")}*/}\r\n                {/*           itemBuilder={buildRecommendItem}*/}\r\n                {/*           onClick={() => {*/}\r\n                {/*           }}*/}\r\n                {/*           onBtnClick={() => {*/}\r\n                {/*             goToPageWithCustomerId(props.history, {*/}\r\n                {/*               pathname: `/list/recommend-${currentGender}.html`,*/}\r\n                {/*               state: {reload: true}*/}\r\n                {/*             })*/}\r\n                {/*           }}*/}\r\n                {/*           btn={\"查看更多\"}*/}\r\n                {/*/>*/}\r\n                <WhiteSpace style={{background: \"#f1f1f1\"}}/>\r\n                {\r\n                  getItemCount(\"published\") === 0\r\n                    ? null\r\n                    : <Fragment>\r\n                      <HomeChunk axis={Axis.horizontal}\r\n                                 itemWidth={\"33.3333%\"}\r\n                                 title={\"出版物\"}\r\n                                 itemPerLine={3}\r\n                                 itemsCount={getItemCount(\"published\")}\r\n                                 itemBuilder={buildPublishedItem}\r\n                                 onClick={() => {\r\n                                 }}\r\n                                 onBtnClick={() => {\r\n                                   goToPageWithCustomerId(props.history, {\r\n                                     pathname: `/list/publications-${currentGender}.html`,\r\n                                     state: {reload: true}\r\n                                   })\r\n                                 }}\r\n                                 btn={\"查看更多\"}\r\n                      />\r\n                      <WhiteSpace style={{background: \"#f1f1f1\"}}/>\r\n                    </Fragment>\r\n                }\r\n                <HomeChunk axis={Axis.vertical}\r\n                           title={\r\n                             currentGender === \"man\"\r\n                               ? \"男生精选\"\r\n                               : \"女生精选\"\r\n                           }\r\n                           itemsCount={getItemCount(\"selection\")}\r\n                           itemBuilder={buildSelection}\r\n                           onClick={() => {\r\n                           }}\r\n                           onBtnClick={() => {\r\n                             goToPageWithCustomerId(props.history, {\r\n                               pathname: \"/book-repository.html\",\r\n                               state: {reload: true}\r\n                             })\r\n                           }}\r\n                           btn={\"查看更多\"}/>\r\n                <WingBlank>\r\n                  <HomeBottomCategories items={\r\n                    currentGender === \"man\"\r\n                      ? [\r\n                        {\r\n                          img: \"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/duShiYongBing.jpg\",\r\n                          key: {typeId: 3, gender: 1}\r\n                        },\r\n                        {\r\n                          img: \"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/yiShuChaoNeng.jpg\",\r\n                          key: {typeId: 5, gender: 1}\r\n                        },\r\n                        {\r\n                          img: \"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/jiPinShuangWen.jpg\",\r\n                          key: {typeId: 7, gender: 1}\r\n                        },\r\n                        {\r\n                          img: \"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/xuanHuanXianXia.jpg\",\r\n                          key: {typeId: 4, gender: 1}\r\n                        },\r\n                      ] : [\r\n                        {\r\n                          img: \"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/chuanYuanShiKong.jpg\",\r\n                          key: {typeId: 14, gender: 2}\r\n                        },\r\n                        {\r\n                          img: \"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/duanPianNueAi.jpg\",\r\n                          key: {typeId: 16, gender: 2}\r\n                        },\r\n                        {\r\n                          img: \"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/haoMenShiJia.jpg\",\r\n                          key: {typeId: 11, gender: 2}\r\n                        },\r\n                        {\r\n                          img: \"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/hunLianYanQing.jpg\",\r\n                          key: {typeId: 12, gender: 2}\r\n                        },\r\n                      ]\r\n                  } onClick={(key) => goToPageWithCustomerId(props.history, {\r\n                    pathname: \"/book-repository.html\",\r\n                    state: {reload: true, typeId: key.typeId, gender: key.gender}\r\n                  })}/>\r\n                </WingBlank>\r\n                <WhiteSpace style={{background: \"#f1f1f1\"}}/>\r\n                {\r\n                  wechatQrcode !== null\r\n                    ? <div onClickCapture={() => goToPageWithCustomerId(props.history, {pathname: \"/contact.html\"})} className=\"bottom-qrcode\">\r\n                      <img className={'bg'} src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/home_contact.png\"\r\n                           alt=\"\"/>\r\n                      <div className=\"qrcode-content-wrapper\">\r\n                        <img className={'qrcode-content'} src={wechatQrcode.customer_service} alt=\"\"/>\r\n                      </div>\r\n                    </div>\r\n                    : null\r\n                }\r\n                {/*<CopyRight/>*/}\r\n              </div>\r\n\r\n            </div>\r\n          </Fragment>\r\n      )}\r\n    />\r\n  )\r\n};\r\n\r\n\r\nexport default Home1;\r\n","import * as React from \"react\";\r\nimport searchIcon from \"./icons/search.png\";\r\nimport {useRef, useState} from \"react\";\r\nimport \"./SearchBar.less\";\r\n\r\nexport interface ISearchBarProps {\r\n  fixed?: boolean;\r\n  onClick?: () => void;\r\n  enable?: boolean;\r\n  value?: string;\r\n  onChange?: (val: string) => void;\r\n  onEnter?: (val: string) => void;\r\n}\r\n\r\nexport const SearchBar: React.FC<ISearchBarProps> = (props) => {\r\n  const [value, setValue] = useState<string | undefined>(props.value);\r\n\r\n  const handlerEnterPress = (evt: any) => {\r\n    const _evt = evt as KeyboardEvent;\r\n    if (evt.keyCode === 13) {\r\n      if (props.onEnter !== undefined) {\r\n        props.onEnter(value as string);\r\n      }\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div onClick={props.onClick === undefined ? () => {\r\n    } : () => (props.onClick as () => void)()} style={{position: (props.fixed as boolean) ? \"absolute\" : \"static\"}}\r\n         className=\"search-bar\">\r\n      <div className=\"search-bar-content\">\r\n        <div className=\"search-icon\">\r\n          <img src={searchIcon} alt=\"\"/>\r\n        </div>\r\n        <input onKeyUp={handlerEnterPress} value={props.value === undefined ? \"\" : (props.value as string)}\r\n               onChange={undefined === props.onChange ? () => {\r\n               } : (evt) => {\r\n                 setValue(evt.target.value);\r\n                 (props.onChange as (val: string) => void)(evt.target.value)\r\n               }}\r\n               disabled={!props.enable}\r\n               className=\"search-placeholder\" placeholder={\"请输入书名搜索\"}/>\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n\r\nSearchBar.defaultProps = {\r\n  fixed: true,\r\n  enable: false,\r\n};\r\n","import * as React from \"react\";\r\nimport \"./HomeNavBar.less\";\r\n\r\nexport interface IHomeNavBarItem<T> {\r\n  name: string;\r\n  key: T\r\n}\r\n\r\nexport interface IHomeNavBarProps<T> {\r\n  currentNav: T;\r\n  items: Array<IHomeNavBarItem<T>>;\r\n  activeColor: string | null,\r\n  onClick: (key: T) => void;\r\n}\r\n\r\nexport const HomeNavBar: React.FC<IHomeNavBarProps<string>> = (props) => {\r\n  const handleOnClick = (key: string) => {\r\n    props.onClick(key);\r\n  };\r\n\r\n  return (\r\n    <div className=\"home-nav-bar\">\r\n      {\r\n        props.items.map((item) => {\r\n          return (\r\n            <div key={item.key} onClick={() => handleOnClick(item.key)} className=\"home-nav-bar-item\">\r\n              <p style={{color: item.key === props.currentNav ? props.activeColor || \"#3688ff\" : \"#333333\"}}\r\n                 className={item.key === props.currentNav ? \"name-active\" : \"name\"}>{item.name}</p>\r\n            </div>\r\n          )\r\n        })\r\n      }\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport  \"./HomeCategories.less\";\r\n\r\nexport interface HomeCategoryItem {\r\n  key: string;\r\n  icon: string | null,\r\n  name: string,\r\n}\r\n\r\nexport interface IHomeCategoriesProps {\r\n  items: Array<HomeCategoryItem>;\r\n  onClick: (key: string) => void;\r\n}\r\n\r\nexport const HomeCategories: React.FC<IHomeCategoriesProps> = (props) => {\r\n  return (\r\n    <div className=\"home-categories\">\r\n      {\r\n        props.items.map((item) => {\r\n          return (\r\n            <div onClick={() => props.onClick(item.key)} key={item.name} className=\"home-category-item\">\r\n              <div className=\"icon\">\r\n                <img src={item.icon || \"\"} alt=\"\"/>\r\n              </div>\r\n              <div className=\"name\">{item.name}</div>\r\n            </div>\r\n          )\r\n        })\r\n      }\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {Fragment, useCallback, useEffect, useRef, useState} from \"react\";\r\nimport {SearchBar} from \"../../common/SearchBar\";\r\nimport {HomeNavBar} from \"./HomeNavBar\";\r\nimport {HomeCarousel} from \"./HomeCarousel\";\r\nimport {HomeCategories, HomeCategoryItem} from \"./HomeCategories\";\r\nimport {HomeChunk} from \"../../co