an className=\"xing-hao\">*</span>1元=100书币, 书币属于虚拟商品, 一经购买不得退换</p>*/}\n      {/*    <p className=\"tips-line\"><span className=\"xing-hao\">*</span>充值后书币到账可能有延迟, 1小时内未到账请与客服联系。<a*/}\n      {/*      onClick={() => {*/}\n      {/*        goToPageWithCustomerId(props.history, {pathname: \"/contact.html\"})*/}\n      {/*      }} className=\"tips-link\">点此联系客服</a></p>*/}\n      {/*    <p className=\"tips-line\"><span className=\"xing-hao\">*</span>工作时间: 周一到周六 8:30-12:00, 13:30-23:00</p>*/}\n      {/*  </div>*/}\n      {/*</WingBlank>*/}\n    </div>\n  )\n};\n","export enum BookManageEvent {\r\n  BOOK_MANAGE_DELETE = \"bookManageDelete\"\r\n}\r\n\r\nexport enum RewardPopupEvent {\r\n  REWARD_CONFIRMED = \"rewardConfirm\"\r\n}\r\n\r\nexport enum ReadMenu {\r\n  TO_DIRECTORY_CLICK = \"toDirectoryClick\",\r\n  ADD_TO_BOOKSHELF_CLICK = \"addToBookShelfClick\",\r\n  REWARD_CLICK = \"rewardClick\"\r\n}\r\n","import * as React from \"react\";\nimport {useEffect} from \"react\";\nimport {RouterProps} from \"react-router\";\nimport {useDocumentTitle, useUserInfo} from \"../../../hooks/Hooks\";\nimport \"./index.less\";\nimport cachetBg from \"./images/cachet.png\";\nimport cachetIcon from \"./icons/cachet_icon.png\";\nimport {Icon, List, WhiteSpace} from \"antd-mobile\";\nimport mineIcon0 from \"./icons/mine_0.png\";\nimport mineIcon2 from \"./icons/mine_2.png\";\nimport mineIcon3 from \"./icons/mine_3.png\";\nimport {getUserInfo, IUserInfo} from \"../../../api/UserApi\";\nimport {goToPageWithCustomerId} from \"../../../utils/RouteUtil\";\n\nexport const Mine: React.FC<RouterProps> = (props) => {\n  useDocumentTitle(\"个人中心\");\n  const date = Math.floor(new Date().getTime() / 1000);\n\n  const [_userInfo, setUserInfo] = useUserInfo();\n  const userInfo = _userInfo as IUserInfo;\n\n\n  useEffect(() => {\n    getUserInfo()\n      .then((data) => {\n        setUserInfo(data.data);\n      });\n  }, []);\n\n  return (\n    <div className=\"mine-container\">\n      <div className=\"background\"></div>\n      <div className=\"header\">\n        <div className=\"left\">\n          <p className=\"balance\">{userInfo !== null ? userInfo.balance : 0}</p>\n          <p className=\"balance-tip\">书币金额</p>\n        </div>\n        <div className=\"right\">\n          <div className=\"avatar-wrapper\">\n            {\n              userInfo === null\n                ? <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/person_default.png\" alt=\"\"/>\n                : <img src={userInfo.img} alt=\"\"/>\n            }\n          </div>\n          <div className={\"user-info\"}>\n            {\n              userInfo !== null && userInfo.vip_end_at > date\n                ? <span className={\"vip-icon\"}>VIP</span>\n                : null\n            }\n            <span className=\"id-number\">ID:{userInfo === null ? 0 : userInfo.id}</span>\n          </div>\n        </div>\n      </div>\n      <div onClick={() => goToPageWithCustomerId(props.history, {pathname: \"/vip-ad.html\"})} className=\"cachet\">\n        <img className=\"cachet-bg\" src={cachetBg} alt=\"\"/>\n        <div className=\"cachet-content\">\n          <div className=\"vip-icon\">\n            <img src={cachetIcon} alt=\"\"/>\n          </div>\n          <div className=\"right\">\n            <p className=\"title\">开通年度VIP</p>\n            <p className=\"tip\">365天每天1元畅读所有小说</p>\n          </div>\n        </div>\n      </div>\n\n      <List>\n        <List.Item onClick={() => goToPageWithCustomerId(props.history, {pathname: \"/pay.html\"})}\n                   extra={<Icon type={\"right\"}/>}>\n          <div className=\"mine-item\">\n            <div className=\"mine-icon\">\n              <img src={mineIcon0} alt=\"\"/>\n            </div>\n            书币充值\n          </div>\n        </List.Item>\n        {/*<List.Item onClick={() => goToPageWithCustomerId(props.history, {pathname: \"/coupon.html\"})} extra={<Icon type={\"right\"} />}>*/}\n        {/*  <div className=\"mine-item\">*/}\n        {/*    <div className=\"mine-icon\">*/}\n        {/*      <img src={mineIcon1} alt=\"\"/>*/}\n        {/*    </div>*/}\n        {/*    我的优惠券*/}\n        {/*  </div>*/}\n        {/*</List.Item>*/}\n        <List.Item onClick={() => goToPageWithCustomerId(props.history, {pathname: \"/pay-history.html\"})}\n                   extra={<Icon type={\"right\"}/>}>\n          <div className=\"mine-item\">\n            <div className=\"mine-icon\">\n              <img src={mineIcon2} alt=\"\"/>\n            </div>\n            消费记录\n          </div>\n        </List.Item>\n        <List.Item onClick={() => goToPageWithCustomerId(props.history, {pathname: \"/book-history.html\"})}\n                   extra={<Icon type={\"right\"}/>}>\n          <div className=\"mine-item\">\n            <div className=\"mine-icon\">\n              <img src={mineIcon3} alt=\"\"/>\n            </div>\n            阅读历史\n          </div>\n        </List.Item>\n        <WhiteSpace size={\"md\"} style={{background: \"#f1f1f1\"}}/>\n        <List.Item onClick={() => goToPageWithCustomerId(props.history, {pathname: \"/contact.html\"})}\n                   extra={<Icon type={\"right\"}/>}>\n          <div className=\"mine-item\">\n            <div className=\"mine-icon\">\n              <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/mine_contact_icon.png\" alt=\"\"/>\n            </div>\n            联系客服\n          </div>\n        </List.Item>\n        {/*<List.Item extra={<Icon type={\"right\"} />}>*/}\n        {/*  <div className=\"mine-item\">*/}\n        {/*    <div className=\"mine-icon\">*/}\n        {/*      <img src={mineIcon5} alt=\"\"/>*/}\n        {/*    </div>*/}\n        {/*    征文活动*/}\n        {/*  </div>*/}\n        {/*</List.Item>*/}\n      </List>\n    </div>\n  )\n};\n","import * as React from \"react\";\r\nimport \"./Check.less\";\r\nimport {useCallback} from \"react\";\r\n\r\nexport interface ICheckProps {\r\n  active: boolean;\r\n  onClick?: () => void;\r\n}\r\n\r\nexport const Check: React.FC<ICheckProps> = (props) => {\r\n\r\n  const handleClick = useCallback((evt: React.MouseEvent<HTMLDivElement>) => {\r\n    evt.stopPropagation();\r\n    if (props.onClick !== undefined) {\r\n      props.onClick();\r\n    }\r\n  }, [props.onClick]);\r\n\r\n  return (\r\n    <div onClick={handleClick} className={props.active ? \"check-wrapper active\" : \"check-wrapper\"}></div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {useBookManage} from \"../../hooks/Hooks\";\r\nimport {BookManageState} from \"../../entities/BookManageState\";\r\nimport {Check} from \"../common/Check\";\r\nimport PubSub from \"pubsub-js\";\r\nimport \"./MyBookManage.less\";\r\nimport {BookManageEvent} from \"../../entities/Event\";\r\n\r\nexport const MyBookManage: React.FC = (props) => {\r\n  const [_bookManageState, setBookManageState] = useBookManage();\r\n  const bookManageState = _bookManageState as BookManageState;\r\n\r\n  const setIsSelectedAll = () => {\r\n    bookManageState.isSelectedAll = !bookManageState.isSelectedAll;\r\n    setBookManageState({...bookManageState});\r\n  };\r\n\r\n  const doDelete = () => {\r\n    PubSub.publish(BookManageEvent.BOOK_MANAGE_DELETE, null);\r\n  };\r\n\r\n  return bookManageState.isOpen\r\n    ? <div className=\"my-book-manage\">\r\n      <div className=\"content\">\r\n        <div className=\"btn-select-all\">\r\n          <Check onClick={setIsSelectedAll} active={bookManageState.isSelectedAll}/>\r\n          <p onClick={setIsSelectedAll}>全选</p>\r\n        </div>\r\n        <div onClick={doDelete} className=\"btn-delete\">\r\n          <p>删除</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    : null\r\n};\r\n","import * as React from \"react\";\r\nimport {Fragment, useEffect, useState} from \"react\";\r\nimport {Icon} from \"antd-mobile\";\r\nimport {LoadingState} from \"./LoadMoreBottomContent\";\r\nimport \"./FutrueElement.less\";\r\n\r\nexport interface IFutureElementProps {\r\n  loadingState: LoadingState;\r\n  showBarrier: boolean;\r\n  contentBuilder: () => JSX.Element | null;\r\n  showLoadingDelay?: number;\r\n}\r\n\r\nexport const FutureElement: React.FC<IFutureElementProps> = (props) => {\r\n  const [loadingShow, setLoadingShow] = useState<boolean>(false);\r\n  const content = props.contentBuilder();\r\n\r\n  useEffect(() => {\r\n    let runner: null | NodeJS.Timeout = null;\r\n    if (\r\n      props.loadingState === LoadingState.LOADED ||\r\n      props.loadingState === LoadingState.FINISHED\r\n    ) {\r\n      setLoadingShow(false);\r\n    } else if (props.loadingState === LoadingState.LOADING) {\r\n      runner = setTimeout(() => {\r\n        setLoadingShow(true);\r\n      }, props.showLoadingDelay as number);\r\n    }\r\n\r\n    return () => {\r\n      if (runner !== null) {\r\n        clearTimeout(runner);\r\n      }\r\n    }\r\n  }, [props.loadingState]);\r\n\r\n  switch (props.loadingState) {\r\n    case LoadingState.LOADING:\r\n      if (loadingShow) {\r\n        return (\r\n          <Fragment>\r\n            <div className={props.showBarrier ? \"loading-wrapper barrier\" : \"loading-wrapper\"}>\r\n\r\n              <div className=\"loading-icon\">\r\n                <Icon type={\"loading\"} />\r\n              </div>\r\n            </div>\r\n            { content }\r\n          </Fragment>\r\n        );\r\n      } else {\r\n        return content;\r\n      }\r\n    case LoadingState.LOADED:\r\n      return props.contentBuilder();\r\n    case LoadingState.FINISHED:\r\n      return props.contentBuilder();\r\n    default:\r\n      return null;\r\n  }\r\n};\r\n\r\nFutureElement.defaultProps = {\r\n  showLoadingDelay: 1000,\r\n};\r\n","import * as React from \"react\";\r\nimport {match, RouteChildrenProps, RouteComponentProps} from \"react-router\";\r\nimport {ICustomerId, useIsLogin} from \"../hooks/Hooks\";\r\nimport {IsLoginState} from \"../hooks/Providers\";\r\nimport {FutureElement} from \"../pages/common/FutrueElement\";\r\nimport {LoadingState} from \"../pages/common/LoadMoreBottomContent\";\r\nimport {SERVER_ROOT} from \"../constants/Server\";\r\nimport qs from \"qs\";\r\n\r\nexport const NeedLogin: React.FC<RouteChildrenProps & { component?: React.ComponentType<RouteComponentProps<any>> | React.ComponentType<any>; }> = (props) => {\r\n  const params = qs.parse(window.location.hash.split(\"?\")[1]) as ICustomerId;\r\n  const isLogin = useIsLogin()[0] as IsLoginState;\r\n  const Child = props.component;\r\n\r\n  /**\r\n   *  目前只做微信登录 应当跳转后端页面进行微信登录唤醒\r\n   */\r\n  if (isLogin === false) {\r\n    window.document.title = \"登录中...\";\r\n      const url = encodeURIComponent(window.location.pathname + window.location.hash);\r\n    window.location.href = (\r\n      SERVER_ROOT +\r\n      `user/login/wechat?backurl=${url}&customer_id=` +\r\n      (params.cid === undefined ? \"\" : params.cid + \"\")\r\n    );\r\n    return <FutureElement\r\n      showBarrier={true}\r\n      showLoadingDelay={0}\r\n      loadingState={LoadingState.LOADING}\r\n      contentBuilder={() => null}\r\n    />;\r\n  } else {\r\n    return Child === undefined\r\n      ? props.children as JSX.Element\r\n      : <Child history={props.history} location={props.location} match={props.match as match}/>\r\n  }\r\n};\r\n","export enum Axis {\r\n  horizontal, vertical\r\n}\r\n","import * as React from \"react\";\r\nimport {CSSProperties} from \"react\";\r\nimport {Axis} from \"../../entities/Axis\";\r\nimport {WingBlank} from \"antd-mobile\";\r\nimport \"./HomeChunk.less\";\r\n\r\nexport interface IHomeChunkProps {\r\n  extra?: JSX.Element;\r\n  axis: Axis;\r\n  // available when axis is Axis.horizontal, list will have infinite height when axis = Axis.vertical\r\n  itemPerLine?: number;\r\n  itemsCount: number;\r\n  itemWidth?: string;\r\n  itemBuilder: (itemIndex: number) => JSX.Element;\r\n  btn: JSX.Element | string;\r\n  title: JSX.Element | string;\r\n  onBtnClick?: () => void;\r\n  onClick?: (index: number) => void;\r\n  withBorder?: boolean;\r\n}\r\n\r\nexport interface IHomeChunk1Props {\r\n  // 0 每行三那个 1 每行四个\r\n  type: 0 | 1,\r\n  head?: JSX.Element;\r\n  extra?: JSX.Element;\r\n  itemsCount: number;\r\n  itemBuilder: (itemIndex: number) => JSX.Element;\r\n  btn: JSX.Element | string;\r\n  title: JSX.Element | string;\r\n  onBtnClick?: () => void;\r\n  onClick?: (index: number) => void;\r\n}\r\n\r\nexport const HomeChunk1: React.FC<IHomeChunk1Props> = (props) => {\r\n\r\n  const buildItems = () => {\r\n    const items: Array<JSX.Element> = [];\r\n    for (let i = 0; i < props.itemsCount; i++) {\r\n      items.push((\r\n        <div key={i}\r\n             onClick={() => props.onClick === undefined ? () => {\r\n             } : props.onClick(i)}\r\n             style={itemStyle() as CSSProperties}\r\n             className={props.type === 0 ? \"item-wrapper1\" : \"item-wrapper2\"}>\r\n          {\r\n            props.itemBuilder(i)\r\n          }\r\n        </div>\r\n      ));\r\n    }\r\n    return items;\r\n  };\r\n\r\n  const containerClass = () => {\r\n    return \"content content-horizontal\";\r\n  };\r\n\r\n  const itemStyle = () => {\r\n    return {\r\n      width: props.type === 0 ? \"1.05rem\" : \".75rem\",\r\n      display: \"block\"\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"home-chunk\">\r\n      <div className={\"head\"}>\r\n        <WingBlank>\r\n          <div className=\"head-content\">\r\n            <WingBlank size={\"sm\"}>\r\n              <div className=\"title-wrapper\">\r\n                {\r\n                  typeof props.title !== \"object\"\r\n                    ? (\r\n                      <div className=\"home-chunk-title\">\r\n                        <p className=\"title-content\">\r\n                          {props.title as string}\r\n                        </p>\r\n                      </div>\r\n                    )\r\n                    : props.title as JSX.Element\r\n                }\r\n              </div>\r\n            </WingBlank>\r\n            <div onClick={props.onBtnClick === undefined ? () => {\r\n            } : props.onBtnClick} className=\"btn-wrapper\">\r\n              {\r\n                typeof props.btn !== \"object\"\r\n                  ? (\r\n                    <div className=\"home-chunk-btn\">\r\n                      <p className=\"btn-content\">\r\n                        {props.btn as string}\r\n                      </p>\r\n                    </div>\r\n                  )\r\n                  : props.btn as JSX.Element\r\n              }\r\n            </div>\r\n          </div>\r\n        </WingBlank>\r\n\r\n      </div>\r\n      {props.head}\r\n      <div className={containerClass()}>\r\n        {buildItems()}\r\n        {props.extra}\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n\r\nexport const HomeChunk: React.FC<IHomeChunkProps> = (props) => {\r\n\r\n  const buildItems = () => {\r\n    const items: Array<JSX.Element> = [];\r\n    for (let i = 0; i < props.itemsCount; i++) {\r\n      items.push((\r\n        <div key={i}\r\n             onClick={() => props.onClick === undefined ? () => {\r\n             } : props.onClick(i)}\r\n             style={itemStyle() as CSSProperties}\r\n             className=\"item-wrapper\">\r\n          {\r\n            props.itemBuilder(i)\r\n          }\r\n        </div>\r\n      ));\r\n    }\r\n    return items;\r\n  };\r\n\r\n  const containerClass = () => {\r\n    return props.axis === Axis.horizontal\r\n      ? (props.itemPerLine as number) <= 0\r\n        ? \"content content-horizontal infinite\"\r\n        : \"content content-horizontal\"\r\n      : \"content content-vertical\";\r\n  };\r\n\r\n  const itemStyle = () => {\r\n    if (props.axis === Axis.vertical) {\r\n      return {};\r\n    }\r\n    return {\r\n      width: (props.itemPerLine as number) <= 0\r\n        ? props.itemWidth\r\n        : 100 / (props.itemPerLine as number) + \"%\",\r\n      display: (props.itemPerLine as number) <= 0\r\n        ? \"inline-block\" : \"block\"\r\n    }\r\n  };\r\n\r\n  return (\r\n\r\n    <div className=\"home-chunk\">\r\n      <div className={props.withBorder === undefined || !props.withBorder ? \"head\" : \"head bordered\"}>\r\n        <WingBlank>\r\n          <div className=\"head-content\">\r\n            <WingBlank size={\"sm\"}>\r\n              <div className=\"title-wrapper\">\r\n                {\r\n                  typeof props.title !== \"object\"\r\n                    ? (\r\n                      <div className=\"home-chunk-title\">\r\n                        <p className=\"title-content\">\r\n                          {props.title as string}\r\n                        </p>\r\n                      </div>\r\n                    )\r\n                    : props.title as JSX.Element\r\n                }\r\n              </div>\r\n            </WingBlank>\r\n            <div onClick={props.onBtnClick === undefined ? () => {\r\n            } : props.onBtnClick} className=\"btn-wrapper\">\r\n              {\r\n                typeof props.btn !== \"object\"\r\n                  ? (\r\n                    <div className=\"home-chunk-btn\">\r\n                      <p className=\"btn-content\">\r\n                        {props.btn as string}\r\n                      </p>\r\n                    </div>\r\n                  )\r\n                  : props.btn as JSX.Element\r\n              }\r\n            </div>\r\n          </div>\r\n        </WingBlank>\r\n\r\n      </div>\r\n      <div className={containerClass()}>\r\n        {buildItems()}\r\n        {props.extra}\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n","import {GlobalVariablesProvider} from \"../hooks/Providers\";\r\nimport {GlobalVariables} from \"../entities/GlobalVariables\";\r\n\r\nconst format = require(\"date-format\");\r\n\r\n\r\nlet global: GlobalVariables = GlobalVariablesProvider.getValue() as GlobalVariables;\r\nGlobalVariablesProvider.addListener(() => global = GlobalVariablesProvider.getValue() as GlobalVariables);\r\n\r\nexport const GetSuiteSize = (size: number): number => {\r\n  return (global.realWidth as number) / (global.screenWidth as number) * size;\r\n};\r\n\r\nexport const GetSuitePx = (size: number): string => {\r\n  return GetSuiteSize(size) + \"px\";\r\n};\r\n\r\nexport const dateToStr = (date: number | string, pattern: string) => {\r\n  if (typeof date === \"string\") {\r\n    return date;\r\n  } else {\r\n    return format(pattern, new Date(date));\r\n  }\r\n};\r\n\r\nexport const strToDate = (str: string, pattern: string) => {\r\n  return format.parse(pattern, str);\r\n};\r\n\r\nexport const arabToChinese = (number: number) => {\r\n  let chnNumChar = [\"零\", \"一\", \"二\", \"三\", \"四\", \"五\", \"六\", \"七\", \"八\", \"九\"];\r\n  let chnUnitChar = [\"\", \"十\", \"百\", \"千\", \"万\", \"亿\", \"万亿\", \"亿亿\"];\r\n  let strIns = '', chnStr = '';\r\n  let unitPos = 0;\r\n  let zero = true;\r\n  while (number > 0) {\r\n    const v = number % 10;\r\n    if (v === 0) {\r\n      if (!zero) {\r\n        zero = true;\r\n        chnStr = chnNumChar[v] + chnStr;\r\n      }\r\n    } else {\r\n      zero = false;\r\n      strIns = chnNumChar[v];\r\n      strIns += chnUnitChar[unitPos];\r\n      chnStr = strIns + chnStr;\r\n    }\r\n    unitPos++;\r\n    number = Math.floor(number / 10);\r\n  }\r\n\r\n  return chnStr;\r\n};\r\n","import * as React from \"react\";\r\nimport {useEffect, useState} from \"react\";\r\nimport {LoadingState} from \"./LoadMoreBottomContent\";\r\n\r\nexport interface ILazyLoadImgProps {\r\n  src: string;\r\n}\r\n\r\nexport const LazyLoadImg: React.FC<ILazyLoadImgProps> = (props) => {\r\n  const [loadingState, setLoadingState] = useState(LoadingState.LOADING);\r\n\r\n  useEffect(() => {\r\n    const img = new Image();\r\n    img.onload = () => {\r\n      setLoadingState(LoadingState.LOADED);\r\n    };\r\n    img.src = props.src;\r\n\r\n    return () => {\r\n      img.onload = null;\r\n    }\r\n\r\n  }, [setLoadingState, props.src]);\r\n\r\n  return (\r\n    <div style={{width: \"100%\", height: \"100%\", background: \"#f1f1f1\"}} className={\"lazy-load-img\"}>\r\n      {\r\n        loadingState === LoadingState.LOADED\r\n          // @ts-ignore\r\n          ? <img style={{width: \"100%\", height: \"100%\"}} src={props.src === true ? \"\" : props.src} alt=\"\"/>\r\n          : null\r\n      }\r\n    </div>\r\n  );\r\n};\r\n\r\n","import * as React from \"react\";\r\nimport {useCallback} from \"react\";\r\nimport {dateToStr} from \"../../utils/Utils\";\r\nimport {LazyLoadImg} from \"./LazyLoadImg\";\r\nimport \"./BookItem.less\";\r\n\r\nexport interface IBookItemVerticalProps {\r\n  img: string;\r\n  name: string | JSX.Element;\r\n  desc: string | JSX.Element;\r\n  tags?: Array<string>;\r\n  id: number;\r\n  onClick?: (id: number) => void;\r\n}\r\n\r\nexport interface IBookItemProps {\r\n  img: string;\r\n  name: string;\r\n  extra?: JSX.Element\r\n  onClick?: () => void;\r\n  className?: string;\r\n}\r\n\r\nexport interface IBookItem1Props {\r\n  img: string;\r\n  name: string;\r\n  extra?: JSX.Element\r\n  onClick?: () => void;\r\n  className?: string;\r\n  author: string | null | JSX.Element;\r\n  type?: 0 | 1;\r\n  height: string;\r\n}\r\n\r\nexport interface IBookItemFreeProps {\r\n  img: string;\r\n  name: string;\r\n  desc: string;\r\n  startDate: number | string;\r\n  endDate: number | string;\r\n  onClick?: () => void;\r\n}\r\n\r\nconst filterDesc = (desc: string): string => {\r\n  return desc.length > 30\r\n    ? desc.slice(0, 27) + \"...\"\r\n    : desc;\r\n};\r\n\r\nexport const BookItem1: React.FC<IBookItem1Props> = (props) => {\r\n\r\n  const handleClick = useCallback(() => {\r\n    if (props.onClick === undefined) {\r\n\r\n    } else {\r\n      props.onClick();\r\n    }\r\n  }, [props]);\r\n\r\n  return (\r\n    <div onClick={handleClick} className={props.className === undefined ? \"book-item1\" : \"book-item1 \" + props.className}>\r\n      {\r\n        props.extra === undefined\r\n        