\" message.\r\n              console.log('Content is cached for offline use.');\r\n\r\n              // Execute callback\r\n              if (config && config.onSuccess) {\r\n                config.onSuccess(registration);\r\n              }\r\n            }\r\n          }\r\n        };\r\n      };\r\n    })\r\n    .catch(error => {\r\n      console.error('Error during service worker registration:', error);\r\n    });\r\n}\r\n\r\nfunction checkValidServiceWorker(swUrl: string, config?: Config) {\r\n  // Check if the service worker can be found. If it can't reload the page.\r\n  fetch(swUrl)\r\n    .then(response => {\r\n      // Ensure service worker exists, and that we really are getting a JS file.\r\n      const contentType = response.headers.get('content-type');\r\n      if (\r\n        response.status === 404 ||\r\n        (contentType != null && contentType.indexOf('javascript') === -1)\r\n      ) {\r\n        // No service worker found. Probably a different app. Reload the page.\r\n        navigator.serviceWorker.ready.then(registration => {\r\n          registration.unregister().then(() => {\r\n            window.location.reload();\r\n          });\r\n        });\r\n      } else {\r\n        // Service worker found. Proceed as normal.\r\n        registerValidSW(swUrl, config);\r\n      }\r\n    })\r\n    .catch(() => {\r\n      console.log(\r\n        'No internet connection found. App is running in offline mode.'\r\n      );\r\n    });\r\n}\r\n\r\nexport function unregister() {\r\n  if ('serviceWorker' in navigator) {\r\n    navigator.serviceWorker.ready.then(registration => {\r\n      registration.unregister();\r\n    });\r\n  }\r\n}\r\n","import * as React from \"react\";\r\nimport {useCallback, useEffect, useState} from \"react\";\r\nimport {usePayCallbackUrl, useRewardItems, useRewardPopup, useTheme, useUserInfo} from \"../../hooks/Hooks\";\r\nimport {Button, Icon, InputItem, Modal, Toast} from \"antd-mobile\";\r\nimport {RewardPopupEvent} from \"../../entities/Event\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport {IUserInfo} from \"../../api/UserApi\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport \"./RewardPopup.less\";\r\nimport {SERVER_ROOT} from \"../../constants/Server\";\r\n\r\nexport interface IRewardConfirmDetail {\r\n  id: number;\r\n  amount: number;\r\n}\r\n\r\nexport const RewardPopup: React.FC<RouteChildrenProps> = (props) => {\r\n  const [userInfo,] = useUserInfo();\r\n  const [rewardPopupOpen, setRewardPopupOpen] = useRewardPopup();\r\n  const [rewardItems,] = useRewardItems();\r\n  const [amount, setAmount] = useState(1);\r\n  const [currentChose, setCurrentChose] = useState(-1);\r\n  const theme = useTheme()[0] as Theme;\r\n  const [payCallbackUrl, setPayCallbackUrl] = usePayCallbackUrl();\r\n\r\n  useEffect(() => {\r\n    if (!rewardPopupOpen) {\r\n      setCurrentChose(-1);\r\n      setAmount(1);\r\n    }\r\n  }, [rewardPopupOpen, setCurrentChose, setAmount]);\r\n\r\n  const handleConfirm = useCallback(() => {\r\n    PubSub.publish(RewardPopupEvent.REWARD_CONFIRMED, {\r\n      id: rewardItems[currentChose].id,\r\n      amount\r\n    });\r\n  }, [currentChose, amount]);\r\n\r\n  const handleAdd = useCallback(() => {\r\n    if (currentChose === -1) {\r\n      return;\r\n    }\r\n    setAmount(amount + 1);\r\n  }, [setAmount, currentChose, amount]);\r\n\r\n  const buttonText = useCallback(() => {\r\n    if (currentChose === -1 || rewardItems[currentChose].coin * amount <= (userInfo as IUserInfo).balance) {\r\n      if (amount <= 0) {\r\n        return \"请至少打赏一个礼物\";\r\n      }\r\n      return \"确认打赏\";\r\n    } else {\r\n      return \"书币不足请先充值\";\r\n    }\r\n\r\n  }, [currentChose, rewardItems, amount, userInfo]);\r\n\r\n  const handleDecrease = useCallback(() => {\r\n    if (currentChose === -1) {\r\n      return;\r\n    }\r\n\r\n    if (amount <= 1) {\r\n      Toast.fail(\"请至少打赏一个礼物\", 1.5, undefined, false);\r\n      return;\r\n    }\r\n\r\n    setAmount(amount - 1);\r\n  }, [setAmount, currentChose, amount]);\r\n\r\n  const handleClick = useCallback(() => {\r\n    if (currentChose === -1) {\r\n      Toast.info(\"请选择打赏的礼物\", 1.5, undefined, false);\r\n      return;\r\n    } else {\r\n      if (amount === 0) {\r\n        Toast.fail(\"请至少打赏一个礼物\", 1.5, undefined, false);\r\n        return;\r\n      }\r\n      if (rewardItems[currentChose].coin * amount <= (userInfo as IUserInfo).balance) {\r\n        handleConfirm();\r\n        return\r\n      }\r\n      setRewardPopupOpen(false);\r\n      setPayCallbackUrl(SERVER_ROOT + \"front/\" + window.location.hash);\r\n      return goToPageWithCustomerId(props.history, {pathname: \"/pay.html\"});\r\n    }\r\n\r\n  }, [currentChose, rewardItems, amount, userInfo, props, setRewardPopupOpen]);\r\n\r\n  return userInfo === null ? null : (\r\n    <Modal\r\n      visible={rewardPopupOpen === null ? false : rewardPopupOpen}\r\n      maskClosable={true}\r\n      popup\r\n      animationType={\"slide-up\"}\r\n    >\r\n      <div className=\"reward-content\">\r\n        <div className=\"header\">\r\n          <p style={{color: theme.mainColor}}>打赏</p>\r\n          <p className=\"balance\">书币余额: {userInfo === null ? 0 : userInfo.balance}</p>\r\n          <div className=\"close-btn\">\r\n            <Icon onClick={() => setRewardPopupOpen(false)} className={\"more-touch\"} type={\"cross\"}/>\r\n          </div>\r\n        </div>\r\n        <div className=\"gifts\">\r\n          {\r\n            rewardItems.map((item, index) => {\r\n              return (\r\n                <div key={index} onClick={() => setCurrentChose(index)}\r\n                     className={currentChose === index ? \"gift-item active\" : \"gift-item\"}>\r\n                  <img style={currentChose === index ? {borderColor: theme.mainColor} : {}} src={item.img} alt=\"\"/>\r\n                  <p style={currentChose === index ? {color: theme.mainColor} : {}} className=\"name\">{item.name}</p>\r\n                  <p style={currentChose === index ? {color: theme.mainColor} : {}} className=\"price\">{item.coin}书币</p>\r\n                </div>\r\n              )\r\n            })\r\n          }\r\n        </div>\r\n        <div className=\"amount\">\r\n          <p className={\"tip\"}>数量:</p>\r\n          <div className=\"actions\">\r\n            <div onClick={handleAdd} style={{background: currentChose > -1 ? theme.mainColor : \"#ccc\"}}\r\n                 className=\"add\"><img src=\"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/%2B_icon.png\" alt=\"\"/>\r\n            </div>\r\n            <InputItem onBlur={() => {\r\n              setTimeout(() => {\r\n                window.scrollTo(0, 0);\r\n              }, 100);\r\n            }} onChange={(evt) => {\r\n              if (evt === \"0\") {\r\n                return;\r\n              }\r\n              setAmount(evt === \"\" ? 0 : parseInt(evt))\r\n            }} type={\"number\"} value={amount + \"\"} />\r\n            <div onClick={handleDecrease} style={{background: currentChose > -1 ? theme.mainColor : \"#ccc\"}}\r\n                 className=\"decrease\"><img src=\"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/-_icon.png\" alt=\"\"/>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <Button style={{background: theme.mainColor}} className={\"btn-confirm\"} onClick={handleClick} type={\"primary\"}>{buttonText()}</Button>\r\n      </div>\r\n    </Modal>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {Button, Icon, Modal} from \"antd-mobile\";\r\nimport {useReadState, useReadThemePopup} from \"../../hooks/Hooks\";\r\nimport {useCallback, useEffect} from \"react\";\r\nimport {IReadState, themeType} from \"../../entities/ReadState\";\r\nimport bookShelfIcon from \"./icons/reading_bookshelf_icon.png\";\r\n\r\nimport homeIcon from \"./icons/reading_home_icon.png\";\r\nimport rewardIcon from \"./icons/reading_like_icon.png\";\r\nimport directoryIcon from \"./icons/reading_list_icon.png\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport {ReadMenu} from \"../../entities/Event\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport {addToCache} from \"../../utils/LocalStorageUtil\";\r\nimport \"./ReadThemePopup.less\";\r\n\r\n\r\nconst iconAttrs = [\r\n  {theme: \"普通\", bg: \"#f5f5f5\", border: \"\"},\r\n  {theme: \"泥巴色\", bg: \"url(https://qcdn.zhangzhongyun.com/spa/179c1ccfd3.jpg) repeat 100% #e4c8a1\", border: \"\"},\r\n  {theme: \"粉色\", bg: \"#fdcdb1\", border: \"\"},\r\n  {theme: \"草原绿\", bg: \"#bbecc2\", border: \"\"},\r\n  {theme: \"灰色\", bg: \"#d1d1d1\", border: \"\"},\r\n  {theme: \"夜间模式\", bg: \"url(https://qcdn.zhangzhongyun.com/spa/d852cd8048.png) no-repeat 50% #1b1714\", border: \"\"},\r\n];\r\n\r\nexport const ReadThemePopup: React.FC<RouteChildrenProps> = (props) => {\r\n  const [readThemePopup, setReadThemePopup] = useReadThemePopup();\r\n  const [readTheme, setReadTheme] = useReadState();\r\n\r\n  useEffect(() => {\r\n    addToCache(\"readTheme\", readTheme);\r\n  }, [readTheme]);\r\n\r\n  const setTheme = useCallback((theme: IReadState) => {\r\n    setReadTheme(theme);\r\n  }, [readTheme, setReadTheme]);\r\n\r\n  const handleFontSizeChange = useCallback((decrease: boolean = false) => {\r\n    if (decrease) {\r\n      if (readTheme.fontSize > 16) {\r\n        setReadTheme({...readTheme, fontSize: readTheme.fontSize - 2});\r\n      }\r\n    } else {\r\n      setReadTheme({...readTheme, fontSize: readTheme.fontSize + 2});\r\n    }\r\n  }, [readTheme, setReadTheme]);\r\n\r\n  return (\r\n    <Modal\r\n      visible={readThemePopup === null ? false : readThemePopup}\r\n      popup\r\n      animationType={\"slide-up\"}>\r\n\r\n      <div style={{background: readTheme.theme === \"夜间模式\" ? \"#333\" : \"#fff\"}} className=\"read-theme-popup\">\r\n        <div onClick={() => setReadThemePopup(false)} className=\"close-btn\">\r\n          <Icon type={\"cross\"} />\r\n        </div>\r\n\r\n        <div style={{borderColor: readTheme.theme === \"夜间模式\" ? \"#666\" : \"#f1f1f1\"}} className=\"skins-wrapper\">\r\n          <div className=\"themes\">\r\n            <div className=\"skins\">\r\n              {\r\n                iconAttrs.map((item) => {\r\n                  return (\r\n                    <div onClick={() => setTheme({...readTheme, theme: item.theme as themeType})} key={item.theme} style={{background: item.bg}} className={item.theme === readTheme.theme ? \"skin-item active\" : \"skin-item\"} />\r\n                  )\r\n                })\r\n              }\r\n            </div>\r\n            <div className=\"font-options-wrapper\">\r\n              <div className=\"font-options\">\r\n                <Button style={{backgroundColor: readTheme.theme === \"夜间模式\" ? \"#333\" : \"#fff\"}} onClick={() => handleFontSizeChange(true)} className={\"btn\"}>A-</Button>\r\n                <span>{ readTheme.fontSize }</span>\r\n                <Button style={{backgroundColor: readTheme.theme === \"夜间模式\" ? \"#333\" : \"#fff\"}} onClick={() => handleFontSizeChange()} className={\"btn\"}>A+</Button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div className=\"options\">\r\n          <div onClick={() => {\r\n            setReadThemePopup(false);\r\n            goToPageWithCustomerId(props.history, {pathname: \"/index.html\", state: {reload: true}})\r\n          }} className=\"option-item\">\r\n            <img src={homeIcon} alt=\"\"/>\r\n            <p>返回首页</p>\r\n          </div>\r\n          <div onClick={() => PubSub.publish(ReadMenu.TO_DIRECTORY_CLICK, null)} className=\"option-item\">\r\n            <img src={directoryIcon} alt=\"\"/>\r\n            <p>前往目录</p>\r\n          </div>\r\n          <div onClick={() => PubSub.publish(ReadMenu.ADD_TO_BOOKSHELF_CLICK, null)} className=\"option-item\">\r\n            <img src={bookShelfIcon} alt=\"\"/>\r\n            <p>加入书架</p>\r\n          </div>\r\n          <div onClick={() => PubSub.publish(ReadMenu.REWARD_CLICK, null)} className=\"option-item\">\r\n            <img src={rewardIcon} alt=\"\"/>\r\n            <p>我要打赏</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n    </Modal>\r\n  )\r\n};\r\n","import React, {Fragment, useEffect, useState} from 'react';\r\nimport ReactDOM from 'react-dom';\r\nimport App from './App';\r\nimport * as serviceWorker from './serviceWorker';\r\nimport {useCheckLogin, useSearchParamsProvider, useSignSuccess, useWechatInfo} from \"./hooks/Hooks\";\r\nimport {RewardPopup} from \"./pages/common/RewardPopup\";\r\nimport {Route} from \"react-router\";\r\nimport {HashRouter} from \"react-router-dom\";\r\nimport {ReadThemePopup} from \"./pages/common/ReadThemePopup\";\r\nimport {getWechatInfo} from \"./api/OtherApi\";\r\nimport qs from \"qs\";\r\nimport \"./index.less\";\r\n\r\nexport interface ICustomerParams {\r\n  [propName: string]: any;\r\n}\r\n\r\nconst Main: React.FC = (props) => {\r\n  useCheckLogin();\r\n\r\n  const [, setSearchParams] = useSearchParamsProvider();\r\n  const [, setWechatInfo] = useWechatInfo();\r\n  const [signSuccessOpen, setSignSuccessOpen] = useSignSuccess();\r\n\r\n  useEffect(() => {\r\n    const params = qs.parse(window.location.hash.split(\"?\")[1]) as ICustomerParams;\r\n\r\n    console.log(params);\r\n    setSearchParams(params);\r\n    if (params.cid !== undefined) {\r\n      getWechatInfo(params[\"cid\"])\r\n        .then((data) => {\r\n          setWechatInfo(data.data);\r\n        });\r\n    }\r\n  }, []);\r\n\r\n  return (\r\n    <Fragment>\r\n      {\r\n        signSuccessOpen\r\n          ? <SignSuccess/>\r\n          : null\r\n      }\r\n      <HashRouter>\r\n        <App/>\r\n        <Route children={\r\n          (router) => <RewardPopup {...router} />\r\n        }/>\r\n        <Route children={\r\n          (router) => <ReadThemePopup {...router} />\r\n        }/>\r\n      </HashRouter>\r\n    </Fragment>\r\n  )\r\n};\r\n\r\nconst SignSuccess: React.FC = (props) => {\r\n  const [random] = useState(Math.random());\r\n  const [successPopup, setSuccessPopup] = useSignSuccess();\r\n\r\n  useEffect(() => {\r\n    const img = new Image();\r\n    img.onload = () => {\r\n      setTimeout(() => setSuccessPopup(false), 2000);\r\n    };\r\n    img.src = \"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/sign_in_success.gif?t=\" + random;\r\n  }, []);\r\n\r\n  // TODO 暂时取消签到成功动画\r\n  // return (\r\n  //   <div className=\"sign-success\">\r\n  //     <img className={\"sign-img\"}\r\n  //          src={\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/sign_in_success.gif?t=\" + random} alt=\"\"/>\r\n  //   </div>\r\n  // )\r\n  return null;\r\n};\r\n\r\nReactDOM.render(<Main/>, document.getElementById('root'));\r\n\r\n// If you want your app to work offline and load faster, you can change\r\n// unregister() to register() below. Note this comes with some pitfalls.\r\n// Learn more about service workers: https://bit.ly/CRA-PWA\r\nserviceWorker.unregister();\r\n"],"sourceRoot":""}