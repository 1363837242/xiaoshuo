wardPopup,\r\n  useSearchParamsProvider,\r\n  useUserInfo\r\n} from \"../../hooks/Hooks\";\r\nimport {IReadState} from \"../../entities/ReadState\";\r\nimport {Toast} from \"antd-mobile\";\r\nimport {getNovelContent, getNovelRecommends, INovelContent, INovelRecommend} from \"../../api/NovelApi\";\r\nimport {LoadingState} from \"../common/LoadMoreBottomContent\";\r\nimport {AutoMaticSetLoadingState} from \"../../HOC/AutoMaticSetLoadingState\";\r\nimport {IPersistentProps} from \"../../HOC/PersistentComp\";\r\nimport {ReadMenu, RewardPopupEvent} from \"../../entities/Event\";\r\nimport {addNovelToBookShelf, IUserInfo} from \"../../api/UserApi\";\r\nimport {IRewardConfirmDetail} from \"../common/RewardPopup\";\r\nimport {doReward} from \"../../api/RewardApi\";\r\nimport {goToPageWithCustomerId, goToReadPage} from \"../../utils/RouteUtil\";\r\nimport {addToCache, getCache} from \"../../utils/LocalStorageUtil\";\r\nimport {ResponseCode} from \"../../utils/ResponseCode\";\r\nimport {ReadNavbar} from \"./ReadNavbar\";\r\nimport {ReadThemeNavbar} from \"./ReadThemeNavbar\";\r\nimport \"./index.less\";\r\nimport {SERVER_ROOT} from \"../../constants/Server\";\r\nimport ServiceButton from \"../common/ServiceButton\";\r\nimport {ReloadRead} from \"../../components/ReloadRead\";\r\nimport {IResponseData} from \"../../utils/RequestShorcuts\";\r\n\r\ninterface IReadRouteParams {\r\n  id: string;\r\n  sectionId: string;\r\n}\r\n\r\ntype theme = \"普通\" | \"泥巴色\" | \"粉色\" | \"草原绿\" | \"灰色\" | \"夜间模式\";\r\n\r\nconst themes = [\"普通\", \"泥巴色\", \"粉色\", \"草原绿\", \"灰色\", \"夜间模式\"];\r\n\r\nconst bgColor = (themeType: theme): string => {\r\n  switch (themeType) {\r\n    case \"普通\":\r\n      return \"#ededed\";\r\n    case \"泥巴色\":\r\n      return \"#e4c8a1 url(https://qcdn.zhangzhongyun.com/spa/179c1ccfd3.jpg) repeat center 100%\";\r\n    case \"粉色\":\r\n      return \"#f1e8e7\";\r\n    case \"草原绿\":\r\n      return \"#e7f1e8\";\r\n    case \"灰色\":\r\n      return \"#d1d1d1\";\r\n    case \"夜间模式\":\r\n      return \"#1b1714\";\r\n  }\r\n};\r\n\r\nconst fontColor = (themeType: theme) => {\r\n  switch (themeType) {\r\n    case \"普通\":\r\n      return \"#373e44\";\r\n    case \"泥巴色\":\r\n      return \"rgba(0, 0, 0, .85)\";\r\n    case \"粉色\":\r\n      return \"#896147\";\r\n    case \"草原绿\":\r\n      return \"#506d54\";\r\n    case \"灰色\":\r\n      return \"#5f5f5f\";\r\n    case \"夜间模式\":\r\n      return \"#746854\";\r\n  }\r\n};\r\n\r\nconst Recommends: React.FC<RouteChildrenProps & { items: Array<INovelRecommend> }> = (props) => {\r\n  const [readTheme] = useReadState();\r\n\r\n  return (\r\n    <div className=\"recommend-wrapper\">\r\n      <p className=\"title\" style={{color: readTheme.theme === \"夜间模式\" ? \"#fff\" : \"#666\"}}>相关推荐</p>\r\n      {\r\n        props.items.map((item) => (\r\n          <a\r\n            style={{color: readTheme.theme === \"夜间模式\" ? \"rgb(173, 143, 94)\" : \"rgb(204, 162, 102)\"}}\r\n            key={item.id}\r\n            className={\"recommend-link\"}\r\n            onClick={() => {\r\n              goToReadPage(props.history, item.id, 0, true, false);\r\n            }}\r\n          >\r\n            {item.desc.length > 40 ? item.desc.slice(0, 37) + \"...\" : item.desc}\r\n          </a>\r\n        ))\r\n      }\r\n    </div>\r\n  )\r\n};\r\n\r\nconst remSize = (size: number) => {\r\n  return size / 100 + \"rem\";\r\n};\r\n\r\nconst RText: React.FC = (props) => {\r\n  const [_readState] = useReadState();\r\n  const readState = _readState as IReadState;\r\n\r\n  return <div style={{fontSize: remSize(readState.fontSize), color: fontColor(readState.theme)}}>{props.children}</div>\r\n};\r\n\r\nexport interface IBottomActionProps {\r\n  onClick?: (prev: boolean) => void;\r\n  isFinished: boolean;\r\n  isFirst: boolean;\r\n  onToDirectory: () => void;\r\n}\r\n\r\nconst BottomActions: React.FC<IBottomActionProps> = (props) => {\r\n  const [_readState, setReadState] = useReadState();\r\n  const readState = _readState as IReadState;\r\n  const [readThemePopup, setReadThemePopup] = useReadThemePopup();\r\n\r\n  useEffect(() => {\r\n    return () => {\r\n      setReadThemePopup(false);\r\n    }\r\n  }, []);\r\n\r\n  const handleOnClick = (prev: boolean) => {\r\n    if (undefined !== props.onClick) {\r\n      props.onClick(prev);\r\n    }\r\n  };\r\n  return (\r\n    <div className=\"bottom-actions\">\r\n      <div onClick={() => {\r\n        if (!props.isFirst) {\r\n          handleOnClick(true);\r\n        }\r\n      }} style={{color: fontColor(readState.theme)}}\r\n           className=\"btn-chapter btn-prev\">\r\n        <p style={{opacity: props.isFirst ? \".5\" : \"1\"}}>上一章节</p>\r\n      </div>\r\n      <div style={{color: fontColor(readState.theme), fontWeight: \"bold\"}} onClick={props.onToDirectory}\r\n           className={\"menu-icon-wrapper\"}>\r\n        目录\r\n      </div>\r\n      <div onClick={() => {\r\n        if (!props.isFinished) {\r\n          handleOnClick(false);\r\n        }\r\n      }} style={{color: fontColor(readState.theme)}}\r\n           className=\"btn-chapter btn-next\">\r\n        <p style={{opacity: props.isFinished ? \".5\" : \"1\"}}>下一章节</p>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport interface IReadPersistentData {\r\n  scrollTop: number;\r\n  novelContent: INovelContent | null;\r\n}\r\n\r\nexport interface ILastRead {\r\n  id: string;\r\n  chapter: string;\r\n}\r\n\r\nexport const Read: React.FC<RouteChildrenProps & IPersistentProps> = (props) => {\r\n  const [_readState, setReadState] = useReadState();\r\n  const readState = _readState as IReadState;\r\n  readState.lineHeight = () => readState.fontSize * 1.5;\r\n  const {id, sectionId} = (((props.match as match).params) as IReadRouteParams);\r\n  const [loadingState, setLoadingState] = useState<LoadingState | null>(null);\r\n  const [novelContent, setNovelContent] = useState<INovelContent | null>(null);\r\n  const [novelRecommends, setNovelRecommends] = useState<Array<INovelRecommend>>([]);\r\n  const [shouldLoad, setShouldLoad] = useState(props.persistentData === null);\r\n  const [rewardPopupOpen, setRewardPopupOpen] = useRewardPopup();\r\n  const [readThemePopup, setReadThemePopup] = useReadThemePopup();\r\n  const containerRef: React.RefObject<HTMLDivElement> = useRef(null);\r\n  const [, setUserInfo] = useUserInfo();\r\n  const [prev, setPrev] = useState(false);\r\n  const [payCallbackUrl, setPayCallbackUrl] = usePayCallbackUrl();\r\n  const [queryParams,] = useSearchParamsProvider();\r\n  const [reloadDisplay, setReloadDisplay] = useState(false);\r\n  const [extraNovel, setExtraNovel] = useState<IResponseData<INovelContent> | undefined>(undefined);\r\n\r\n  const getNovelContentWithExtra = useCallback((() => {\r\n    const section = parseInt(sectionId);\r\n    let _extraNovel = extraNovel;\r\n    return (id: number, section: number, page?: string, params: { [propName: string]: any } = {}): Promise<IResponseData<INovelContent>> => {\r\n      if (page === 'last') {\r\n        setExtraNovel(undefined);\r\n        _extraNovel = undefined;\r\n      }\r\n\r\n      console.log(sectionId, '预载内容', _extraNovel);\r\n      if (_extraNovel !== undefined && (_extraNovel.err_code !== 0 || _extraNovel.data.num >= section)) {\r\n        return new Promise((res) => {\r\n          if ((_extraNovel as IResponseData<INovelContent>).err_code === ResponseCode.SUCCESS) {\r\n            getNovelContent(id, section + 1, page, {...params, pre_content: 1})\r\n              .then((_data) => {\r\n                if (_data.err_code !== ResponseCode.SUCCESS || _data.data.num > section) {\r\n                  setExtraNovel(_data);\r\n                }\r\n              });\r\n          }\r\n\r\n          res(_extraNovel);\r\n        });\r\n      }\r\n\r\n      return getNovelContent(id, section, page, params)\r\n        .then((data) => {\r\n          if (data.err_code === ResponseCode.SUCCESS) {\r\n            getNovelContent(id, data.data.num + 1, page, {...params, pre_content: 1})\r\n              .then((_data) => {\r\n                if (_data.err_code !== ResponseCode.SUCCESS || _data.data.num > section) {\r\n                  setExtraNovel(_data);\r\n                }\r\n              });\r\n          }\r\n\r\n          return data;\r\n        })\r\n    }\r\n  })(), [sectionId]);\r\n\r\n  const persistData = useCallback(() => {\r\n    props.persistData({\r\n      scrollTop: 0,\r\n      novelContent: novelContent\r\n    });\r\n  }, [novelContent, setNovelContent, containerRef]);\r\n\r\n  useEffect(() => {\r\n    const menuSubId = PubSub.subscribe(ReadMenu.TO_DIRECTORY_CLICK, (evt: ReadMenu) => {\r\n      goToPageWithCustomerId(props.history, {pathname: `/detail/chapter-${id}.html`})\r\n    });\r\n\r\n    const addSubId = PubSub.subscribe(ReadMenu.ADD_TO_BOOKSHELF_CLICK, (evt: ReadMenu) => {\r\n      addNovelToBookShelf(parseInt(id))\r\n        .then((data) => {\r\n          if (data.err_code === ResponseCode.SUCCESS) {\r\n            Toast.success(\"已添加到书架\", 1.5, undefined, false);\r\n          }\r\n        })\r\n    });\r\n\r\n    const rewardSubId = PubSub.subscribe(ReadMenu.REWARD_CLICK, (evt: ReadMenu) => {\r\n      setReadThemePopup(false);\r\n      setRewardPopupOpen(true);\r\n    });\r\n\r\n    const doRewardSubId = PubSub.subscribe(RewardPopupEvent.REWARD_CONFIRMED, (messageType: RewardPopupEvent, data: IRewardConfirmDetail) => {\r\n      doReward(data.id, parseInt(id), data.amount)\r\n        .then((data) => {\r\n          if (data.err_code === ResponseCode.SUCCESS) {\r\n            Toast.success(data.err_msg, 1.5, undefined, false);\r\n            setRewardPopupOpen(false);\r\n          }\r\n        });\r\n    });\r\n\r\n    return () => {\r\n      PubSub.unsubscribe(menuSubId);\r\n      PubSub.unsubscribe(addSubId);\r\n      PubSub.unsubscribe(rewardSubId);\r\n      PubSub.unsubscribe(doRewardSubId);\r\n    }\r\n  }, [setReadThemePopup, setRewardPopupOpen]);\r\n\r\n  useEffect(() => {\r\n    if (shouldLoad && novelContent !== null) {\r\n      addToCache(\"lastRead\", {\r\n        id, sectionId\r\n      });\r\n      persistData();\r\n    }\r\n  }, [novelContent, containerRef]);\r\n\r\n\r\n  // 获取缓存并设置\r\n  useEffect(() => {\r\n    if (!shouldLoad && props.persistentData !== null) {\r\n      setLoadingState(LoadingState.LOADED);\r\n      const data = props.persistentData as IReadPersistentData;\r\n      setNovelContent(data.novelContent);\r\n      if (containerRef.current !== null) {\r\n        setTimeout(() => {\r\n          (containerRef.current as HTMLDivElement).scrollTop = data.scrollTop;\r\n        })\r\n      }\r\n    }\r\n\r\n    if (loadingState !== null || !shouldLoad) {\r\n      return;\r\n    }\r\n\r\n  }, [props, prev, shouldLoad, loadingState, containerRef]);\r\n\r\n  const handleLoadData = useCallback(() => {\r\n    const _queryParams: { [propName: string]: string } = JSON.parse(JSON.stringify(queryParams));\r\n    delete _queryParams[\"section\"];\r\n\r\n    Toast.loading(\"加载中\", 0);\r\n    // 加载小说数据\r\n    AutoMaticSetLoadingState(\r\n      Promise.all([\r\n        getNovelContentWithExtra(\r\n          parseInt(id),\r\n          parseInt(sectionId),\r\n          prev ? \"last\" : \"next\",\r\n          _queryParams === null ? undefined : _queryParams\r\n        )\r\n          .then((data) => {\r\n            Toast.hide();\r\n            if (data.err_code === ResponseCode.SUCCESS) {\r\n              if (!data.data.content) {\r\n                setReloadDisplay(true);\r\n                return;\r\n              }\r\n              setReloadDisplay(false);\r\n              window.document.title = `第${data.data.num}章 ` + data.data.title;\r\n              // 加载出错重新加载\r\n\r\n              if (data.data.content.indexOf(\"<div\") !== -1) {\r\n                data.data.content = data.data.content.replace(/<br>|<br\\/>/g, \"<br>&nbsp;&nbsp;\").replace(/<p/g, \"<p class='content-line'\");\r\n                setNovelContent(data.data);\r\n              } else {\r\n                const elem = document.createElement(\"div\");\r\n                elem.innerHTML = data.data.content;\r\n                data.data.content = [...elem.childNodes]\r\n                // 过滤br标签\r\n                  .filter((_node) => _node.nodeName !== \"BR\")\r\n                  // 映射出文字\r\n                  .map((_node) => _node.nodeName === \"#text\" ? _node.nodeValue : _node.textContent)\r\n                  // 去掉前面的空格\r\n                  .map((_val) => {\r\n                    return _val === null ? \"\" : _val.replace(/^\\s*/g, \"\").replace(/nbsp;/g, \"\");\r\n                  })\r\n                  // 生成p标签\r\n                  .map((_val) => `<p class=\"content-line\">${_val}</p>`)\r\n                  // 合并数组为字符串\r\n                  .reduce((prev, next) => prev + next, \"\");\r\n\r\n                setNovelContent(data.data);\r\n                elem.remove();\r\n              }\r\n\r\n              // 需要充值才能阅读\r\n            } else if (data.err_code === ResponseCode.TO_RECHARGE) {\r\n              // 如果该章节已经跳转支付则跳转小说详情以避免死循环 并标记为未跳转支付\r\n              if (payCallbackUrl !== null && payCallbackUrl.split(\"#\")[1].indexOf(`/read/${id}-${sectionId}.html`) !== -1) {\r\n                console.log(payCallbackUrl.split(\"#\")[1].split(\"/\")[2].split(\"-\")[0]);\r\n                const lastRead = getCache<ILastRead>(\"lastRead\");\r\n                if (\r\n                  lastRead !== null &&\r\n                  lastRead.id === payCallbackUrl.split(\"#\")[1].split(\"/\")[2].split(\"-\")[0]\r\n                ) {\r\n                  setPrev(true);\r\n                  goToReadPage(props.history, parseInt(lastRead.id), 0, false, true);\r\n                } else {\r\n                  Toast.fail(\"请先充值后再阅读\", 1.5, undefined, false);\r\n                  props.history.go(-1);\r\n                  // goToPageWithCustomerId(props.history, {pathname: `/detail/novel-${id}.html`, state: {reload: true}}, true);\r\n                }\r\n                setPayCallbackUrl(null);\r\n                return;\r\n              }\r\n\r\n              // 标记为已跳转支付 并跳转支付\r\n              setPayCallbackUrl(SERVER_ROOT + \"front/\" + window.location.hash);\r\n              goToPageWithCustomerId(props.history, {\r\n                pathname: `/need-pay-${id}-${sectionId}.html`,\r\n              }, false);\r\n            } else {\r\n              if (data.err_code === ResponseCode.NOW_FOLLOW) {\r\n                goToPageWithCustomerId(props.history, {\r\n                  pathname: `/follow-${id}-${sectionId}.html`,\r\n                  state: {reload: true}\r\n                }, true);\r\n              } else {\r\n                props.history.go(-1);\r\n                Toast.fail(data.err_msg, 1.5, undefined, false);\r\n              }\r\n            }\r\n          }),\r\n        getNovelRecommends(parseInt(id))\r\n          .then((data) => setNovelRecommends(data.data))\r\n      ]),\r\n      (state) => setLoadingState(state)\r\n    )\r\n  }, [props.location]);\r\n\r\n  useEffect(handleLoadData, [props.location]);\r\n\r\n  // 内容变动的时候持久化数据\r\n  useEffect(() => {\r\n    if (loadingState === LoadingState.LOADED) {\r\n      (containerRef.current as HTMLDivElement).scrollTop = 0;\r\n    }\r\n  }, [loadingState]);\r\n\r\n  const handleScroll = useCallback(() => {\r\n    if (containerRef.current !== null) {\r\n      const scrollTop = (containerRef.current as HTMLDivElement).scrollTop;\r\n      props.persistData({\r\n        novelContent, scrollTop\r\n      })\r\n    }\r\n  }, [containerRef, novelContent]);\r\n\r\n  useEffect(() => {\r\n    return () => {\r\n      setRewardPopupOpen(false);\r\n    }\r\n  }, []);\r\n\r\n  const handleAddToBookStore = useCallback(() => {\r\n    if (novelContent !== null) {\r\n      addNovelToBookShelf(novelContent.novel_id, novelContent.num)\r\n        .then((data) => {\r\n          if (data.err_code === ResponseCode.SUCCESS) {\r\n            Toast.success(data.err_msg, 1.5, undefined, false);\r\n            setNovelContent({\r\n              ...novelContent,\r\n              is_bookstore: true,\r\n            })\r\n          } else {\r\n            Toast.fail(data.err_msg, 1.5, undefined, false);\r\n          }\r\n        });\r\n    }\r\n  }, [novelContent]);\r\n\r\n  // TODO 客服按钮修改咱不提交\r\n  // const [_userInfo] = useUserInfo();\r\n  // const userInfo = _userInfo as IUserInfo;\r\n  // const date = Math.floor(new Date().getTime() / 1000);\r\n\r\n  return (\r\n    <Fragment>\r\n      <ReloadRead onClick={() => {\r\n        setReloadDisplay(false);\r\n        handleLoadData();\r\n      }} display={reloadDisplay} btnText={'重新加载'} title={'重新加载'} desc={'网络出现错误, 请点击按钮重新加载'}/>\r\n      {\r\n        novelContent === null\r\n          ? <div className={\"mask\"}/>\r\n          : <ReadNavbar {...props} onFeedback={() => {\r\n            goToPageWithCustomerId(props.history, {pathname: `/feedback-${novelContent.novel_id}.html`});\r\n          }} onComplain={() => window.location.href = '/tousu'}/>\r\n      }\r\n      <div onScroll={handleScroll} ref={containerRef} className=\"read-scroller\">\r\n        <div style={{background: bgColor(readState.theme)}} className=\"read-container\">\r\n          <Fragment>\r\n            {\r\n              novelContent === null\r\n                ? null\r\n                : (\r\n                  <Fragment>\r\n                    { /* TODO 客服按钮修改咱不提交 */}\r\n                    {/*{ userInfo !== null ? ((userInfo.balance > 500 || userInfo.vip_end_at > date) ? null : <ServiceButton />) : null }*/}\r\n                    <ServiceButton/>\r\n                    <ReadThemeNavbar/>\r\n                    <div style={\r\n                      {\r\n                        color: fontColor(readState.theme),\r\n                        fontSize: remSize(readState.fontSize),\r\n                        lineHeight: remSize(readState.lineHeight === undefined ? 30 : readState.lineHeight())\r\n                      }} className=\"content\" dangerouslySetInnerHTML={{__html: novelContent.content}}>\r\n                    </div>\r\n\r\n                    {\r\n                      novelContent.is_bookstore\r\n                        ? (\r\n                          <p className=\"book-shelf-options book-shelf-in\">\r\n                              <span style={{\r\n                                background: readState.theme === \"夜间模式\" ? \"rgb(40, 40, 40)\" : \"transparent\",\r\n                                color: readState.theme === \"夜间模式\" ? \"#999\" : fontColor(readState.theme)\r\n                              }}>本书已在书架</span>\r\n                          </p>\r\n                        )\r\n                        : (\r\n                          <p onClick={handleAddToBookStore} className={\"book-shelf-options book-shelf-not-in\"}>\r\n                              <span style={{\r\n                                color: fontColor(readState.theme)\r\n                              }}>点击加入书架</span>\r\n                          </p>\r\n                        )\r\n                    }\r\n\r\n                    <BottomActions\r\n                      onToDirectory={() => {\r\n                        goToPageWithCustomerId(props.history, {pathname: `/detail/chapter-${id}.html`})\r\n                      }}\r\n                      onClick={(prev) => {\r\n                        if (prev) {\r\n                          setPrev(true);\r\n                          setLoadingState(null);\r\n                          setShouldLoad(true);\r\n                          goToReadPage(props.history, parseInt(id), novelContent.num - 1, false, true)\r\n                        } else {\r\n                          setPrev(false);\r\n                          setLoadingState(null);\r\n                          setShouldLoad(true);\r\n                          goToReadPage(props.history, parseInt(id), novelContent.num + 1, false, true)\r\n                        }\r\n                      }}\r\n                      isFinished={novelContent.num >= novelContent.section_count}\r\n                      isFirst={novelContent.num as number === 1}\r\n                    />\r\n                    <Recommends {...props} items={novelRecommends}/>\r\n                    {/*<ReadSubscribe/>*/}\r\n                  </Fragment>\r\n                )\r\n            }\r\n          </Fragment>\r\n        </div>\r\n      </div>\r\n    </Fragment>\r\n  );\r\n};\r\n","import * as React from \"react\";\r\nimport {useEffect, useState} from \"react\";\r\nimport {PageWithNavBar} from \"../common/PageWithNavBar\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport {dateToStr, strToDate} from \"../../utils/Utils\";\r\nimport usedIcon from \"./icons/used.png\";\r\nimport outOfTimeIcon from \"./icons/out_of_time.png\"\r\nimport {useDocumentTitle, useTheme, useUserInValidCoupons, useUserValidCoupons} from \"../../hooks/Hooks\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport \"./index.less\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport {getCoupons} from \"../../api/UserApi\";\r\n\r\nexport interface ICouponItemProps {\r\n  active: boolean;\r\n  name: string;\r\n  desc: string;\r\n  balance: number;\r\n  startDate: number | string;\r\n  endDate: number | string;\r\n  status: 1 | 2 | 0;\r\n  id: number;\r\n}\r\n\r\nconst filterDate = (date: number | string) => {\r\n  return dateToStr(date, \"yyyy.MM.dd\");\r\n};\r\n\r\nconst judgeState = (props: ICouponItemProps): -1 | 0 | 1 => {\r\n  const now: number = new Date().getTime();\r\n  let endDate: null | number = null;\r\n  if (typeof props.endDate === \"string\") {\r\n    endDate = strToDate(props.endDate, \"yyyy.MM.dd\").getTime()\r\n  } else {\r\n    endDate = props.endDate;\r\n  }\r\n\r\n  (endDate as number) += 86400000 - 1000;\r\n  if (now > (endDate as number)) {\r\n    return -1;\r\n  }\r\n\r\n  if (props.active) {\r\n    return 1;\r\n  } else {\r\n    return -1;\r\n  }\r\n};\r\n\r\nconst buildCouponIcon = (props: ICouponItemProps) => {\r\n  const state = judgeState(props);\r\n  if (state === 1) {\r\n    return (\r\n      <div className=\"btn-go-use\">\r\n        <p>去使用</p>\r\n      </div>\r\n    )\r\n  } else if (state === 0) {\r\n    return (\r\n      <div className=\"coupon-icon-wrapper\">\r\n        <img src={usedIcon} alt=\"\"/>\r\n      </div>\r\n    )\r\n  } else {\r\n    return (\r\n      <div className=\"coupon-icon-wrapper\">\r\n        <img src={outOfTimeIcon} alt=\"\"/>\r\n      </div>\r\n    )\r\n  }\r\n\r\n};\r\n\r\nconst CouponItem: React.FC<ICouponItemProps> = (props) => {\r\n  return (\r\n    <div className={judgeState(props) === 1 ? \"coupon-item active\" : \"coupon-item\"}>\r\n      <div className=\"left\">\r\n        <p className=\"balance\">{props.balance}<span className=\"rmb\">元</span></p>\r\n      </div>\r\n      <div className=\"middle\">\r\n        <p className=\"name\">{props.name}</p>\r\n        <p className=\"desc\">{props.desc}</p>\r\n        <p className=\"date\">{filterDate(props.startDate)} - {filterDate(props.endDate)}</p>\r\n      </div>\r\n      <div className=\"right-wrapper\">\r\n        <div className=\"right\">\r\n          {buildCouponIcon(props)}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n\r\nexport const Coupon: React.FC<RouteChildrenProps> = (props) => {\r\n  useDocumentTitle(\"优惠券\");\r\n  const [validCoupons, setValidCoupons] = useUserValidCoupons();\r\n  const [inValidCoupons, setInValidCoupons] = useUserInValidCoupons();\r\n  const [currentTab, setCurrentTab] = useState(0);\r\n  const theme = useTheme()[0] as Theme;\r\n\r\n  useEffect(() => {\r\n    getCoupons(\"valid\")\r\n      .then((data) => {\r\n        setValidCoupons(data.data);\r\n      });\r\n    getCoupons(\"invalid\")\r\n      .then((data) => {\r\n        setInValidCoupons(data.data);\r\n      })\r\n  }, []);\r\n\r\n  return (\r\n    <PageWithNavBar\r\n      {...props}\r\n      title={\"优惠券\"}\r\n      rightContent={\r\n        <span\r\n          style={{color: theme.mainColor}} className={\"more-touch\"}\r\n          onClick={() => {\r\n            goToPageWithCustomerId(props.history, {pathname: \"/index.html\", state: {reload: true}})\r\n          }}\r\n        >\r\n          首页\r\n        </span>}>\r\n      <div className={\"coupon-container\"}>\r\n        <div className=\"coupon-tabs\">\r\n          <div\r\n            onClick={() => setCurrentTab(0)}\r\n            style={{color: currentTab === 0 ? theme.mainColor : \"#333\"}} className=\"coupon-tab-item\"\r\n          >可用优惠券\r\n          </div>\r\n          <div className=\"blank-space\" />\r\n          <div\r\n            onClick={() => setCurrentTab(1)}\r\n            style={{color: currentTab === 1 ? theme.mainColor : \"#333\"}} className=\"coupon-tab-item\"\r\n          >无效优惠券\r\n          </div>\r\n        </div>\r\n        <div className=\"coupon-list\">\r\n\r\n          {\r\n            currentTab === 0 && validCoupons !== null\r\n              ? validCoupons.map((coupon) => {\r\n                return (\r\n                  <CouponItem\r\n                    status={coupon.status as 0 | 1 | 2}\r\n                    id={coupon.coupon.id}\r\n                    active={true}\r\n                    desc={\"只能是《我爱一根柴》使用\"}\r\n                    balance={10} name={coupon.coupon.name}\r\n                    endDate={coupon.coupon.end_at * 1000}\r\n                    startDate={coupon.coupon.start_at * 1000}/>\r\n                )\r\n              })\r\n              : null\r\n          }\r\n\r\n          {\r\n            currentTab === 1 && inValidCoupons !== null\r\n              ? inValidCoupons.map((coupon) => {\r\n                return (\r\n                  <CouponItem\r\n                    status={coupon.status as 0 | 1 | 2}\r\n                    id={coupon.coupon.id}\r\n                    active={false}\r\n                    desc={\"只能是《我爱一根柴》使用\"}\r\n                    balance={10} name={coupon.coupon.name}\r\n                    endDate={coupon.coupon.end_at * 1000}\r\n                    startDate={coupon.coupon.start_at * 1000}/>\r\n                )\r\n              })\r\n              : null\r\n          }\r\n        </div>\r\n\r\n      </div>\r\n    </PageWithNavBar>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {Fragment, useCallback, useEffect, useState} from \"react\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport \"./index.less\";\r\nimport {Icon, Toast, WhiteSpace, WingBlank} from \"antd-mobile\";\r\nimport {SearchBar} from \"../common/SearchBar\";\r\nimport {useDefaultSearchArgument, useTheme} from \"../../hooks/Hooks\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport {getSearchResults, ISearchResult} from \"../../api/SearchApi\";\r\nimport {getCache, mixWithCache} from \"../../utils/LocalStorageUtil\";\r\nimport {BookItem, BookItemVertical} from \"../common/BookItem\";\r\nimport {LoadMoreList} from \"../common/LoadMoreList\";\r\nimport {LoadingState, LoadMoreBottomContent} from \"../common/LoadMoreBottomContent\";\r\nimport {HomeChunk} from \"../common/HomeChunk\";\r\nimport {Axis} from \"../../entities/Axis\";\r\nimport {NoContent} from \"../common/NoContent\";\r\nimport {AutoMaticSetLoadingState} from \"../../HOC/AutoMaticSetLoadingState\";\r\nimport {FutureElement} from \"../common/FutrueElement\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\n\r\nexport interface ISearchHeaderProps {\r\n  onSearch: (val: string) => void;\r\n  initialVal?: string;\r\n}\r\n\r\nconst SearchHeader: React.FC<ISearchHeaderProps & RouteChildrenProps> = (props) => {\r\n  const [searchWord, setSearchWord] = useState<string>(props.initialVal === undefined ? \"\" : props.initialVal);\r\n  const theme = useTheme()[0] as Theme;\r\n\r\n  useEffect(() => {\r\n    if (undefined !== props.initialVal) {\r\n      setSearchWord(props.initialVal);\r\n    }\r\n  }, [props.initialVal, setSearchWord]);\r\n\r\n  return (\r\n    <div className=\"search-header\">\r\n      <div onClick={() => props.history.go(-1)} className=\"icon-back more-touch\">\r\n        <Icon type={\"left\"}/>\r\n      </div>\r\n      <SearchBar onEnter={(val: string) => props.onSearch === undefined ? () => {\r\n      } : props.onSearch(val)} onChange={(val: string) => setSearchWord(val)} value={searchWord} enable={true}/>\r\n      <p onClick={() => props.onSearch(searchWord)} style={{color: theme.mainColor}} className=\"more-touch\">搜索</p>\r\n    </div>\r\n  )\r\n};\r\n\r\ninterface ISearchChunkProps {\r\n  searchKeys: Array<string>;\r\n  title: string;\r\n  onClick?: (val: string) => void;\r\n}\r\n\r\nconst SearchChunk: React.FC<ISearchChunkProps> = (props) => {\r\n  const theme = useTheme()[0] as Theme;\r\n\r\n  const handleClick = (key: string) => {\r\n    if (props.onClick !== undefined) {\r\n      props.onClick(key);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"search-history\">\r\n      <p style={{color: \"#999\"}} className=\"search-title\">{props.title}</p>\r\n      <div className=\"search-keys\">\r\n        {\r\n          props.searchKeys.map((key, index) => {\r\n            return (\r\n              <div onClick={() => handleClick(key)} key={index} className=\"search-key-item\">{key}</div>\r\n            )\r\n          })\r\n        }\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n\r\nexport const Search: React.FC<RouteChildrenProps> = (props) => {\r\n  const [histories] = useState<Array<string> | null>(getCache<Array<string>>(\"search_histories\"));\r\n  const theme = useTheme()[0] as Theme;\r\n  const [searchResult, setSearchResult] = useState<Array<ISearchResult> | null>(null);\r\n  const [currentPage, setCurrentPage] = useState<number>(1);\r\n  const [loadingState, setLoadingState] = useState<LoadingState | null>(null);\r\n  const [initData, setInitData] = useDefaultSearchArgument();\r\n  const [loadMoreState, setLoadMoreState] = useState(LoadingState.LOADING);\r\n\r\n  const [keyword, setKeyword] = useState<string>(\"\");\r\n\r\n  const addHistoryToCache = (keyword: string) => {\r\n    mixWithCache<Array<string>>(\"search_histories\", (item) => {\r\n      if (item === null) {\r\n        return [keyword]\r\n      } else {\r\n        if (item.length > 7) {\r\n          item.pop();\r\n        }\r\n        if (item.find((_item) => _item === keyword) === undefined) {\r\n          item.unshift(keyword);\r\n        } else {\r\n          const elem = item[item.findIndex((_item) => _item === keyword)];\r\n          delete item[item.findIndex((_item) => _item === keyword)];\r\n          item = [...item];\r\n          item = item.filter((_item) => _item !== undefined);\r\n          console.log(item);\r\n          item.unshift(elem);\r\n        }\r\n        return item;\r\n      }\r\n    });\r\n  };\r\n\r\n  const handleSearch = async (val: string, shouldAddToCache: boolean = true) => {\r\n    if (val === \"\") {\r\n      Toast.info(\"请输入搜索内容\", 1.5, undefined, false);\r\n      return;\r\n    }\r\n    setKeyword(val);\r\n\r\n    addHistoryToCache(val);\r\n\r\n    setLoadingState(LoadingState.LOADING);\r\n    AutoMaticSetLoadingState(\r\n      getSearchResults({title: val,}).then((data) => {\r\n        if (data.data.last_page === null) {\r\n          setLoadMoreState(LoadingState.FINISHED);\r\n          setLoadingState(LoadingState.FINISHED);\r\n        } else {\r\n          setLoadingState(LoadingState.LOADED);\r\n        }\r\n        setSearchResult(data.data.data.novel_list);\r\n      }),\r\n      (state) => {\r\n        if (state !== LoadingState.LOADED) {\r\n          setLoadingState(state);\r\n        }\r\n      }\r\n    );\r\n  };\r\n\r\n  useEffect(() => {\r\n    if (currentPage > 1) {\r\n      AutoMaticSetLoadingState(\r\n        getSearchResults({title: keyword, page: currentPage}).then((data) => {\r\n          if (data.data.last_page === null) {\r\n            setLoadMoreState(LoadingState.FINISHED);\r\n          } else {\r\n            setLoadMoreState(LoadingState.LOADED);\r\n          }\r\n\r\n          const result = searchResult === null ? [] : searchResult;\r\n          setSearchResult([...result, ...data.data.data.novel_list]);\r\n        }),\r\n        (state) => {\r\n          if (state !== LoadingState.LOADED) {\r\n            setLoadMoreState(state);\r\n          }\r\n        }\r\n      );\r\n    }\r\n  }, [currentPage]);\r\n\r\n  const handleLoadMore = () => {\r\n    setCurrentPage(currentPage + 1);\r\n  };\r\n\r\n  const buildBookName = (name: string) => {\r\n    const splits = name.split(\"\");\r\n    return (\r\n      <Fragment>\r\n        {\r\n          splits.map((word) => {\r\n            if (keyword.indexOf(word) !== -1) {\r\n              return <span key={word} style={{color: theme.mainColor}}>{word}</span>;\r\n            } else {\r\n              return word;\r\n            }\r\n          })\r\n        }\r\n      </Fragment>\r\n    );\r\n  };\r\n\r\n  const mapData2Title = useCallback(() => {\r\n    if (initData === null) {\r\n      return [];\r\n    }\r\n\r\n    return initData.data.title_list.map((item) => {\r\n      return item.title;\r\n    });\r\n  }, [initData]);\r\n\r\n  const buildDefaultList = useCallback(() => {\r\n    if (initData === null) {\r\n      return [];\r\n    }\r\n\r\n    const data = initData.data.novel_list;\r\n    return (\r\n      <HomeChunk\r\n        onClick={(index) => {\r\n          goToPageWithCustomerId(props.history, {pathname: `/detail/novel-${data[index].novel_id}.html`, state: {reload: true}})\r\n        }}\r\n        itemPerLine={3}\r\n        title={<span style={{fontWeight: \"bold\", fontSize: \".18rem\", color: theme.mainColor}}>畅销书单</span>}\r\n        itemsCount={data.length}\r\n        axis={Axis.horizontal}\r\n        btn={\"\"}\r\n        itemBuilder={(index: number) => {\r\n          return (\r\n            <BookItem\r\n              name={data[index].title}\r\n              img={data[index].img}\r\n            />\r\n          )\r\n        }}\r\n      />\r\n    )\r\n\r\n  }, [initData]);\r\n\r\n  return (\r\n    <div className=\"search-container\">\r\n      <SearchHeader initialVal={keyword} {...props} onSearch={handleSearch}/>\r\n      {\r\n        loadingState === null\r\n          ? <Fragment>\r\n            <SearchChunk onClick={(val) => handleSearch(val)} title={\"大家都在搜索\"} searchKeys={mapData2Title()}/>\r\n            <WhiteSpace style={{background: \"#f1f1f1\"}}/>\r\n            {\r\n              histories !== null && histories.length > 0\r\n              ? <SearchChunk onClick={(key: string) => handleSearch(key, false)} title={\"搜索历史\"}\r\n                             searchKeys={histories === null ? [] : histories}/>\r\n                             : null\r\n            }\r\n            <WingBlank>\r\n              <div className=\"border-bottom\"></div>\r\n            </WingBlank>\r\n\r\n            {buildDefaultList()}\r\n\r\n          </Fragment>\r\n          : <FutureElement\r\n            contentBuilder={() => {\r\n              return searchResult !== null\r\n                ? searchResult.length > 0\r\n                  ? <LoadMoreList\r\n                    itemCount={searchResult.length}\r\n                    onLoadMore={handleLoadMore}\r\n                    itemBuilder={(index) => {\r\n                      const buildTags = (tags: string | null) => {\r\n                        if (tags === null) {\r\n                          return [];\r\n                        } else {\r\n                          return tags.split(\"|\");\r\n                        }\r\n                      };\r\n\r\n                      return (\r\n                        <Fragment>\r\n                          <BookItemVertical\r\n                            onClick={(id) => goToPageWithCustomerId(props.history, {pathname: `/detail/novel-${id}.html`, state: {reload: true}})}\r\n                            id={searchResult[index].id}\r\n                            key={searchResult[index].id}\r\n                            tags={buildTags(searchResult[index].tags)}\r\n                            desc={searchResult[index].desc}\r\n                            name={buildBookName(searchResult[index].title)}\r\n                            img={searchResult[index].img}\r\n                          />\r\n                          {\r\n                            index === searchResult.length - 1\r\n                              ? <LoadMoreBottomContent status={loadMoreState}/>\r\n                              : null\r\n                          }\r\n                        </Fragment>\r\n                      )\r\n                    }}\r\n                  />\r\n                  : <NoContent/>\r\n                : null\r\n            }} loadingState={loadingState} showBarrier={true} showLoadingDelay={0}/>\r\n\r\n      }\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport \"./TransparencyNavBar.less\";\r\n\r\nexport interface ITransparencyNavBarProps {\r\n  height: number;\r\n  title: string | JSX.Element;\r\n  leftIcon: string | JSX.Element;\r\n  rightIcon: string | JSX.Element;\r\n}\r\n\r\nexport const TransparencyNavBar: React.FC<ITransparencyNavBarProps> = (props) => {\r\n  return (\r\n    <div style={{height: props.height / 100 + \"rem\"}} className={\"transparency-nav-bar\"}>\r\n      { props.leftIcon }\r\n      { props.title }\r\n      { props.rightIcon }\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {useCallback, useEffect} from \"react\";\r\nimport {TransparencyNavBar} from \"../common/TransparenctNavBar\";\r\nimport {Icon, Toast} from \"antd-mobile\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport didIcon from \"./images/fl/fl_finish_icon.png\";\r\nimport moneyIcon from \"./images/fl/fl_money_icon.png\";\r\nimport bgImg from \"./images/fl/fl_top_bac.png\";\r\nimport adImg from \"./images/ad.png\";\r\nimport \"./index.less\";\r\nimport {doUserSign, getUserSignedRecord} from \"../../api/UserApi\";\r\nimport {useDocumentTitle, useUserSignedRecord} from \"../../hooks/Hooks\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport {getCache} from \"../../utils/LocalStorageUtil\";\r\nimport {ResponseCode} from \"../../utils/ResponseCode\";\r\nimport qs from \"qs\";\r\n\r\ninterface ISignitemProps {\r\n  signed: boolean;\r\n  reward: number;\r\n  today: boolean;\r\n}\r\n\r\ninterface ILastRead {\r\n  id: string;\r\n  sectionId: string;\r\n}\r\n\r\ninterface IJumpToHomeParams {\r\n  jumpto?: string;\r\n}\r\n\r\nconst SignItem: React.FC<ISignitemProps> = (props) => {\r\n  return (\r\n    <div className={props.signed ? \"sign-item signed\" : \"sign-item\"}>\r\n      <div className=\"icon-wrapper\">\r\n        {\r\n          props.signed\r\n            ? <img src={didIcon} alt=\"\"/>\r\n            : <img src={moneyIcon} alt=\"\"/>\r\n        }\r\n      </div>\r\n      <p className=\"balance\">{props.reward}书币</p>\r\n    </div>\r\n  )\r\n};\r\n\r\nexport const SignIn: React.FC<RouteChildrenProps> = (props) => {\r\n  useDocumentTitle(\"福利中心\");\r\n  const [signedData, setSignedData] = useUserSignedRecord();\r\n  const params = qs.parse(props.location.search.split(\"?\")[1]) as IJumpToHomeParams;\r\n\r\n  const doSign = useCallback(() => {\r\n    if (signedData !== null && signedData.signed) {\r\n      const lastRead = getCache<ILastRead>(\"lastRead\");\r\n      console.log(\"最后签到数据\", lastRead);\r\n      if (lastRead !== null) {\r\n        goToPageWithCustomerId(props.history, {pathname: `/read/${lastRead.id}-${lastRead.sectionId}.html`, state: {reload: true}});\r\n      } else {\r\n        goToPageWithCustomerId(props.history, {pathname: \"/index.html\", state: {reload: true}});\r\n      }\r\n\r\n      return;\r\n    }\r\n\r\n    doUserSign()\r\n      .then((data) => {\r\n        if (data.err_code === ResponseCode.SUCCESS) {\r\n          Toast.info(\"签到成功\", 1.5, undefined, false);\r\n          getUserSignedRecord()\r\n            .then((data) => {\r\n              setSignedData(data.data);\r\n            })\r\n        }\r\n      });\r\n  }, [signedData, setSignedData]);\r\n\r\n  useEffect(() => {\r\n    getUserSignedRecord()\r\n      .then((data) => {\r\n        setSignedData(data.data);\r\n      })\r\n  }, []);\r\n\r\n  const buildSignedItem = useCallback(() => {\r\n    if (signedData === null) {\r\n      return null;\r\n    }\r\n\r\n    const signedElements: Array<JSX.Element> = [];\r\n\r\n    // 今天时间\r\n    const now = new Date();\r\n    const continueDay = signedData.continue_day > 6 ? 6 : signedData.continue_day;\r\n\r\n    // 循环添加已签到的时候\r\n    for (const day in signedData.day_coins) {\r\n      if (signedData.day_coins.hasOwnProperty(day)) {\r\n        // if (parseInt(day) === continueDay) {\r\n        //   // const today = new Date();\r\n        //   // const signedDate = new Date(signedData.created_at * 1000);\r\n        //   // const signed = signedData.signed && (\r\n        //   //   today.getFullYear() === signedDate.getFullYear() &&\r\n        //   //   today.getMonth() === signedDate.getMonth() &&\r\n        //   //   today.getDate() === signedDate.getDate()\r\n        //   // );\r\n        //\r\n        //   signedElements.push((\r\n        //     <SignItem\r\n        //       key={day}\r\n        //       today={true}\r\n        //       reward={signedData.day_coins[day]}\r\n        //       signed={signedData.signed}/>\r\n        //   ))\r\n        // } else {\r\n        //   signedElements.push((\r\n        //     <SignItem\r\n        //       key={day}\r\n        //       today={false}\r\n        //       reward={signedData.day_coins[day]}\r\n        //       signed={parseInt(day) <= signedData.continue_day}/>\r\n        //   ))\r\n        // }\r\n        signedElements.push((\r\n          <SignItem\r\n            key={day}\r\n            today={false}\r\n            reward={signedData.day_coins[day]}\r\n            signed={parseInt(day) <= signedData.continue_day}/>\r\n        ))\r\n      }\r\n    }\r\n\r\n    return signedElements;\r\n  }, [signedData]);\r\n\r\n  return (\r\n    <div style={{color: \"#ffffff\"}} className=\"sign-in-container\">\r\n      <TransparencyNavBar height={45} leftIcon={(\r\n        <Icon className={\"more-touch\"} onClick={() => {\r\n          if (params !== null && params !== undefined && params.jumpto !== undefined) {\r\n            goToPageWithCustomerId(props.history, {pathname: \"/index.html\", state: {reload: true}}, true);\r\n            return;\r\n          }\r\n          props.history.go(-1)\r\n        }} type={\"left\"}/>\r\n      )} title={<span className={\"title\"}>福利中心</span>} rightIcon={(\r\n        <div style={{color: \"#3688ff\"}} className=\"more-touch\"></div>\r\n      )}/>\r\n      <div className=\"bg-wrapper\">\r\n        <img src={bgImg} alt=\"\"/>\r\n      </div>\r\n\r\n      <div className=\"content\">\r\n        <div className=\"info\">\r\n          <p className=\"score\">+{signedData === null ? 0 : signedData.coin}</p>\r\n          <p className=\"tip\">今日签到奖励</p>\r\n        </div>\r\n\r\n        <div className=\"panel\">\r\n          <div className=\"header\">\r\n            <p className=\"tip\">已连续签到 <span className=\"day\">{signedData === null ? 0 : signedData.continue_day}天</span>\r\n            </p>\r\n            <div onClick={doSign}\r\n                 className={\r\n                   signedData === null\r\n                     ? \"do-sign\"\r\n                     : signedData.signed\r\n                     ? \"do-sign disabled\"\r\n                     : \"do-sign\"}\r\n            >\r\n\r\n              {signedData !== null && signedData.signed ? \"继续阅读\" : \"点击签到\"}\r\n            </div>\r\n          </div>\r\n          <div className=\"sign-in-items\">\r\n            {\r\n              buildSignedItem()\r\n            }\r\n          </div>\r\n        </div>\r\n\r\n        <div className=\"tip-list\">\r\n          <p>1.每天登录签到领取书币，连续签到能够得到更多书币；</p>\r\n          <p>2.本签到为累计签到，用户若中途中断签到，签到记录将自动重新开始；</p>\r\n          <p>3.签到周期为七天。连续签到7天得350个书币；</p>\r\n          <p>4.本APP对本次签到活动拥有最终解释权</p>\r\n        </div>\r\n\r\n        <div className=\"ad-list\">\r\n          <p className=\"add-content\">更多赚取书币方法</p>\r\n\r\n          <div className=\"add-wrapper\">\r\n            <img onClick={() => {\r\n              goToPageWithCustomerId(props.history, {pathname: \"/activity-sweepstakes.html\"})\r\n            }} src={adImg} alt=\"\"/>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {Fragment, useCallback, useEffect, useState} from \"react\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport {Icon, Tabs} from \"antd-mobile\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport {useDocumentTitle, useTheme, useUserCostLogs, useUserRechargeLogs} from \"../../hooks/Hooks\";\r\nimport \"./index.less\";\r\nimport {dateToStr} from \"../../utils/Utils\";\r\n\r\nimport bookIcon from \"./icon/book_icon.png\";\r\nimport dashingIcon from \"./icon/dashang_icon.png\";\r\nimport otherIcon from \"./icon/other_icon.png\";\r\nimport rewardIcon from \"./icon/reward_icon.png\";\r\nimport {CostType, getCostRecors, getRechargeRecors} from \"../../api/UserApi\";\r\nimport {LoadingState, LoadMoreBottomContent} from \"../common/LoadMoreBottomContent\";\r\nimport {LoadMoreList} from \"../common/LoadMoreList\";\r\nimport {AutoMaticSetLoadingState} from \"../../HOC/AutoMaticSetLoadingState\";\r\n\r\nconst switchIcon = (type: CostType) => {\r\n  switch (type) {\r\n    case 1:\r\n      return dashingIcon;\r\n    case 3:\r\n      return bookIcon;\r\n    case 5:\r\n      return rewardIcon;\r\n    case 6:\r\n      return otherIcon;\r\n  }\r\n};\r\n\r\n\r\nexport const PayHistory: React.FC<RouteChildrenProps> = (props) => {\r\n  useDocumentTitle(\"消费记录\");\r\n  const [currentChose, setCurrentChose] = useState<number>(0);\r\n  const theme = useTheme()[0] as Theme;\r\n\r\n  const [userRechargeLogs, setUserRechargeLogs] = useUserRechargeLogs();\r\n  const [userCostLogs, setUserCostLogs] = useUserCostLogs();\r\n\r\n  const [costLoadMoreState, setCostLoadMoreState] = useState<LoadingState | null>(null);\r\n  const [payLoadMoreState, setPayLoadMoreState] = useState<LoadingState | null>(null);\r\n\r\n  const [costPage, setCostPage] = useState(1);\r\n  const [rechargePage, setRechargePage] = useState(1);\r\n\r\n  const tabs = [\r\n    {title: <div className={currentChose === 0 ? \"tab-item active\" : \"tab-item\"}>消费记录</div>},\r\n    {title: <div className={currentChose === 1 ? \"tab-item active\" : \"tab-item\"}>充值记录</div>},\r\n  ];\r\n\r\n  useEffect(() => {\r\n    getCostRecors()\r\n      .then((data) => {\r\n        if (data.data.last_page === null) {\r\n          setCostLoadMoreState(LoadingState.FINISHED);\r\n        }\r\n\r\n        setUserCostLogs(data.data.data)\r\n      });\r\n\r\n    getRechargeRecors()\r\n      .then((data) => {\r\n        if (data.data.last_page === null) {\r\n          setPayLoadMoreState(LoadingState.FINISHED)\r\n        }\r\n\r\n        setUserRechargeLogs(data.data.data)\r\n      });\r\n\r\n  }, []);\r\n\r\n  useEffect(() => {\r\n    console.log(payLoadMoreState);\r\n  }, [payLoadMoreState]);\r\n\r\n  const handleCostLoadMore = useCallback(() => {\r\n    if (costPage + 1 === 1 || costLoadMoreState === LoadingState.FINISHED) {\r\n      return;\r\n    }\r\n\r\n    AutoMaticSetLoadingState(\r\n      getCostRecors(costPage + 1)\r\n        .then((data) => {\r\n          setCostPage(costPage + 1);\r\n          if (data.data.last_page === null) {\r\n            setCostLoadMoreState(LoadingState.FINISHED);\r\n          } else {\r\n            setCostLoadMoreState(LoadingState.LOADING);\r\n          }\r\n\r\n          setUserCostLogs([...userCostLogs === null ? [] : userCostLogs, ...data.data.data]);\r\n        }),\r\n      (state) => {\r\n        if (state === LoadingState.LOADING) {\r\n          setCostLoadMoreState(state);\r\n        }\r\n      }\r\n    )\r\n  }, [costPage, setCostPage, setCostLoadMoreState, setUserCostLogs, userCostLogs, costLoadMoreState]);\r\n\r\n\r\n  const handleRechargeLoadMore = useCallback(() => {\r\n    if (rechargePage + 1 === 1 || payLoadMoreState === LoadingState.FINISHED) {\r\n      return;\r\n    }\r\n\r\n    AutoMaticSetLoadingState(\r\n      getRechargeRecors(rechargePage + 1)\r\n        .then((data) => {\r\n          setRechargePage(rechargePage + 1);\r\n          if (data.data.last_page === null) {\r\n            setPayLoadMoreState(LoadingState.FINISHED);\r\n          } else {\r\n            setPayLoadMoreState(LoadingState.LOADED);\r\n          }\r\n\r\n          setUserRechargeLogs([...userRechargeLogs === null ? [] : userRechargeLogs, ...data.data.data]);\r\n        }),\r\n      (state) => {\r\n        if (state === LoadingState.LOADING) {\r\n          setPayLoadMoreState(state);\r\n        }\r\n      }\r\n    )\r\n  }, [rechargePage, setRechargePage, setUserRechargeLogs, setPayLoadMoreState, payLoadMoreState, userRechargeLogs]);\r\n\r\n  return (\r\n    <div className=\"pay-history-container\">\r\n      <div className={\"bg\"}></div>\r\n      <div className=\"inner-bg\"/>\r\n      <div onClick={() => props.history.go(-1)} className=\"back more-touch\"><Icon type={\"left\"}/></div>\r\n      <Tabs\r\n        initialPage={currentChose}\r\n        onChange={(current, index) => setCurrentChose(index)}\r\n        tabs={tabs}\r\n        swipeable={false}\r\n        tabBarTextStyle={{color: \"rgba(255, 255, 255, .5)\"}}\r\n        tabBarUnderlineStyle={{transform: \"scaleX(.25) scaleY(1.2)\", borderColor: \"#fff\"}}\r\n        tabBarActiveTextColor={\"#fff\"}\r\n      >\r\n        {\r\n          userCostLogs === null\r\n            ? <div className={\"records\"}/>\r\n            : <LoadMoreList\r\n              onLoadMore={handleCostLoadMore}\r\n              className={\"records\"}\r\n              itemCount={userCostLogs === null ? 0 : userCostLogs.length}\r\n              itemBuilder={(index) => (\r\n                <div className=\"cost-log-item\">\r\n                  <img className={\"icon\"} src={switchIcon(userCostLogs[index].type)} alt=\"\"/>\r\n                  <div className=\"middle coin-middle\">\r\n                    <p className=\"title\">{userCostLogs[index].title}</p>\r\n                    <p className={\"chapter\"}>{userCostLogs[index].desc}</p>\r\n                    <p className=\"desc\">{dateToStr(userCostLogs[index].updated_at * 1000, \"yyyy-MM-dd hh:mm:ss\")}</p>\r\n                  </div>\r\n                  <div className=\"right\">\r\n                    <div className=\"info coin-info\">\r\n                      <p\r\n                        className=\"coin success\">{userCostLogs[index].coin > 0 ? \"+\" : \"-\"}{Math.abs(userCostLogs[index].coin)}</p>\r\n                      <div className=\"type\">{\"书币\"}</div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              )}>\r\n              <LoadMoreBottomContent status={costLoadMoreState}/>\r\n            </LoadMoreList>\r\n        }\r\n        {\r\n          userRechargeLogs === null\r\n            ? <div className={\"records\"}/>\r\n            : <LoadMoreList\r\n              onLoadMore={handleRechargeLoadMore}\r\n              className={\"records\"}\r\n              itemCount={userRechargeLogs === null ? 0 : userRechargeLogs.length}\r\n              itemBuilder={(index) => (\r\n                <div className=\"recharge-log-item\">\r\n                  <div className=\"middle\">\r\n                    <p className=\"title\">{\r\n                      userRechargeLogs[index].desc.split(\"|\")[0] + \"-\" + (\r\n                        userRechargeLogs[index].status === 0\r\n                          ? \"未支付\"\r\n                          : userRechargeLogs[index].status === 1\r\n                          ? \"充值成功\"\r\n                          : \"充值失败\"\r\n                      )}</p>\r\n                    <p\r\n                      className=\"desc\">{dateToStr(userRechargeLogs[index].updated_at * 1000, \"yyyy-MM-dd hh:mm:ss\")}</p>\r\n                  </div>\r\n                  <div className=\"right\">\r\n                    <div className=\"info\">\r\n                      {\r\n                        userRechargeLogs[index].type === \"coin\"\r\n                          ? <Fragment>\r\n                            {\r\n                              userRechargeLogs[index].coin === null\r\n                                ? <p\r\n                                  className={userRechargeLogs[index].status === 1 ? \"coin success\" : \"coin\"}>\r\n                                  0\r\n                                </p>\r\n                                : <p\r\n                                  className={userRechargeLogs[index].status === 1 ? \"coin success\" : \"coin\"}>\r\n                                  +{Math.abs(userRechargeLogs[index].coin as number)}\r\n                                </p>\r\n                            }\r\n                            <div className=\"type\">{userRechargeLogs[index].desc.split(\"|\")[0]}</div>\r\n                          </Fragment>\r\n                          : <Fragment>\r\n                            <p\r\n                              className={userRechargeLogs[index].status === 1 ? \"vip-info success\" : \"vip-info\"}>{userRechargeLogs[index].desc.split(\"|\")[0]}</p>\r\n                            <p className={\"vip-coin\"}>({userRechargeLogs[index].money / 100}天)</p>\r\n                          </Fragment>\r\n                      }\r\n\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              )}>\r\n              <LoadMoreBottomContent status={payLoadMoreState}/>\r\n            </LoadMoreList>\r\n        }\r\n      </Tabs>\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {useEffect, useState} from \"react\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport {TransparencyNavBar} from \"../common/TransparenctNavBar\";\r\nimport {Icon} from \"antd-mobile\";\r\nimport {LoadingState, LoadMoreBottomContent} from \"../common/LoadMoreBottomContent\";\r\nimport {useUserPrized} from \"../../hooks/Hooks\";\r\nimport couponIcon from \"./icons/myReward_coupon_icon.png\";\r\nimport moneyIcon from \"./icons/myReward_money_icon.png\";\r\nimport vipIcon from \"./icons/myReward_vip_icon.png\";\r\nimport {dateToStr} from \"../../utils/Utils\";\r\nimport {LoadMoreList} from \"../common/LoadMoreList\";\r\nimport {getMyPrizes} from \"../../api/UserApi\";\r\nimport \"./MyPrizes.less\";\r\n\r\nconst getIcon = (type: string) => {\r\n  switch (type) {\r\n    case \"coupon\":\r\n      return couponIcon;\r\n    case \"coin\":\r\n      return moneyIcon;\r\n    default:\r\n      return vipIcon;\r\n  }\r\n};\r\n\r\nexport const MyPrizes: React.FC<RouteChildrenProps> = (props) => {\r\n  const [prizes, setPrizes] = useUserPrized();\r\n  const [page, setPage] = useState(1);\r\n  const [loadingState, setLoadingState] = useState(LoadingState.LOADED);\r\n\r\n  useEffect(() => {\r\n    getMyPrizes(page)\r\n      .then((data) => {\r\n        if (data.data.last_page === null) {\r\n          setLoadingState(LoadingState.FINISHED);\r\n        } else {\r\n          setLoadingState(LoadingState.LOADED);\r\n        }\r\n        setPrizes(data.data);\r\n      })\r\n  }, []);\r\n\r\n  useEffect(() => {\r\n    setLoadingState(LoadingState.LOADING);\r\n    if (page === 1) {\r\n      return;\r\n    }\r\n    getMyPrizes(page)\r\n      .then((data) => {\r\n        if (data.data.last_page === null) {\r\n          setLoadingState(LoadingState.FINISHED);\r\n        } else {\r\n          setLoadingState(LoadingState.LOADED);\r\n        }\r\n\r\n        if (prizes === null) {\r\n          setPrizes(data.data);\r\n        } else {\r\n          setPrizes({\r\n            last_page: data.data.last_page,\r\n            data: [...prizes.data, ...data.data.data]\r\n          })\r\n        }\r\n      });\r\n  }, [page]);\r\n\r\n  return (\r\n    <div className=\"my-prized-container\">\r\n      <TransparencyNavBar\r\n        rightIcon={<div></div>}\r\n        title={<p style={{color: \"#333\"}}\r\n                  className={\"title\"}>我的奖品</p>}\r\n        height={44}\r\n        leftIcon={\r\n          <div onClick={() => props.history.go(-1)}\r\n               className={\"more-touch\"}>\r\n            <Icon style={{color: \"#333\"}}\r\n                  type={\"left\"}/>\r\n          </div>}/>\r\n\r\n      {\r\n        prizes === null\r\n          ? <div className={\"content\"}/>\r\n          : <LoadMoreList\r\n            onLoadMore={() => {\r\n              if (loadingState === LoadingState.FINISHED) {\r\n                return;\r\n              }\r\n\r\n              setPage(page + 1);\r\n            }}\r\n            className={\"content\"}\r\n            itemCount={prizes.data.length}\r\n            itemBuilder={(index) => (\r\n              <div className=\"prize-item\">\r\n                <div className=\"middle\">\r\n                  <p className=\"title\">{prizes.data[index].prize_name}</p>\r\n                  <p className=\"desc\">{dateToStr(prizes.data[index].updated_at * 1000, \"yyyy-MM-dd hh:mm:ss\")}</p>\r\n                </div>\r\n                <div className=\"right\">\r\n                  <div className=\"info\">\r\n                    <img className={\"icon\"} src={getIcon(prizes.data[index].type)} alt=\"\"/>\r\n                    <p className=\"coin\">+{prizes.data[index].num}</p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            )}>\r\n            <LoadMoreBottomContent status={loadingState}/>\r\n          </LoadMoreList>\r\n      }\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {Fragment, useCallback, useState} from \"react\";\r\nimport \"./index.less\";\r\nimport {Button, Icon, InputItem, List, Toast} from \"antd-mobile\";\r\nimport RadioItem from \"antd-mobile/lib/radio/RadioItem\";\r\nimport {TransparencyNavBar} from \"../common/TransparenctNavBar\";\r\nimport {doFeedBack} from \"../../api/UserApi\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport {ResponseCode} from \"../../utils/ResponseCode\";\r\nimport {useTheme} from \"../../hooks/Hooks\";\r\nimport {Theme} from \"../../entities/Theme\";\r\n\r\nconst ListItem = List.Item;\r\n\r\ninterface IFeedBackItem {\r\n  key: number;\r\n  label: string;\r\n  children?: IFeedBackItem[];\r\n}\r\n\r\nconst data: Array<IFeedBackItem> = [\r\n  {key: 1, label: \"内容更新太慢\"},\r\n  {key: 2, label: \"标题或封面图片低俗\"},\r\n  {key: 3, label: \"无法继续阅读\"},\r\n  {key: 4, label: \"功能无效\"},\r\n  {key: 5, label: \"页面排版紊乱\"},\r\n  {key: 6, label: \"页面加载慢\"},\r\n  {key: 7, label: \"广告骚扰\"},\r\n  {key: 8, label: \"其他\"},\r\n];\r\n\r\nconst complainData: Array<IFeedBackItem> = [\r\n  {\r\n    key: 1, label: '提交给微信团队审核', children: [\r\n      {key: 2, label: '网页包含欺诈信息（如：假红包）'},\r\n      {key: 2, label: '网页包含色情信息'},\r\n      {key: 2, label: '网页包含暴力恐怖信息'},\r\n      {key: 2, label: '网页包含政治敏感信息'},\r\n      {key: 2, label: '网页正在收集个人隐私信息（如：钓鱼链接）'},\r\n      {key: 2, label: '网页包含诱导分享/关注性质的内容'},\r\n      {key: 2, label: '网页可能包含谣言信息'},\r\n      {key: 2, label: '网页包含赌博信息'},\r\n    ]\r\n  }\r\n];\r\n\r\nexport const FeedBack: React.FC<RouteChildrenProps> = (props) => {\r\n  const [current, setCurrent] = useState(0);\r\n  const [currentInput, setCurrentInput] = useState<string>(\"\");\r\n  const [phoneNumber, setPhoneNumber] = useState<string>(\"\");\r\n  const theme = useTheme()[0] as Theme;\r\n\r\n  const handleConfirm = useCallback(() => {\r\n    if (current === 0) {\r\n      Toast.fail(\"请选择反馈原因\", 1.5, undefined, false);\r\n      return\r\n    }\r\n\r\n    if (currentInput.trim().length === 0) {\r\n      Toast.fail(\"请输入反馈内容\", 1.5, undefined, false);\r\n      return;\r\n    }\r\n\r\n    if (!/^1[3456789]\\d{9}$/.test(phoneNumber)) {\r\n      if (phoneNumber.length > 0) {\r\n        Toast.fail(\"手机号码格式不匹配\", 1.5, undefined, false);\r\n        return;\r\n      }\r\n    }\r\n\r\n    doFeedBack((data.find((_item) => _item.key === current) as IFeedBackItem).label, currentInput, phoneNumber.length > 0 ? phoneNumber : undefined)\r\n      .then((data) => {\r\n        if (data.err_code === ResponseCode.SUCCESS) {\r\n          Toast.success(\"感谢您的反馈\", 1.5, undefined, false);\r\n          props.history.go(-1);\r\n        } else {\r\n          Toast.fail(data.err_msg, 1.5, undefined, false);\r\n        }\r\n      });\r\n  }, [current, currentInput, phoneNumber]);\r\n\r\n  return (\r\n    <div className={\"feedback-container\"}>\r\n      <TransparencyNavBar\r\n        height={44}\r\n        rightIcon={<Fragment></Fragment>}\r\n        title={\"\"}\r\n        leftIcon={<Icon onClick={() => props.history.go(-1)} className={\"more-touch\"} type={\"left\"}/>}\r\n      />\r\n      <List renderHeader={() => {\r\n        return <p>请选择反馈原因</p>\r\n      }}>\r\n        {\r\n          data.map((reason) => {\r\n            return (\r\n              <RadioItem\r\n                onChange={() => setCurrent(reason.key)}\r\n                key={reason.key}\r\n                checked={current === reason.key}\r\n              >\r\n                {reason.label}\r\n              </RadioItem>\r\n            )\r\n          })\r\n        }\r\n      </List>\r\n\r\n      {\r\n        props.location.pathname.indexOf('feedback') === -1\r\n          ? null\r\n          : <List renderHeader={() => {\r\n            return <p>反馈原因描述</p>\r\n          }}>\r\n            <InputItem\r\n              onBlur={() => {\r\n                setTimeout(() => {\r\n                  window.scrollTo(0, 0);\r\n                });\r\n              }}\r\n              value={currentInput}\r\n              extra={`${currentInput.length} / 100`}\r\n              placeholder={\"请输入反馈内容\"}\r\n              onChange={(str) => {\r\n                if (str.length > 100) {\r\n                  Toast.fail(\"内容超过字数限制\", 1.5, undefined, false)\r\n                  return;\r\n                }\r\n\r\n                setCurrentInput(str);\r\n              }}\r\n            />\r\n          </List>\r\n      }\r\n\r\n      {\r\n        props.location.pathname.indexOf('feedback') === -1\r\n          ? null\r\n          : <List renderHeader={() => (\r\n            <p>请留下您的联系方式，方便我们核实情况</p>\r\n          )}>\r\n            <InputItem\r\n              onBlur={() => {\r\n                setTimeout(() => {\r\n                  window.scrollTo(0, 0);\r\n                });\r\n              }}\r\n              type={\"number\"}\r\n              value={phoneNumber}\r\n              placeholder={\"请输入您的手机号(选填)\"}\r\n              onChange={(str) => {\r\n                setPhoneNumber(str);\r\n              }}\r\n            />\r\n          </List>\r\n      }\r\n\r\n      <Button style={{background: theme.mainColor}} onClick={handleConfirm} type={\"primary\"}>确定提交</Button>\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\nimport {RouteChildrenProps} from \"react-router\";\nimport {useDocumentTitle, useUserInfo, useWechatInfo} from \"../../hooks/Hooks\";\nimport {Button, Toast} from \"antd-mobile\";\nimport {IUserInfo} from \"../../api/UserApi\";\nimport CopyToClipBoard from \"react-copy-to-clipboard\";\nimport \"./index.less\";\nimport {useEffect} from \"react\";\nimport {History, LocationDescriptorObject} from \"history\";\nimport {searchParamsProvider} from \"../../hooks/Providers\";\nimport qs from \"qs\";\n\nexport const Contact: React.FC<RouteChildrenProps> = (props) => {\n  useDocumentTitle(\"联系客服\");\n  const [wechatInfo,] = useWechatInfo();\n  const [_userInfo, setUserInfo] = useUserInfo();\n  const userInfo = _userInfo as IUserInfo;\n\n\n  const goToPageWithId = (\n    history: History<any>,\n    args: LocationDescriptorObject<any>\n  ) => {\n      if (args.search === undefined) {\n        args.search = \"\";\n      }\n      let _query = qs.parse(args.search.split(\"?\")[1]) as { [propName: string]: any };\n      if ( _query !== null && _query[\"jumpto\"] === \"index\") {\n        console.log(_query)\n        console.log(\"11111\");\n        history.push('/index')\n        console.log(_query)\n      } else {\n        console.log(\"2222\")\n        history.go(-1);\n      }\n  };\n\n\n  useEffect(() => {\n    console.log(wechatInfo);\n  }, [wechatInfo]);\n\n  return (\n    <div className={\"contact-container\"}>\n      <div className=\"btn-back\" onClick={() => {\n        goToPageWithId(props.history, {search: props.history.location.search})\n      }}/>\n      <img className={\"bg\"} src=\"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/contact_bac.png\" alt=\"\"/>\n      <img className={\"qrcode\"} src={wechatInfo === null ? \"\" : wechatInfo.customer_service} alt=\"\"/>\n      <div className=\"content\">\n        <p className={\"tip\"}>咨询时请提供ID: <span className={\"blue\"}>{userInfo === null ? \"\" : userInfo.id}</span></p>\n\n        <CopyToClipBoard onCopy={() => {\n          Toast.success(\"ID已复制到您的剪切板\", 1.5, undefined, false);\n        }} text={\n          userInfo ? userInfo.id + \"\" : \"\"\n        }>\n          <Button type={\"primary\"}>点此复制ID</Button>\n        </CopyToClipBoard>\n      </div>\n    </div>\n  )\n};\n","import * as React from \"react\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport {Icon} from \"antd-mobile\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport \"./index.less\";\r\n\r\nexport const VipAd: React.FC<RouteChildrenProps> = (props) => {\r\n  return (\r\n    <div className={\"vip-ad-container\"}>\r\n      <img className={\"bg\"} src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/vipBac.png\" alt=\"\"/>\r\n      <div onClick={() => {\r\n        props.history.go(-1);\r\n      }} className=\"back\">\r\n        <Icon type={\"left\"} />\r\n      </div>\r\n      <div className=\"content\">\r\n        <img className={\"content-img\"} src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/vipContent.png\" alt=\"\"/>\r\n        <div onClick={() => {\r\n          goToPageWithCustomerId(props.history, {pathname: \"/pay.html\", state: {vip: true}});\r\n        }} className=\"btn\" />\r\n      </div>\r\n\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\nimport {getPayResult} from \"../../api/UserApi\";\r\nimport qs from \"qs\";\r\nimport {wxPay} from \"../../utils/WxUtil\";\r\nimport {Toast} from \"antd-mobile\";\r\nimport {useUserInfo} from \"../../hooks/Hooks\";\r\nimport {useEffect} from \"react\";\r\nimport {urlEscapeChar} from \"../../utils/UrlUtil\";\r\nimport \"./index.less\";\r\n\r\nexport interface ICbParams {\r\n  cb: string;\r\n}\r\n\r\nexport interface IOutTradeNoParams {\r\n  out_trade_no: string;\r\n}\r\n\r\nexport interface IPayUrlParams {\r\n  backhost: string;\r\n  backurl: string;\r\n  user_id: number;\r\n  cid: number;\r\n  order: string;\r\n  money_btn_id: number;\r\n  out_trade_num: string;\r\n}\r\n\r\nexport const WxPay: React.FC<RouteChildrenProps> = (props) => {\r\n  const [, setUserInfo] = useUserInfo();\r\n  const search = decodeURIComponent(props.location.search.split(\"?\")[1] as string);\r\n  const outTradeNo = (qs.parse(search) as IOutTradeNoParams).out_trade_no;\r\n  let data: IPayUrlParams | null = null;\r\n\r\n  if (props.location.search !== undefined) {\r\n    const query = decodeURIComponent(props.location.search);\r\n    data = qs.parse(query.split(\"?\")[1]) as IPayUrlParams;\r\n  }\r\n\r\n  useEffect(() => {\r\n    if (data === null || data === undefined) {\r\n      return;\r\n    }\r\n\r\n    // alert('最终支付回调' + JSON.stringify(data));\r\n\r\n    wxPay(JSON.parse(data.order))\r\n      .then((res) => {\r\n        getPayResult(outTradeNo);\r\n        if (res.err_msg === \"get_brand_wcpay_request:ok\") {\r\n          Toast.success(\"充值成功\", 1.5, undefined, false);\r\n          if (data !== null) {\r\n            const cbUrl = urlEscapeChar(data.backhost + data.backurl, true);\r\n            window.location.replace(cbUrl);\r\n            return;\r\n          }\r\n        }\r\n\r\n        props.history.go(-1);\r\n      });\r\n  }, []);\r\n  return (\r\n    <div className=\"wx-pay-container\">\r\n      <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/pay_now.png\" alt=\"\"/>\r\n      <p className={\"tip\"}>支付安全由中国人民财产保险股份有限公司承包</p>\r\n    </div>\r\n  );\r\n};\r\n","export const wxPay = (info) => {\r\n  const handleReady = (res) => {\r\n    // eslint-disable-next-line no-undef\r\n    WeixinJSBridge.invoke(\"getBrandWCPayRequest\", info, res);\r\n  };\r\n\r\n  return new Promise((res) => {\r\n    if (typeof WeixinJSBridge == \"undefined\") {\r\n      if (document.addEventListener) {\r\n        document.addEventListener('WeixinJSBridgeReady', () => handleReady(res), false);\r\n      } else if (document.attachEvent) {\r\n        document.attachEvent('WeixinJSBridgeReady', () => handleReady(res));\r\n        document.attachEvent('onWeixinJSBridgeReady', () => handleReady(res));\r\n      }\r\n    } else {\r\n      handleReady(res);\r\n    }\r\n  });\r\n};\r\n","import * as React from \"react\";\r\nimport {useEffect, useState} from \"react\";\r\nimport {getNovelWechatQrcode, INovelWechatQrcode} from \"../api/UserApi\";\r\nimport {LoadingState} from \"../pages/common/LoadMoreBottomContent\";\r\nimport {match, RouteChildrenProps} from \"react-router\";\r\nimport {getNovelContent} from \"../api/NovelApi\";\r\nimport {goToPageWithCustomerId} from \"../utils/RouteUtil\";\r\nimport {AutoMaticSetLoadingState} from \"../HOC/AutoMaticSetLoadingState\";\r\nimport {ResponseCode} from \"../utils/ResponseCode\";\r\nimport {Toast} from \"antd-mobile\";\r\nimport \"./index.less\";\r\n\r\nexport interface IFollowParams {\r\n  id: number;\r\n  chapter: number;\r\n}\r\n\r\nexport const Follow: React.FC<RouteChildrenProps> = (props) => {\r\n  const [qrcode, setQrcode] = useState<INovelWechatQrcode | null>(null);\r\n  const [loadingState, setLoadingState] = useState(LoadingState.LOADING);\r\n  const params = (props.match as match).params as IFollowParams;\r\n\r\n  useEffect(() => {\r\n    if (loadingState === LoadingState.LOADING) {\r\n      Toast.loading(\"加载中\", 0);\r\n    } else {\r\n      Toast.hide();\r\n    }\r\n  }, [loadingState]);\r\n\r\n  useEffect(() => {\r\n    if (params !== null && params !== undefined) {\r\n      AutoMaticSetLoadingState(\r\n        getNovelWechatQrcode(params.id, params.chapter)\r\n          .then((data) => {\r\n            setQrcode(data.data);\r\n          }),\r\n        (state) => setLoadingState(state)\r\n      )\r\n    }\r\n\r\n    const cb = () => {\r\n      if (!document.hidden && params !== null && params !== undefined) {\r\n        getNovelContent(params.id, params.chapter)\r\n          .then((data) => {\r\n            if (data.err_code === ResponseCode.SUCCESS) {\r\n              goToPageWithCustomerId(props.history, {\r\n                pathname: `/read/${params.id}-${params.chapter}.html`,\r\n                state: {reload: true}\r\n              }, true)\r\n            }\r\n          });\r\n      }\r\n    };\r\n\r\n    window.addEventListener(\"visibilitychange\", cb);\r\n    return () => {\r\n      window.removeEventListener(\"visibilitychange\", cb);\r\n    }\r\n\r\n  }, []);\r\n\r\n  return (\r\n    <div className={\"follow-container\"}>\r\n      {/*<img className={\"bg\"} src=\"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/%E5%9B%BE%E5%B1%824.png\" alt=\"\"/>*/}\r\n      {/*<img className={\"qrcode\"} src={qrcode === null ? \"\" : qrcode.qrcode} alt=\"\"/>*/}\r\n      <div className=\"content\">\r\n        <p className={\"tip\"}>篇幅有限，长按关注公众号</p>\r\n        <p className=\"sub-tip\">继续阅读后续精彩内容</p>\r\n\r\n        <div className=\"qrcode-wrapper\">\r\n          <img className={\"qrcode\"} src={qrcode == null ? \"\" : qrcode.qrcode} alt=\"\"/>\r\n        </div>\r\n        <p className=\"sub-tip mb\">长按上图二维码3秒</p>\r\n        <p className=\"sub-tip mb\">点击【前往图中包含的公众号】</p>\r\n        <p className=\"sub-tip mb\">关注公众号，精彩内容继续看</p>\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {useEffect, useState} from \"react\";\r\nimport {useCommonSet, useUserInfo} from \"../../hooks/Hooks\";\r\nimport {match, RouteChildrenProps} from \"react-router\";\r\nimport \"./index.less\";\r\nimport {getNovelBasicInfo, INovelBasicInfo} from \"../../api/NovelApi\";\r\nimport {FutureElement} from \"../common/FutrueElement\";\r\nimport {LoadingState} from \"../common/LoadMoreBottomContent\";\r\nimport {Icon} from \"antd-mobile\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\n\r\nexport interface IReadNeedPayParams {\r\n  id: number;\r\n  chapter: number;\r\n}\r\n\r\nexport const ReadNeedPay: React.FC<RouteChildrenProps> = (props) => {\r\n  const [userInfo] = useUserInfo();\r\n  const [commonSet] = useCommonSet();\r\n  const [basicInfo, setBasicInfo] = useState<INovelBasicInfo | null>(null);\r\n  const params = (props.match as match).params as IReadNeedPayParams;\r\n  const [loadingState, setLoadingState] = useState(LoadingState.LOADING);\r\n\r\n  useEffect(() => {\r\n    getNovelBasicInfo(params.id, params.chapter)\r\n      .then((data) => {\r\n        setBasicInfo(data.data);\r\n      });\r\n  }, []);\r\n\r\n  useEffect(() => {\r\n    if (userInfo !== null && basicInfo !== null && commonSet !== null) {\r\n      setLoadingState(LoadingState.LOADED);\r\n    }\r\n  }, [userInfo, basicInfo, commonSet]);\r\n\r\n  return <FutureElement loadingState={loadingState} showBarrier={false} contentBuilder={() => {\r\n    if (userInfo === null || basicInfo === null || commonSet === null) {\r\n      return null;\r\n    } else {\r\n      return (\r\n        <div className={\"read-need-pay\"}>\r\n          <div className={\"bg\"} >\r\n            <img className={\"bg-icon\"} src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/yebz_top_bac.png\" alt=\"\"/>\r\n          </div>\r\n          <div onClick={() => {\r\n            props.history.go(-1);\r\n          }} className=\"back-wrapper\">\r\n            <Icon type={\"left\"}/>\r\n          </div>\r\n          <div className=\"content\">\r\n            <p className={\"title\"}>对不起，您的余额不足</p>\r\n            <div className=\"inner-content\">\r\n              <div className=\"user-header\">\r\n                <img className={\"avatar\"} src={userInfo.img} alt=\"\"/>\r\n                <p className={\"id-num\"}>ID: {userInfo.id}</p>\r\n                <div className=\"btn-wrapper\">\r\n                  <div onClick={() => {\r\n                    goToPageWithCustomerId(props.history, {pathname: \"/pay.html\"});\r\n                  }} className=\"to-book-shelf\">去充值</div>\r\n                </div>\r\n              </div>\r\n\r\n              <div className=\"cachet-wrapper\">\r\n                <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/yebz_text_bac.png\" alt=\"\"/>\r\n                <div className=\"content\">\r\n                  <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/yebz_vip_icon.png\" alt=\"\"/>\r\n                  <p className=\"title-name\">{basicInfo.title}</p>\r\n                </div>\r\n              </div>\r\n\r\n              <p className=\"line\"><span className={\"line-tip\"}>本章需要：</span>{commonSet}书币</p>\r\n              <p className=\"line\"><span className={\"line-tip\"}>账户余额：</span>{userInfo.balance}书币</p>\r\n\r\n              <div onClick={() => {\r\n                goToPageWithCustomerId(props.history, {pathname: \"/pay.html\"});\r\n              }} className=\"recharge-btn\">立即充值书币</div>\r\n\r\n              <p className=\"tip first-tip\">*余额大于50书币不能阅读，请联系客服</p>\r\n              {/*<p className=\"tip\">客服热线</p>*/}\r\n            </div>\r\n          </div>\r\n        </div>\r\n      )\r\n    }\r\n  }\r\n  }/>\r\n};\r\n","import * as React from \"react\";\r\nimport {useEffect, useState} from \"react\";\r\nimport {usePayCallbackUrl, usePayDomain, useUserInfo} from \"../../hooks/Hooks\";\r\nimport {FutureElement} from \"../common/FutrueElement\";\r\nimport {LoadingState} from \"../common/LoadMoreBottomContent\";\r\nimport {Toast} from \"antd-mobile\";\r\nimport {getPayInfo} from \"../../api/UserApi\";\r\nimport qs from \"qs\";\r\nimport {RouteChildrenProps} from \"react-router\";\r\n\r\nexport interface ICheckLoginPageParams {\r\n  cb?: string;\r\n  btn_id: number;\r\n}\r\n\r\nexport const CheckLoginPage: React.FC<RouteChildrenProps> = (props) => {\r\n  const [userInfo] = useUserInfo();\r\n  const [loadingState, setLoadingState] = useState<LoadingState>(LoadingState.LOADING);\r\n  const [payDomain] = usePayDomain();\r\n  const params = qs.parse(props.location.search.split(\"?\")[1]) as ICheckLoginPageParams;\r\n\r\n  useEffect(() => {\r\n    if (payDomain !== null && payDomain !== \"/\" && userInfo !== null && params !== null && params.btn_id !== null) {\r\n      setLoadingState(LoadingState.LOADED);\r\n      Toast.loading(\"加载中\", 0, undefined, false);\r\n      getPayInfo(params.btn_id)\r\n        .then((data) => {\r\n          alert(JSON.stringify(data));\r\n          Toast.hide();\r\n          window.location.href = payDomain + `/front/#/wx-pay.html?cb=${encodeURIComponent(params.cb === undefined ? \"\" : params.cb)}&` + encodeURIComponent(qs.stringify(data.data.order) + \"&out_trade_no=\" + data.data.out_trade_no);\r\n        })\r\n        .finally(() => Toast.hide());\r\n    }\r\n  }, [payDomain]);\r\n\r\n  return <FutureElement showBarrier={false} contentBuilder={() => null} loadingState={loadingState}/>\r\n};\r\n","import * as React from \"react\";\r\nimport \"./Read2Popup.less\";\r\nimport CSSTransition from \"react-transition-group/CSSTransition\";\r\n\r\nexport interface IRead2PopupProps {\r\n  qrcode: string | null;\r\n  open: boolean;\r\n  onClose: () => void;\r\n}\r\n\r\nexport const Read2Popup: React.FC<IRead2PopupProps> = (props) => {\r\n  return (\r\n    <CSSTransition unmountOnExit={true} mountOnEnter={true} appear={true} in={props.open} timeout={150} classNames={\"popup-fade\"}>\r\n      <div className=\"read2-popup-container\">\r\n        { props.open ? <div className=\"barrier\"/> : null}\r\n        <div className={\"read2-popup\"}>\r\n          <p className=\"title\">由于版权问题</p>\r\n          <p className=\"sub-title\">请扫下方二维码继续阅读</p>\r\n          <div className=\"qrcode-wrapper\">\r\n            <img className={\"qrcode\"} src={props.qrcode === null ? \"\" : props.qrcode} alt=\"\"/>\r\n          </div>\r\n          <p className=\"tip\">长按上图识别二维码</p>\r\n          <p className=\"continue\">继续阅读方式</p>\r\n          <p className=\"tip2\">公众号菜单-【阅读记录】</p>\r\n          <div onClick={props.onClose} className=\"close\"><span>关闭</span></div>\r\n        </div>\r\n      </div>\r\n    </CSSTransition>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {Fragment, useCallback, useEffect, useRef, useState} from \"react\";\r\nimport {match, RouteChildrenProps} from \"react-router\";\r\nimport {usePayCallbackUrl, useReadState, useReadThemePopup, useSearchParamsProvider,} from \"../../hooks/Hooks\";\r\nimport {IReadState} from \"../../entities/ReadState\";\r\nimport {Toast} from \"antd-mobile\";\r\nimport {getNovelContent, INovelContent} from \"../../api/NovelApi\";\r\nimport {LoadingState} from \"../common/LoadMoreBottomContent\";\r\nimport {AutoMaticSetLoadingState} from \"../../HOC/AutoMaticSetLoadingState\";\r\nimport {IPersistentProps} from \"../../HOC/PersistentComp\";\r\nimport {addNovelToBookShelf, getNovelWechatQrcode, INovelWechatQrcode} from \"../../api/UserApi\";\r\nimport {goToPageWithCustomerId, goToReadPage, goToReadPage2} from \"../../utils/RouteUtil\";\r\nimport {addToCache, getCache} from \"../../utils/LocalStorageUtil\";\r\nimport {ResponseCode} from \"../../utils/ResponseCode\";\r\nimport {SERVER_ROOT} from \"../../constants/Server\";\r\nimport {Read2Popup} from \"./Read2Popup\";\r\nimport \"./Read2.less\";\r\nimport ServiceButton from \"../common/ServiceButton\";\r\n\r\ninterface IReadRouteParams {\r\n  id: string;\r\n  sectionId: string;\r\n}\r\n\r\ntype theme = \"普通\" | \"泥巴色\" | \"粉色\" | \"草原绿\" | \"灰色\" | \"夜间模式\";\r\n\r\nconst fontColor = (themeType: theme) => {\r\n  switch (themeType) {\r\n    case \"普通\":\r\n      return \"#373e44\";\r\n    case \"泥巴色\":\r\n      return \"rgba(0, 0, 0, .85)\";\r\n    case \"粉色\":\r\n      return \"#896147\";\r\n    case \"草原绿\":\r\n      return \"#506d54\";\r\n    case \"灰色\":\r\n      return \"#5f5f5f\";\r\n    case \"夜间模式\":\r\n      return \"#746854\";\r\n  }\r\n};\r\n\r\nconst remSize = (size: number) => {\r\n  return size / 100 + \"rem\";\r\n};\r\n\r\nexport interface IBottomActionProps {\r\n  onClick?: (prev: boolean) => void;\r\n  isFinished: boolean;\r\n  isFirst: boolean;\r\n  onToDirectory: () => void;\r\n}\r\n\r\nconst BottomActions: React.FC<IBottomActionProps> = (props) => {\r\n  const [_readState, setReadState] = useReadState();\r\n  const readState = _readState as IReadState;\r\n  const [readThemePopup, setReadThemePopup] = useReadThemePopup();\r\n\r\n  useEffect(() => {\r\n    return () => {\r\n      setReadThemePopup(false);\r\n    }\r\n  }, []);\r\n\r\n  const handleOnClick = (prev: boolean) => {\r\n    if (undefined !== props.onClick) {\r\n      props.onClick(prev);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"bottom-actions\">\r\n      <div onClick={() => {\r\n        if (!props.isFirst) {\r\n          handleOnClick(true);\r\n        }\r\n      }} style={{color: fontColor(readState.theme)}}\r\n           className=\"btn-chapter btn-prev\">\r\n        <p style={{lineHeight: \".4rem\", height: \"100%\", opacity: props.isFirst ? \".5\" : \"1\"}}>上一章节</p>\r\n      </div>\r\n\r\n      <div onClick={() => {\r\n        if (!props.isFinished) {\r\n          handleOnClick(false);\r\n        }\r\n      }} style={{color: fontColor(readState.theme)}}\r\n           className=\"btn-chapter btn-next\">\r\n        <p style={{lineHeight: \".4rem\", height: \"100%\", opacity: props.isFinished ? \".5\" : \"1\"}}>下一章节</p>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport interface IReadPersistentData {\r\n  scrollTop: number;\r\n  novelContent: INovelContent | null;\r\n}\r\n\r\nexport interface ILastRead {\r\n  id: string;\r\n  chapter: string;\r\n}\r\n\r\nexport const Read2: React.FC<RouteChildrenProps & IPersistentProps> = (props) => {\r\n  const [_readState, setReadState] = useReadState();\r\n  const readState = _readState as IReadState;\r\n  readState.lineHeight = () => readState.fontSize * 1.5;\r\n  const {id, sectionId} = (((props.match as match).params) as IReadRouteParams);\r\n  const [loadingState, setLoadingState] = useState<LoadingState | null>(null);\r\n  const [novelContent, setNovelContent] = useState<INovelContent | null>(null);\r\n  const [shouldLoad, setShouldLoad] = useState(props.persistentData === null);\r\n  const containerRef: React.RefObject<HTMLDivElement> = useRef(null);\r\n  const [prev, setPrev] = useState(false);\r\n  const [payCallbackUrl, setPayCallbackUrl] = usePayCallbackUrl();\r\n  const [queryParams,] = useSearchParamsProvider();\r\n  const [popupOpen, setPopupOpen] = useState(false);\r\n  const [qrcode, setQrcode] = useState<INovelWechatQrcode | null>(null);\r\n\r\n\r\n  const persistData = useCallback(() => {\r\n    props.persistData({\r\n      scrollTop: 0,\r\n      novelContent: novelContent\r\n    });\r\n  }, [novelContent, setNovelContent, containerRef]);\r\n\r\n  useEffect(() => {\r\n  }, [novelContent]);\r\n\r\n  useEffect(() => {\r\n    if (shouldLoad && novelContent !== null) {\r\n      console.log(\"开始缓存\");\r\n      addToCache(\"lastRead\", {\r\n        id, sectionId\r\n      });\r\n      persistData();\r\n    }\r\n  }, [novelContent, containerRef]);\r\n\r\n\r\n  // 获取缓存并设置\r\n  useEffect(() => {\r\n    if (!shouldLoad && props.persistentData !== null) {\r\n      setLoadingState(LoadingState.LOADED);\r\n      const data = props.persistentData as IReadPersistentData;\r\n      setNovelContent(data.novelContent);\r\n      if (containerRef.current !== null) {\r\n        setTimeout(() => {\r\n          (containerRef.current as HTMLDivElement).scrollTop = data.scrollTop;\r\n        })\r\n      }\r\n    }\r\n\r\n    if (loadingState !== null || !shouldLoad) {\r\n      return;\r\n    }\r\n\r\n  }, [props, prev, shouldLoad, loadingState, containerRef]);\r\n\r\n  useEffect(() => {\r\n    // 加载小说数据\r\n    AutoMaticSetLoadingState(\r\n      Promise.all([\r\n        getNovelContent(\r\n          parseInt(id),\r\n          parseInt(sectionId),\r\n          prev ? \"last\" : \"next\",\r\n          queryParams === null ? undefined : queryParams\r\n        )\r\n          .then((data) => {\r\n            if (data.err_code === ResponseCode.SUCCESS) {\r\n              window.document.title = `第${data.data.num}章 ` + data.data.title;\r\n\r\n              if (data.data.content.indexOf(\"<div\") !== -1) {\r\n                console.log(data.data.content);\r\n                data.data.content = data.data.content.replace(/<br>|<br\\/>/g, \"<br>&nbsp;&nbsp;\").replace(/<p/g, \"<p class='content-line'\");\r\n                setNovelContent(data.data);\r\n              } else {\r\n                const elem = document.createElement(\"div\");\r\n                elem.innerHTML = data.data.content;\r\n                data.data.content = [...elem.childNodes]\r\n                // 过滤br标签\r\n                  .filter((_node) => _node.nodeName !== \"BR\")\r\n                  // 映射出文字\r\n                  .map((_node) => _node.nodeName === \"#text\" ? _node.nodeValue : _node.textContent)\r\n                  // 去掉前面的空格\r\n                  .map((_val) => {\r\n                    console.log(_val);\r\n                    return _val === null ? \"\" : _val.replace(/^\\s*/g, \"\").replace(/nbsp;/g, \"\");\r\n                  })\r\n                  // 生成p标签\r\n                  .map((_val) => `<p class=\"content-line\">${_val}</p>`)\r\n                  // 合并数组为字符串\r\n                  .reduce((prev, next) => prev + next, \"\");\r\n\r\n                setNovelContent(data.data);\r\n                elem.remove();\r\n              }\r\n\r\n              // 需要充值才能阅读\r\n            } else if (data.err_code === ResponseCode.TO_RECHARGE) {\r\n              Toast.hide();\r\n              // 如果该章节已经跳转支付则跳转小说详情以避免死循环 并标记为未跳转支付\r\n              if (payCallbackUrl !== null && payCallbackUrl.split(\"#\")[1] === `/read/${id}-${sectionId}.html`) {\r\n                console.log(payCallbackUrl.split(\"#\")[1].split(\"/\")[2].split(\"-\")[0]);\r\n                const lastRead = getCache<ILastRead>(\"lastRead\");\r\n                if (\r\n                  lastRead !== null &&\r\n                  lastRead.id === payCallbackUrl.split(\"#\")[1].split(\"/\")[2].split(\"-\")[0]\r\n                ) {\r\n                  setPrev(true);\r\n                  goToReadPage(props.history, parseInt(lastRead.id), 0, false, true);\r\n                } else {\r\n                  Toast.fail(\"请先充值后再阅读\", 1.5, undefined, false);\r\n                  props.history.go(-1);\r\n                  // goToPageWithCustomerId(props.history, {pathname: `/detail/novel-${id}.html`, state: {reload: true}}, true);\r\n                }\r\n                setPayCallbackUrl(null);\r\n                return;\r\n              }\r\n\r\n              // 标记为已跳转支付 并跳转支付\r\n              setPayCallbackUrl(SERVER_ROOT + \"front/\" + window.location.hash);\r\n              goToPageWithCustomerId(props.history, {\r\n                pathname: `/need-pay-${id}-${sectionId}.html`,\r\n              }, false);\r\n            } else if (data.err_code === ResponseCode.NOW_FOLLOW) {\r\n              setPopupOpen(true);\r\n              // if (data.err_code === ResponseCode.NOW_FOLLOW) {\r\n              //   goToPageWithCustomerId(props.history, {\r\n              //     pathname: `/follow-${id}-${sectionId}.html`,\r\n              //     state: {reload: true}\r\n              //   }, true);\r\n              // } else {\r\n              //   props.history.go(-1);\r\n              //   Toast.fail(data.err_msg, 1.5, undefined, false);\r\n              // }\r\n            }\r\n          })\r\n      ]),\r\n      (state) => setLoadingState(state)\r\n    );\r\n    getNovelWechatQrcode(parseInt(id), parseInt(sectionId))\r\n      .then((data) => setQrcode(data.data));\r\n  }, [props.location]);\r\n\r\n  // 内容变动的时候持久化数据\r\n  useEffect(() => {\r\n    if (loadingState === LoadingState.LOADED) {\r\n      (containerRef.current as HTMLDivElement).scrollTop = 0;\r\n    }\r\n  }, [loadingState]);\r\n\r\n  useEffect(() => {\r\n    if (loadingState === LoadingState.LOADING) {\r\n      Toast.loading(\"加载中\", 0);\r\n    } else if (loadingState === LoadingState.LOADED) {\r\n      Toast.hide();\r\n    }\r\n  }, [loadingState]);\r\n\r\n  const handleScroll = useCallback(() => {\r\n    if (containerRef.current !== null) {\r\n      const scrollTop = (containerRef.current as HTMLDivElement).scrollTop;\r\n      props.persistData({\r\n        novelContent, scrollTop\r\n      })\r\n    }\r\n  }, [containerRef, novelContent]);\r\n\r\n  const handleAddToBookStore = useCallback(() => {\r\n    if (novelContent !== null) {\r\n      addNovelToBookShelf(novelContent.novel_id, novelContent.num)\r\n        .then((data) => {\r\n          if (data.err_code === ResponseCode.SUCCESS) {\r\n            Toast.success(data.err_msg, 1.5, undefined, false);\r\n            setNovelContent({\r\n              ...novelContent,\r\n              is_bookstore: true,\r\n            })\r\n          } else {\r\n            Toast.fail(data.err_msg, 1.5, undefined, false);\r\n          }\r\n        });\r\n    }\r\n  }, [novelContent]);\r\n\r\n  return (\r\n    <Fragment>\r\n      <Read2Popup onClose={() => setPopupOpen(false)} qrcode={qrcode === null ? null : qrcode.qrcode} open={popupOpen}/>\r\n      <div onScroll={handleScroll} ref={containerRef} className=\"read2-scroller\">\r\n        <div style={{background: \"#fff\"}} className=\"read2-container\">\r\n          <Fragment>\r\n            {\r\n              novelContent === null\r\n                ? null\r\n                : (\r\n                  <Fragment>\r\n                    <ServiceButton />\r\n                    <p className=\"novel-title\">{novelContent.title}</p>\r\n                    <div style={\r\n                      {\r\n                        color: fontColor(readState.theme),\r\n                        fontSize: remSize(readState.fontSize),\r\n                        lineHeight: remSize(readState.lineHeight === undefined ? 30 : readState.lineHeight())\r\n                      }} className=\"content\" dangerouslySetInnerHTML={{__html: novelContent.content}}>\r\n                    </div>\r\n                    <p onClick={() => setPopupOpen(true)} className={\"subscribe\"}>[追书神技能]快人一步阅读最新章节</p>\r\n                    <BottomActions\r\n                      onToDirectory={() => {\r\n                        goToPageWithCustomerId(props.history, {pathname: `/detail/chapter-${id}.html`})\r\n                      }}\r\n                      onClick={(prev) => {\r\n                        if (prev) {\r\n                          setPrev(true);\r\n                          setLoadingState(null);\r\n                          setShouldLoad(true);\r\n                          goToReadPage2(props.history, parseInt(id), novelContent.num - 1, false, true)\r\n                        } else {\r\n                          setPrev(false);\r\n                          setLoadingState(null);\r\n                          setShouldLoad(true);\r\n                          goToReadPage2(props.history, parseInt(id), novelContent.num + 1, false, true)\r\n                        }\r\n                      }}\r\n                      isFinished={novelContent.num >= novelContent.section_count}\r\n                      isFirst={novelContent.num as number === 1}\r\n                    />\r\n                  </Fragment>\r\n                )\r\n            }\r\n          </Fragment>\r\n        </div>\r\n      </div>\r\n    </Fragment>\r\n  );\r\n};\r\n","import * as React from \"react\";\r\nimport {useReadState} from \"../../hooks/Hooks\";\r\nimport \"./ReadNavbar3.less\";\r\nimport {RouteChildrenProps, useHistory} from \"react-router\";\r\nimport {goBack, goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport {Icon} from \"antd-mobile\";\r\n\r\nexport interface IReadNavBar {\r\n  onFeedback(): void;\r\n  onComment(): void;\r\n  onReward(): void;\r\n  active: boolean;\r\n}\r\n\r\nexport const ReadNavbar: React.FC<IReadNavBar> = (props) => {\r\n  const [readTheme, setReadTheme] = useReadState();\r\n  const history = useHistory();\r\n\r\n  return (\r\n    <div className={\"read-nav-bar3\" + (props.active ? \" active\" : \"\")}>\r\n      <Icon onClick={() => goBack(history)} type={\"left\"} className={\"back\"}  />\r\n      <div className=\"right\">\r\n        <img onClick={props.onComment} src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/Read_comment_icon.png\" alt=\"\"/>\r\n        <img onClick={props.onReward} src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/read_money_icon.png\" alt=\"\"/>\r\n        <img onClick={props.onFeedback} src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/feedback_icon.png\" alt=\"\"/>\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport \"./ReadTabbar.less\";\r\nimport {useReadState, useTheme} from \"../../hooks/Hooks\";\r\n\r\nexport interface IReadTabbarProps {\r\n  onToDic(): void;\r\n  active: boolean;\r\n  onFontClick(): void;\r\n}\r\n\r\nexport const ReadTabbar: React.FC<IReadTabbarProps> = (props) => {\r\n  const [theme, setTheme] = useReadState();\r\n\r\n  return (\r\n    <div className={\"read-tabbar\" + (props.active ? \" active\" : \"\")}>\r\n      <div onClick={() => {\r\n        if (theme.theme === \"夜间模式\") {\r\n          setTheme({\r\n            ...theme,\r\n            theme: \"普通\",\r\n          })\r\n        } else {\r\n          setTheme({\r\n            ...theme,\r\n            theme: \"夜间模式\",\r\n          })\r\n        }\r\n      }} className=\"tab-bar-item\">\r\n        <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/read_night_icon.png\" alt=\"\" className=\"icon\"/>\r\n        <p>{theme.theme === \"夜间模式\" ? \"普通\" : \"夜间\"}</p>\r\n      </div>\r\n\r\n      <div onClick={props.onToDic} className=\"tab-bar-item\">\r\n        <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/read_list_icon.png\" alt=\"\" className=\"icon\"/>\r\n        <p>目录</p>\r\n      </div>\r\n\r\n      <div onClick={props.onFontClick} className=\"tab-bar-item\">\r\n        <img src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/read_setting_icon.png\" alt=\"\" className=\"icon\"/>\r\n        <p>字体</p>\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {Fragment, useCallback, useEffect, useRef, useState} from \"react\";\r\nimport {match, RouteChildrenProps} from \"react-router\";\r\nimport {\r\n  usePayCallbackUrl,\r\n  useReadState,\r\n  useReadThemePopup,\r\n  useRewardPopup,\r\n  useSearchParamsProvider,\r\n  useUserInfo\r\n} from \"../../hooks/Hooks\";\r\nimport {IReadState} from \"../../entities/ReadState\";\r\nimport {Button, Modal, Slider, Toast} from \"antd-mobile\";\r\nimport {getNovelContent, getNovelRecommends, INovelContent, INovelRecommend} from \"../../api/NovelApi\";\r\nimport {LoadingState} from \"../common/LoadMoreBottomContent\";\r\nimport {AutoMaticSetLoadingState} from \"../../HOC/AutoMaticSetLoadingState\";\r\nimport {IPersistentProps} from \"../../HOC/PersistentComp\";\r\nimport {ReadMenu, RewardPopupEvent} from \"../../entities/Event\";\r\nimport {addNovelToBookShelf, getUserInfo} from \"../../api/UserApi\";\r\nimport {IRewardConfirmDetail} from \"../common/RewardPopup\";\r\nimport {doReward} from \"../../api/RewardApi\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport {addToCache, getCache} from \"../../utils/LocalStorageUtil\";\r\nimport {ResponseCode} from \"../../utils/ResponseCode\";\r\nimport {ReadNavbar} from \"./ReadnavBar3\";\r\nimport {ReadSubscribe} from \"./ReadSubscribe\";\r\nimport {SERVER_ROOT} from \"../../constants/Server\";\r\nimport {ReadTabbar} from \"./ReadTabbar\";\r\nimport {arabToChinese} from \"../../utils/Utils\";\r\nimport \"./Read3.less\";\r\nimport {History} from \"history\";\r\nimport ServiceButton from \"../common/ServiceButton\";\r\n\r\nexport const goToReadPage = (\r\n  history: History<any>,\r\n  id: number,\r\n  chapter: number,\r\n  reload: boolean = false,\r\n  replace: boolean = false,\r\n  setLoadingState?: (state: LoadingState) => void\r\n) => {\r\n  if (setLoadingState !== undefined) {\r\n    setLoadingState(LoadingState.LOADING);\r\n  }\r\n  goToPageWithCustomerId(history, {pathname: `/read3/${id}-${chapter}.html`, state: {reload}}, replace);\r\n};\r\n\r\ninterface IReadRouteParams {\r\n  id: string;\r\n  sectionId: string;\r\n}\r\n\r\ntype theme = \"普通\" | \"泥巴色\" | \"粉色\" | \"草原绿\" | \"灰色\" | \"夜间模式\";\r\n\r\nconst themes = [\"普通\", \"泥巴色\", \"粉色\", \"草原绿\", \"灰色\", \"夜间模式\"];\r\n\r\nconst bgColor = (themeType: theme): string => {\r\n  switch (themeType) {\r\n    case \"普通\":\r\n      return \"#ededed\";\r\n    case \"泥巴色\":\r\n      return \"#e4c8a1 url(https://qcdn.zhangzhongyun.com/spa/179c1ccfd3.jpg) repeat center 100%\";\r\n    case \"粉色\":\r\n      return \"#f1e8e7\";\r\n    case \"草原绿\":\r\n      return \"#e7f1e8\";\r\n    case \"灰色\":\r\n      return \"#d1d1d1\";\r\n    case \"夜间模式\":\r\n      return \"#1b1714\";\r\n  }\r\n};\r\n\r\nconst fontColor = (themeType: theme) => {\r\n  switch (themeType) {\r\n    case \"普通\":\r\n      return \"#373e44\";\r\n    case \"泥巴色\":\r\n      return \"rgba(0, 0, 0, .85)\";\r\n    case \"粉色\":\r\n      return \"#896147\";\r\n    case \"草原绿\":\r\n      return \"#506d54\";\r\n    case \"灰色\":\r\n      return \"#5f5f5f\";\r\n    case \"夜间模式\":\r\n      return \"#746854\";\r\n  }\r\n};\r\n\r\nconst Recommends: React.FC<RouteChildrenProps & { items: Array<INovelRecommend> }> = (props) => {\r\n  const [readTheme] = useReadState();\r\n\r\n  return (\r\n    <div className=\"recommend-wrapper\">\r\n      <p className=\"title\" style={{color: readTheme.theme === \"夜间模式\" ? \"#fff\" : \"#666\"}}>相关推荐</p>\r\n      {\r\n        props.items.map((item) => (\r\n          <a\r\n            style={{color: readTheme.theme === \"夜间模式\" ? \"rgb(173, 143, 94)\" : \"rgb(204, 162, 102)\"}}\r\n            key={item.id}\r\n            className={\"recommend-link\"}\r\n            onClick={() => {\r\n              goToReadPage(props.history, item.id, 0, true, false);\r\n            }}\r\n          >\r\n            {item.desc.length > 40 ? item.desc.slice(0, 37) + \"...\" : item.desc}\r\n          </a>\r\n        ))\r\n      }\r\n    </div>\r\n  )\r\n};\r\n\r\nconst remSize = (size: number) => {\r\n  return size / 100 + \"rem\";\r\n};\r\n\r\nconst RText: React.FC = (props) => {\r\n  const [_readState] = useReadState();\r\n  const readState = _readState as IReadState;\r\n\r\n  return <div style={{fontSize: remSize(readState.fontSize), color: fontColor(readState.theme)}}>{props.children}</div>\r\n};\r\n\r\nexport interface IBottomActionProps {\r\n  onClick?: (prev: boolean) => void;\r\n  isFinished: boolean;\r\n  isFirst: boolean;\r\n  onToDirectory: () => void;\r\n}\r\n\r\nconst BottomActions: React.FC<IBottomActionProps> = (props) => {\r\n  const [_readState, setReadState] = useReadState();\r\n  const readState = _readState as IReadState;\r\n  const [readThemePopup, setReadThemePopup] = useReadThemePopup();\r\n\r\n  useEffect(() => {\r\n    return () => {\r\n      setReadThemePopup(false);\r\n    }\r\n  }, []);\r\n\r\n  const handleOnClick = (prev: boolean) => {\r\n    if (undefined !== props.onClick) {\r\n      props.onClick(prev);\r\n    }\r\n  };\r\n  return (\r\n    <div className=\"bottom-actions\">\r\n      <div onClick={() => {\r\n        if (!props.isFirst) {\r\n          handleOnClick(true);\r\n        }\r\n      }} style={{color: fontColor(readState.theme)}}\r\n           className=\"btn-chapter btn-prev\">\r\n        <p style={{opacity: props.isFirst ? \".5\" : \"1\"}}>上一章</p>\r\n      </div>\r\n      <div style={{color: fontColor(readState.theme), fontWeight: \"bold\"}} onClick={props.onToDirectory}\r\n           className={\"menu-icon-wrapper\"}>\r\n        目录\r\n      </div>\r\n      <div onClick={() => {\r\n        if (!props.isFinished) {\r\n          handleOnClick(false);\r\n        }\r\n      }} style={{color: fontColor(readState.theme)}}\r\n           className=\"btn-chapter btn-next\">\r\n        <p style={{opacity: props.isFinished ? \".5\" : \"1\"}}>下一章</p>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport interface IReadPersistentData {\r\n  scrollTop: number;\r\n  novelContent: INovelContent | null;\r\n}\r\n\r\nexport interface ILastRead {\r\n  id: string;\r\n  chapter: string;\r\n}\r\n\r\nexport const Read3: React.FC<RouteChildrenProps & IPersistentProps> = (props) => {\r\n  const [_readState, setReadState] = useReadState();\r\n  const readState = _readState as IReadState;\r\n  readState.lineHeight = () => readState.fontSize * 1.5;\r\n  const {id, sectionId} = (((props.match as match).params) as IReadRouteParams);\r\n  const [loadingState, setLoadingState] = useState<LoadingState | null>(null);\r\n  const [novelContent, setNovelContent] = useState<INovelContent | null>(null);\r\n  const [novelRecommends, setNovelRecommends] = useState<Array<INovelRecommend>>([]);\r\n  const [shouldLoad, setShouldLoad] = useState(props.persistentData === null);\r\n  const [rewardPopupOpen, setRewardPopupOpen] = useRewardPopup();\r\n  const [readThemePopup, setReadThemePopup] = useReadThemePopup();\r\n  const containerRef: React.RefObject<HTMLDivElement> = useRef(null);\r\n  const [, setUserInfo] = useUserInfo();\r\n  const [prev, setPrev] = useState(false);\r\n  const [payCallbackUrl, setPayCallbackUrl] = usePayCallbackUrl();\r\n  const [queryParams,] = useSearchParamsProvider();\r\n\r\n  const [menuActive, setMenuActive] = useState(false);\r\n  const [fontPopupOpen, setFontPopupOpen] = useState(false);\r\n\r\n  const persistData = useCallback(() => {\r\n    props.persistData({\r\n      scrollTop: 0,\r\n      novelContent: novelContent\r\n    });\r\n  }, [novelContent, setNovelContent, containerRef]);\r\n\r\n  useEffect(() => {\r\n    const menuSubId = PubSub.subscribe(ReadMenu.TO_DIRECTORY_CLICK, (evt: ReadMenu) => {\r\n      goToPageWithCustomerId(props.history, {pathname: `/detail/chapter-${id}.html`})\r\n    });\r\n\r\n    const addSubId = PubSub.subscribe(ReadMenu.ADD_TO_BOOKSHELF_CLICK, (evt: ReadMenu) => {\r\n      addNovelToBookShelf(parseInt(id))\r\n        .then((data) => {\r\n          if (data.err_code === ResponseCode.SUCCESS) {\r\n            Toast.success(\"已添加到书架\", 1.5, undefined, false);\r\n          }\r\n        })\r\n    });\r\n\r\n    const rewardSubId = PubSub.subscribe(ReadMenu.REWARD_CLICK, (evt: ReadMenu) => {\r\n      setReadThemePopup(false);\r\n      setRewardPopupOpen(true);\r\n    });\r\n\r\n    const doRewardSubId = PubSub.subscribe(RewardPopupEvent.REWARD_CONFIRMED, (messageType: RewardPopupEvent, data: IRewardConfirmDetail) => {\r\n      doReward(data.id, parseInt(id), data.amount)\r\n        .then(async (data) => {\r\n          if (data.err_code === ResponseCode.SUCCESS) {\r\n            Toast.success(data.err_msg, 1.5, undefined, false);\r\n            setRewardPopupOpen(false);\r\n            setUserInfo(await getUserInfo().then((data) => data.data));\r\n          }\r\n        });\r\n    });\r\n\r\n    return () => {\r\n      PubSub.unsubscribe(menuSubId);\r\n      PubSub.unsubscribe(addSubId);\r\n      PubSub.unsubscribe(rewardSubId);\r\n      PubSub.unsubscribe(doRewardSubId);\r\n    }\r\n  }, [setReadThemePopup, setRewardPopupOpen]);\r\n\r\n  useEffect(() => {\r\n    if (shouldLoad && novelContent !== null) {\r\n      console.log(\"开始缓存\");\r\n      addToCache(\"lastRead\", {\r\n        id, sectionId\r\n      });\r\n      persistData();\r\n    }\r\n  }, [novelContent, containerRef]);\r\n\r\n\r\n  // 获取缓存并设置\r\n  useEffect(() => {\r\n    if (!shouldLoad && props.persistentData !== null) {\r\n      setLoadingState(LoadingState.LOADED);\r\n      const data = props.persistentData as IReadPersistentData;\r\n      setNovelContent(data.novelContent);\r\n      if (containerRef.current !== null) {\r\n        setTimeout(() => {\r\n          (containerRef.current as HTMLDivElement).scrollTop = data.scrollTop;\r\n        })\r\n      }\r\n    }\r\n\r\n    if (loadingState !== null || !shouldLoad) {\r\n      return;\r\n    }\r\n\r\n  }, [props, prev, shouldLoad, loadingState, containerRef]);\r\n\r\n  useEffect(() => {\r\n    const _queryParams: { [propName: string]: string } = JSON.parse(JSON.stringify(queryParams));\r\n    delete _queryParams[\"section\"];\r\n\r\n    Toast.loading(\"加载中\", 0);\r\n    // 加载小说数据\r\n    AutoMaticSetLoadingState(\r\n      Promise.all([\r\n        getNovelContent(\r\n          parseInt(id),\r\n          parseInt(sectionId),\r\n          prev ? \"last\" : \"next\",\r\n          _queryParams === null ? undefined : _queryParams\r\n        )\r\n          .then((data) => {\r\n            Toast.hide();\r\n            if (data.err_code === ResponseCode.SUCCESS) {\r\n              window.document.title = `第${data.data.num}章 ` + data.data.title;\r\n              if (data.data.content.indexOf(\"<div\") !== -1) {\r\n                console.log(data.data.content);\r\n                data.data.content = data.data.content.replace(/<br>|<br\\/>/g, \"<br>&nbsp;&nbsp;\").replace(/<p/g, \"<p class='content-line'\");\r\n                setNovelContent(data.data);\r\n              } else {\r\n                const elem = document.createElement(\"div\");\r\n                elem.innerHTML = data.data.content;\r\n                data.data.content = [...elem.childNodes]\r\n                // 过滤br标签\r\n                  .filter((_node) => _node.nodeName !== \"BR\")\r\n                  // 映射出文字\r\n                  .map((_node) => _node.nodeName === \"#text\" ? _node.nodeValue : _node.textContent)\r\n                  // 去掉前面的空格\r\n                  .map((_val) => {\r\n                    console.log(_val);\r\n                    return _val === null ? \"\" : _val.replace(/^\\s*/g, \"\").replace(/nbsp;/g, \"\");\r\n                  })\r\n                  // 生成p标签\r\n                  .map((_val) => `<p class=\"content-line\">${_val}</p>`)\r\n                  // 合并数组为字符串\r\n                  .reduce((prev, next) => prev + next, \"\");\r\n\r\n                setNovelContent(data.data);\r\n                elem.remove();\r\n              }\r\n\r\n              // 需要充值才能阅读\r\n            } else if (data.err_code === ResponseCode.TO_RECHARGE) {\r\n              // 如果该章节已经跳转支付则跳转小说详情以避免死循环 并标记为未跳转支付\r\n              if (payCallbackUrl !== null && payCallbackUrl.split(\"#\")[1].indexOf(`/read/${id}-${sectionId}.html`) !== -1) {\r\n                console.log(payCallbackUrl.split(\"#\")[1].split(\"/\")[2].split(\"-\")[0]);\r\n                const lastRead = getCache<ILastRead>(\"lastRead\");\r\n                if (\r\n                  lastRead !== null &&\r\n                  lastRead.id === payCallbackUrl.split(\"#\")[1].split(\"/\")[2].split(\"-\")[0]\r\n                ) {\r\n                  setPrev(true);\r\n                  goToReadPage(props.history, parseInt(lastRead.id), 0, false, true);\r\n                } else {\r\n                  Toast.fail(\"请先充值后再阅读\", 1.5, undefined, false);\r\n                  props.history.go(-1);\r\n                  // goToPageWithCustomerId(props.history, {pathname: `/detail/novel-${id}.html`, state: {reload: true}}, true);\r\n                }\r\n                setPayCallbackUrl(null);\r\n                return;\r\n              }\r\n\r\n              // 标记为已跳转支付 并跳转支付\r\n              setPayCallbackUrl(SERVER_ROOT + \"front/\" + window.location.hash);\r\n              goToPageWithCustomerId(props.history, {\r\n                pathname: `/need-pay-${id}-${sectionId}.html`,\r\n              }, false);\r\n            } else {\r\n              if (data.err_code === ResponseCode.NOW_FOLLOW) {\r\n                goToPageWithCustomerId(props.history, {\r\n                  pathname: `/follow-${id}-${sectionId}.html`,\r\n                  state: {reload: true}\r\n                }, true);\r\n              } else {\r\n                props.history.go(-1);\r\n                Toast.fail(data.err_msg, 1.5, undefined, false);\r\n              }\r\n            }\r\n          }),\r\n        getNovelRecommends(parseInt(id))\r\n          .then((data) => setNovelRecommends(data.data))\r\n      ]),\r\n      (state) => setLoadingState(state)\r\n    )\r\n  }, [props.location]);\r\n\r\n  // 内容变动的时候持久化数据\r\n  useEffect(() => {\r\n    if (loadingState === LoadingState.LOADED) {\r\n      (containerRef.current as HTMLDivElement).scrollTop = 0;\r\n    }\r\n  }, [loadingState]);\r\n\r\n  const handleScroll = useCallback(() => {\r\n    if (menuActive) {\r\n      setMenuActive(false);\r\n    }\r\n\r\n    if (containerRef.current !== null) {\r\n      const scrollTop = (containerRef.current as HTMLDivElement).scrollTop;\r\n      props.persistData({\r\n        novelContent, scrollTop\r\n      })\r\n    }\r\n  }, [containerRef, novelContent, menuActive]);\r\n\r\n  useEffect(() => {\r\n    return () => {\r\n      setRewardPopupOpen(false);\r\n    }\r\n  }, []);\r\n\r\n  const handleAddToBookStore = useCallback(() => {\r\n    if (novelContent !== null) {\r\n      addNovelToBookShelf(novelContent.novel_id, novelContent.num)\r\n        .then((data) => {\r\n          if (data.err_code === ResponseCode.SUCCESS) {\r\n            Toast