             if (data.data.last_page === null) {\r\n                setCurrentLoadingStateByPage(LoadingState.FINISHED);\r\n              } else {\r\n                setCurrentLoadingStateByPage(LoadingState.LOADED)\r\n              }\r\n              setList([...(list === null ? [] : list), ...data.data.data.novel_list.map((item): IHomeMoreListItem => ({\r\n                img: item.img,\r\n                desc: item.desc,\r\n                novel_id: item.id,\r\n                suitable_sex: item.suitable_sex,\r\n                tags: item.tags,\r\n                title: item.title\r\n              }))]);\r\n            }),\r\n          (state) => {\r\n            if (state === LoadingState.LOADED) {\r\n              setCurrentLoadingStateByPage(LoadingState.FINISHED);\r\n            } else {\r\n              setCurrentLoadingStateByPage(state);\r\n            }\r\n          }\r\n        );\r\n        break;\r\n    }\r\n  }, [setList, currentPage, shouldLoad]);\r\n\r\n  return (\r\n    <PageWithNavBar {...props} title={listTypes[type]} rightContent={\r\n      <p className={\"more-touch\"}\r\n         style={{color: theme.mainColor}}\r\n         onClick={() => goToPageWithCustomerId(props.history, {pathname: \"/index.html\", state: {reload: true}})}\r\n      >首页</p>\r\n    }>\r\n      <FutureElement showLoadingDelay={0} showBarrier={false} loadingState={loadingState} contentBuilder={() => (\r\n        <LoadMoreList onScroll={handleScroll} initPos={scrollTop} onLoadMore={() => {\r\n          setShouldLoad(true);\r\n          if (loadMoreState === LoadingState.FINISHED) {\r\n            return;\r\n          }\r\n          setCurrentPage(currentPage + 1);\r\n        }} className={\"book-list-container\"}\r\n                      itemCount={list === null ? 0 : list.length} itemBuilder={(index) => {\r\n          if (list === null) {\r\n            return null;\r\n          }\r\n\r\n          if (type === \"free\") {\r\n            return (\r\n              <BookItemFree\r\n                // TODO 真实数据待加\r\n                startDate={12345}\r\n                endDate={1232323}\r\n                onClick={() => goToPageWithCustomerId(props.history, {pathname: `/detail/novel-${list[index].novel_id}.html`})}\r\n                desc={list[index].desc}\r\n                name={list[index].title}\r\n                img={list[index].img}\r\n              />\r\n            )\r\n          }\r\n\r\n          return (\r\n            <BookItemVertical\r\n              onClick={() => goToPageWithCustomerId(props.history, {pathname: `/detail/novel-${list[index].novel_id}.html`})}\r\n              key={list[index].novel_id}\r\n              desc={list[index].desc}\r\n              img={list[index].img}\r\n              name={list[index].title}\r\n              id={list[index].novel_id}\r\n              tags={list[index].tags === null ? [] : (list[index].tags as string).split(\"|\")}\r\n            />\r\n          );\r\n        }}>\r\n\r\n          {\r\n            list !== null\r\n              ? list.length > 0\r\n              ? <LoadMoreBottomContent status={loadMoreState}/>\r\n              : <NoContent/>\r\n              : null\r\n          }\r\n        </LoadMoreList>\r\n      )}/>\r\n    </PageWithNavBar>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport \"./Category.less\";\r\nimport {useTheme} from \"../../hooks/Hooks\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport {CSSProperties, useState} from \"react\";\r\n\r\ninterface ITopListCategoryItem<T> {\r\n  name: string;\r\n  key: T\r\n}\r\n\r\nexport interface ITopListCategory<T> {\r\n  items: Array<ITopListCategoryItem<T>>;\r\n  onChange?: (key: T) => boolean;\r\n  initialCategory: T\r\n}\r\n\r\nexport const TopListCategory: React.FC<ITopListCategory<string>> = (props) => {\r\n  const theme = useTheme()[0] as Theme;\r\n\r\n  const activeStyle: CSSProperties = {\r\n    background: theme.mainColor\r\n  };\r\n\r\n  const activeFontStyle: CSSProperties = {\r\n    color: \"#ffffff\"\r\n  };\r\n\r\n  const handleClick = (key: string) => {\r\n    if (props.onChange !== undefined) {\r\n      props.onChange(key);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <div className=\"top-list-category\">\r\n      {\r\n        props.items.map((category) => {\r\n          return (\r\n            <div style={props.initialCategory === category.key ? activeStyle : undefined} onClick={() => handleClick(category.key)} key={category.key} className=\"category-item\">\r\n              <p style={props.initialCategory === category.key ? activeFontStyle : undefined} className=\"content\">{category.name}</p>\r\n            </div>\r\n          )\r\n        })\r\n      }\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport \"./DetailChunk.less\";\r\nimport {useCallback} from \"react\";\r\n\r\nexport interface IDetailChunk {\r\n  title: string;\r\n  subtitle?: string;\r\n  btn?: JSX.Element;\r\n  withBorder?: boolean;\r\n}\r\n\r\nexport interface IRightBtnProps {\r\n  color: string;\r\n  backgroundColor: string;\r\n  icon?: JSX.Element;\r\n  title: string;\r\n  onClick?: () => void;\r\n}\r\n\r\nexport const RightBtn: React.FC<IRightBtnProps> = (props) => {\r\n  const handleOnClick = useCallback(() => {\r\n    if (props.onClick !== undefined) {\r\n      props.onClick();\r\n    }\r\n  }, []);\r\n\r\n  return (\r\n    <div onClick={handleOnClick} style={{background: props.backgroundColor, padding: \"0 .1rem\"}} className=\"right-btn\">\r\n      {\r\n        props.icon === undefined\r\n          ? null\r\n          : props.icon\r\n      }\r\n      <span style={{color: props.color}} className={\"title\"}>{props.title}</span>\r\n    </div>\r\n  )\r\n};\r\n\r\nexport const DetailChunk: React.FC<IDetailChunk> = (props) => {\r\n  return (\r\n    <div className=\"detail-chunk\">\r\n      <div className={props.withBorder === undefined ? \"header\" : \"header bordered\"}>\r\n        <p className={\"title\"}>{props.title}</p>\r\n        {\r\n          props.subtitle === undefined\r\n            ? null\r\n            : <p className={\"subtitle\"}>{props.subtitle}</p>\r\n        }\r\n        <div className=\"btn-wrapper\">\r\n          {props.btn}\r\n        </div>\r\n      </div>\r\n      {props.children}\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {dateToStr} from \"../../utils/Utils\";\r\nimport {INovelComment} from \"../../api/NovelApi\";\r\nimport \"./NovelComment.less\";\r\n\r\nexport const NovelComment: React.FC<INovelComment> = (props) => {\r\n  return (\r\n    <div className=\"comment\">\r\n      <div className=\"header\">\r\n        <div className=\"avatar-wrapper\">\r\n          <img src={props.user_img} alt=\"\"/>\r\n        </div>\r\n        <p className=\"username\">{props.username}</p>\r\n      </div>\r\n      <p className=\"content\">{props.content}</p>\r\n      <p className=\"date\">{dateToStr(props.created_at * 1000, \"yyyy-MM-dd hh:mm\")}</p>\r\n    </div>\r\n  )\r\n};\r\n","import {PostForm} from \"../utils/RequestShorcuts\";\r\n\r\nexport interface IRewardItem {\r\n  id: number;\r\n  name: string;\r\n  coin: number;\r\n  img: string;\r\n  sale_num: number;\r\n}\r\n\r\nexport const getRewardItems = () => {\r\n  return PostForm<Array<IRewardItem>>(\"/operate/goodslist\");\r\n};\r\n\r\nexport const doReward = (goods_id: number, novel_id: number, num: number) => {\r\n  return PostForm<null>(\"/operate/doreward\", undefined, {goods_id, novel_id, num});\r\n};\r\n","import * as React from \"react\";\r\nimport {IRewardListItem} from \"../../api/NovelApi\";\r\nimport {dateToStr} from \"../../utils/Utils\";\r\nimport \"./RewardRecord.less\";\r\n\r\nexport const RewardRecord: React.FC<IRewardListItem> = (props) => {\r\n\r\n  return (\r\n    <div className=\"reward-record\">\r\n      <div className=\"avatar-wrapper\">\r\n        <img src={props.user_img} alt=\"\"/>\r\n      </div>\r\n      <div className=\"middle\">\r\n        <p className={\"username\"}>{props.username}</p>\r\n        <p className=\"date\">{dateToStr(props.created_at * 1000, \"yyyy-MM-dd hh:mm\")}</p>\r\n      </div>\r\n      <div className=\"right\">\r\n        <div className=\"record-icon\"><img src={props.goods_img} alt=\"\"/></div>\r\n        <p className={\"amount\"}>x{props.goods_num}</p>\r\n      </div>\r\n    </div>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {Fragment, useCallback, useEffect, useRef, useState} from \"react\";\r\nimport {PageWithNavBar} from \"../common/PageWithNavBar\";\r\nimport {match, RouteChildrenProps} from \"react-router\";\r\nimport {BookItemVertical} from \"../common/BookItem\";\r\nimport {Icon, Toast, WhiteSpace, WingBlank} from \"antd-mobile\";\r\nimport {HomeChunk} from \"../common/HomeChunk\";\r\nimport {Axis} from \"../../entities/Axis\";\r\nimport {DetailChunk, RightBtn} from \"./DetailChunk\";\r\nimport likeIcon from \"./icons/like.png\";\r\nimport firstIcon from \"./icons/honor/first_icon.png\";\r\nimport secondIcon from \"./icons/honor/second_icon.png\";\r\nimport thirdIcon from \"./icons/honor/third_icon.png\";\r\nimport {\r\n  getNovelCommentsList,\r\n  getNovelDetail,\r\n  getNovelRewardList,\r\n  INovelComment,\r\n  INovelDetail,\r\n  IRewardListItem\r\n} from \"../../api/NovelApi\";\r\nimport {useDocumentTitle, useRewardPopup, useTheme, useUserInfo} from \"../../hooks/Hooks\";\r\nimport {LoadingState} from \"../common/LoadMoreBottomContent\";\r\nimport {AutoMaticSetLoadingState} from \"../../HOC/AutoMaticSetLoadingState\";\r\nimport {FutureElement} from \"../common/FutrueElement\";\r\nimport {RewardPopupEvent} from \"../../entities/Event\";\r\nimport {NovelComment} from \"../common/NovelComment\";\r\nimport {IPersistentProps} from \"../../HOC/PersistentComp\";\r\nimport {addNovelToBookShelf, getUserInfo} from \"../../api/UserApi\";\r\nimport {doReward} from \"../../api/RewardApi\";\r\nimport {IRewardConfirmDetail} from \"../common/RewardPopup\";\r\nimport commentIcon from \"./icons/comment.png\";\r\nimport \"./NovelDetail.less\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport {goToPageWithCustomerId, goToReadPage} from \"../../utils/RouteUtil\";\r\nimport {RewardRecord} from \"../common/RewardRecord\";\r\nimport {ResponseCode} from \"../../utils/ResponseCode\";\r\nimport ServiceButton from \"../common/ServiceButton\";\r\nimport {arabToChinese} from \"../../utils/Utils\";\r\n\r\nexport interface IBottomButtonsProp {\r\n  onJoin(): void;\r\n\r\n  isIn?: boolean;\r\n\r\n  onRead(): void;\r\n}\r\n\r\nconst BottomButtons: React.FC<IBottomButtonsProp> = (props) => {\r\n  const theme = useTheme()[0] as Theme;\r\n\r\n  return (\r\n    <div className=\"bottom-buttons\">\r\n      <div style={{color: \"#333\"}} onClick={props.onJoin}\r\n           className=\"collect\">{props.isIn === true ? \"已在书架\" : \"加入书架\"}</div>\r\n      <div style={{background: theme.mainColor}} onClick={props.onRead} className=\"start-read\">开始阅读</div>\r\n    </div>\r\n  )\r\n};\r\n\r\nBottomButtons.defaultProps = {\r\n  isIn: false,\r\n};\r\n\r\nconst topBorderClass = (index: number) => {\r\n  switch (index) {\r\n    case 0:\r\n      return \"avatar-wrapper top1\";\r\n    case 1:\r\n      return \"avatar-wrapper top2\";\r\n    case 2:\r\n      return \"avatar-wrapper top3\";\r\n  }\r\n};\r\n\r\nconst buildHonor = (index: number) => {\r\n  return (\r\n    <div className=\"honor-icon\">\r\n      <img src={\r\n        index === 0\r\n          ? firstIcon\r\n          : index === 1\r\n          ? secondIcon\r\n          : index === 2\r\n            ? thirdIcon\r\n            : \"\"\r\n      } alt=\"\"/>\r\n    </div>\r\n  )\r\n};\r\n\r\ninterface IComment {\r\n  avatar: string;\r\n  content: string;\r\n  name: string;\r\n  date: number;\r\n}\r\n\r\nconst buildComments = (comment: INovelComment, index: number): JSX.Element => {\r\n  return <NovelComment\r\n    key={index}\r\n    content={comment.content}\r\n    created_at={comment.created_at}\r\n    user_img={comment.user_img}\r\n    username={comment.username}\r\n  />\r\n};\r\n\r\ninterface INovelDetailId {\r\n  id: number;\r\n}\r\n\r\nexport interface INovelDetailPersistentData {\r\n  novelDetail: INovelDetail;\r\n  novelDetailComments: Array<INovelComment>;\r\n  novelRewardList: Array<IRewardListItem>;\r\n  novelDetailTopRewardList: Array<IRewardListItem>;\r\n  scrollTop: number;\r\n}\r\n\r\nexport const NovelDetail: React.FC<RouteChildrenProps & IPersistentProps> = (props) => {\r\n  useDocumentTitle(\"详情\");\r\n  const [userInfo, setUserInfo] = useUserInfo();\r\n  const [novelDetail, setNovelDetail] = useState<INovelDetail | null>(null);\r\n  const [novelDetailTopRewardList, setNovelDetailTopRewardList] = useState<Array<IRewardListItem> | null>(null);\r\n  const [novelDetailComments, setNovelDetailComments] = useState<Array<INovelComment> | null>(null);\r\n  const [novelRewardList, setNovelRewardList] = useState<Array<IRewardListItem> | null>(null);\r\n  const [rewardPopupOpen, setRewardPopupOpen] = useRewardPopup();\r\n  const theme = useTheme()[0] as Theme;\r\n  const scrollRef = useRef<HTMLDivElement>(null);\r\n\r\n  const [loadingState, setLoadingState] = useState(LoadingState.LOADING);\r\n  const id = ((props.match as match).params as INovelDetail).id;\r\n\r\n  useEffect(() => {\r\n    return () => {\r\n      setRewardPopupOpen(false);\r\n    }\r\n  }, []);\r\n\r\n  const handleReward = useCallback(() => {\r\n    setRewardPopupOpen(true);\r\n  }, [setRewardPopupOpen]);\r\n\r\n  const handleJoin = useCallback(() => {\r\n    if (novelDetail === null) {\r\n      return;\r\n    }\r\n    addNovelToBookShelf(novelDetail.id)\r\n      .then((data) => {\r\n        if (data.err_code === ResponseCode.SUCCESS) {\r\n          Toast.success(\"添加成功\", 1.5, undefined, false);\r\n          const data = JSON.parse(JSON.stringify(novelDetail)) as INovelDetail;\r\n          data.is_bookstore = true;\r\n          setNovelDetail(data);\r\n        } else {\r\n          Toast.fail(data.err_msg, 1.5, undefined, false);\r\n        }\r\n      })\r\n  }, [novelDetail]);\r\n\r\n  const handleRead = useCallback(() => {\r\n    if (novelDetail === null) {\r\n      return;\r\n    }\r\n\r\n    goToReadPage(props.history, novelDetail.id, 0);\r\n  }, [novelDetail]);\r\n\r\n  useEffect(() => {\r\n    if (novelDetail === null) {\r\n      return;\r\n    }\r\n\r\n    const id = PubSub.subscribe(RewardPopupEvent.REWARD_CONFIRMED, (messageType: RewardPopupEvent, data: IRewardConfirmDetail) => {\r\n      doReward(data.id, novelDetail.id, data.amount)\r\n        .then((data) => {\r\n          if (data.err_code === ResponseCode.SUCCESS) {\r\n            Toast.success(data.err_msg, 1.5, undefined, false);\r\n            setRewardPopupOpen(false);\r\n\r\n            getUserInfo()\r\n              .then((data) => {\r\n                setUserInfo(data.data);\r\n              });\r\n\r\n            getNovelDetail(novelDetail.id)\r\n              .then((data) => {\r\n                setNovelDetail(data.data);\r\n              });\r\n\r\n            getNovelRewardList(novelDetail.id, 1, \"created_at\")\r\n              .then((data) => {\r\n                setNovelRewardList(data.data.data);\r\n              });\r\n\r\n            getNovelRewardList(novelDetail.id, 1, \"coin_num\")\r\n              .then((data) => {\r\n                setNovelDetailTopRewardList(data.data.data);\r\n              });\r\n          }\r\n        });\r\n    });\r\n\r\n    return () => {\r\n      PubSub.unsubscribe(id);\r\n    }\r\n  }, [novelDetail]);\r\n\r\n  useEffect(() => {\r\n    console.log(\"进入了\");\r\n    if (rewardPopupOpen) {\r\n      return;\r\n    }\r\n    if (scrollRef.current !== null && props.persistentData !== null) {\r\n      const data = props.persistentData as INovelDetailPersistentData;\r\n      setTimeout(() => {\r\n        (scrollRef.current as HTMLDivElement).scrollTop = data.scrollTop;\r\n      }, 0);\r\n    }\r\n  }, [props.location, scrollRef]);\r\n\r\n  useEffect(() => {\r\n    if (props.persistentData !== null) {\r\n      setLoadingState(LoadingState.LOADED);\r\n      const data = props.persistentData as INovelDetailPersistentData;\r\n      setNovelDetail(data.novelDetail);\r\n      setNovelDetailTopRewardList(data.novelDetailTopRewardList);\r\n      setNovelDetailComments(data.novelDetailComments);\r\n      setNovelRewardList(data.novelRewardList);\r\n      if (scrollRef.current !== null) {\r\n        setTimeout(() => {\r\n          (scrollRef.current as HTMLDivElement).scrollTop = data.scrollTop;\r\n        })\r\n      }\r\n    }\r\n\r\n    AutoMaticSetLoadingState(\r\n      Promise.all([\r\n        getNovelDetail(id)\r\n          .then((data) => setNovelDetail(data.data)),\r\n      ]),\r\n      (state) => {\r\n        if (props.persistentData === null) {\r\n          setLoadingState(state)\r\n        }\r\n      }\r\n    );\r\n\r\n    getNovelCommentsList(id)\r\n      .then((data) => setNovelDetailComments(data.data.data));\r\n\r\n    getNovelRewardList(id, 1, \"created_at\")\r\n      .then((data) => setNovelRewardList(data.data.data));\r\n\r\n    getNovelRewardList(id, 1, \"coin_num\")\r\n      .then((data) => setNovelDetailTopRewardList(data.data.data));\r\n\r\n  }, [setNovelDetailComments, setNovelRewardList, setNovelDetail, setNovelDetailTopRewardList, scrollRef]);\r\n\r\n  const persistData = useCallback(() => {\r\n    props.persistData({\r\n      novelDetail,\r\n      novelRewardList,\r\n      novelDetailComments,\r\n      novelDetailTopRewardList,\r\n      scrollTop: (scrollRef.current as HTMLDivElement).scrollTop\r\n    })\r\n  }, [props.persistData, novelDetailComments, novelRewardList, novelDetail, novelDetailTopRewardList, scrollRef]);\r\n\r\n  useEffect(() => {\r\n    if (novelDetail !== null) {\r\n      persistData()\r\n    }\r\n  }, [persistData, novelDetailComments, novelRewardList, novelDetail, novelDetailTopRewardList]);\r\n\r\n  const handleScroll = useCallback(() => {\r\n    if (scrollRef.current !== null) {\r\n      persistData();\r\n    }\r\n  }, [scrollRef, novelDetailComments, novelRewardList, novelDetail, novelDetailTopRewardList]);\r\n\r\n  const buildDirectory = (index: number) => {\r\n    const val = (novelDetail as INovelDetail).section_first2last;\r\n    if (index === 0 && val.last !== undefined) {\r\n      return (\r\n        <WingBlank>\r\n          <p onClick={() => {\r\n            goToReadPage(props.history, val.last.novel_id, val.last.num, true);\r\n          }}\r\n             style={{color: theme.mainColor}}\r\n             className=\"newest-chapter\">第{arabToChinese((novelDetail as INovelDetail).section_first2last.last.num)}章 {(novelDetail as INovelDetail).section_first2last.last.title}</p>\r\n        </WingBlank>\r\n      )\r\n    } else if (index === 1 && val.first !== undefined) {\r\n      return (\r\n        <WingBlank>\r\n          <p onClick={() => {\r\n            goToReadPage(props.history, val.first.novel_id, val.first.num, true);\r\n          }}\r\n             className=\"first-chapter\">第{arabToChinese((novelDetail as INovelDetail).section_first2last.first.num)}章 {(novelDetail as INovelDetail).section_first2last.first.title}</p>\r\n        </WingBlank>\r\n      )\r\n    } else {\r\n      return <Fragment></Fragment>\r\n    }\r\n  };\r\n\r\n  const buildStatusName = (status: 1 | 2 | 3) => {\r\n    switch (status) {\r\n      case 1:\r\n        return \"连载中\";\r\n      case 2:\r\n        return \"已完结\";\r\n      case 3:\r\n        return \"限时免费\";\r\n    }\r\n  };\r\n\r\n  return (\r\n    <PageWithNavBar\r\n      rightContent={\r\n        <p className={\"more-touch\"}\r\n           style={{color: theme.mainColor}}\r\n           onClick={() => {\r\n             goToPageWithCustomerId(props.history, {pathname: \"/index.html\", state: {reload: true}}, true);\r\n           }}\r\n        >\r\n          首页\r\n        </p>}\r\n      title={\"详情\"}\r\n      {...props}\r\n    >\r\n      <FutureElement showLoadingDelay={0} showBarrier={true} loadingState={loadingState} contentBuilder={() => {\r\n        return (\r\n          <Fragment>\r\n            <div onScroll={handleScroll} ref={scrollRef} className=\"novel-detail\">\r\n              {\r\n                novelDetail === null\r\n                  ? null\r\n                  : <Fragment>\r\n                    <ServiceButton />\r\n                    <BookItemVertical\r\n                      id={0}\r\n                      tags={novelDetail.tags === null ? [] : novelDetail.tags.split(\"|\")}\r\n                      desc={(\r\n                        <p className={\"subtitle\"}>{buildStatusName(novelDetail.serial_status)} <span\r\n                          className=\"separator\">|</span> {novelDetail.word_count}万字</p>\r\n                      )}\r\n                      img={novelDetail.img}\r\n                      name={novelDetail.title}/>\r\n                    <WingBlank>\r\n                      <p className=\"desc\">{novelDetail.desc}</p>\r\n                    </WingBlank>\r\n                    <WhiteSpace size={\"md\"} style={{background: \"#f1f1f1\"}}/>\r\n\r\n                    <HomeChunk\r\n                      withBorder={true}\r\n                      axis={Axis.vertical}\r\n                      title={\"目录\"}\r\n                      itemsCount={2}\r\n                      itemBuilder={buildDirectory}\r\n                      btn={(\r\n                        <div onClick={() => goToPageWithCustomerId(props.history, {\r\n                          pathname: `/detail/chapter-${id}.html`,\r\n                          state: {reload: true}\r\n                        })}\r\n                             className={\"total-count\"}>共{novelDetail.sections}章 <Icon type={\"right\"}/></div>\r\n                      )}\r\n                    />\r\n                    {\r\n                      novelDetailTopRewardList !== null && novelDetailTopRewardList.length > 0\r\n                        ? <Fragment>\r\n                          <WhiteSpace size={\"md\"} style={{background: \"#f1f1f1\"}}/>\r\n                          <DetailChunk\r\n                            title={\"打赏榜单\"}\r\n                            subtitle={`（收到打赏${novelDetail.reward_coin}书币）`}\r\n                            withBorder={true}>\r\n                            <div className=\"tops\">\r\n                              {\r\n                                novelDetailTopRewardList.slice(0, 3).map((item, index) => {\r\n                                  return (\r\n                                    <div key={index} className={\"top-item\"}>\r\n                                      <div className=\"top-wrapper\">\r\n                                        <div className={topBorderClass(index)}>\r\n                                          <img src={item.user_img} alt=\"\"/>\r\n                                        </div>\r\n                                        {buildHonor(index)}\r\n                                      </div>\r\n                                      <p className=\"username\">{item.username}</p>\r\n                                      <p className=\"desc\">共打赏<span className=\"number\">{item.coin_num}</span>书币</p>\r\n                                    </div>\r\n                                  )\r\n                                })\r\n                              }\r\n                            </div>\r\n                          </DetailChunk>\r\n                        </Fragment>\r\n                        : null\r\n                    }\r\n                    <WhiteSpace size={\"md\"} style={{background: \"#f1f1f1\"}}/>\r\n                    {\r\n                      novelRewardList !== null\r\n                        ? (\r\n                          <DetailChunk\r\n                            btn={\r\n                              <RightBtn\r\n                                onClick={handleReward}\r\n                                color={\"#ffffff\"}\r\n                                backgroundColor={\"#ffc71a\"}\r\n                                title={\"我要打赏\"}\r\n                                icon={(\r\n                                  <div className=\"icon\">\r\n                                    <img src={likeIcon} alt=\"\"/>\r\n                                  </div>\r\n                                )}/>}\r\n                            title={\"打赏记录\"}\r\n                            subtitle={`（收到打赏${novelDetail.reward_coin}书币）`}\r\n                            withBorder={true}>\r\n\r\n                            {\r\n                              novelRewardList.length > 0\r\n                                ? (\r\n                                  <div className=\"records\">\r\n                                    {\r\n                                      novelRewardList.slice(0, 3).map((record, index) => {\r\n                                        return (\r\n                                          <RewardRecord key={index} {...record} />\r\n                                        )\r\n                                      })\r\n                                    }\r\n                                    {\r\n                                      novelRewardList.length > 3\r\n                                        ? <div className=\"view-more\">\r\n                                          <p onClick={() => {\r\n                                            goToPageWithCustomerId(props.history, {pathname: `/detail/reward-list-${novelDetail.id}.html`})\r\n                                          }}>查看更多</p><Icon type={\"right\"}/>\r\n                                        </div>\r\n                                        : null\r\n                                    }\r\n                                  </div>\r\n                                )\r\n                                : <p className={\"no-record\"}>大哥，赏点钱吧</p>\r\n                            }\r\n                          </DetailChunk>\r\n                        )\r\n                        : null\r\n                    }\r\n                    <WhiteSpace size={\"md\"} style={{background: \"#f1f1f1\"}}/>\r\n                    {\r\n                      novelDetailComments !== null\r\n                        ? (\r\n                          <DetailChunk\r\n                            btn={\r\n                              <RightBtn\r\n                                onClick={() => {\r\n                                  goToPageWithCustomerId(props.history, {\r\n                                    pathname: `/detail/do-comment-${novelDetail.id}.html`,\r\n                                    state: {\r\n                                      id: novelDetail.id,\r\n                                      img: novelDetail.img,\r\n                                      title: novelDetail.title,\r\n                                      state: novelDetail.serial_status\r\n                                    }\r\n                                  })\r\n                                }}\r\n                                color={\"#ffffff\"}\r\n                                backgroundColor={\"#9be13b\"}\r\n                                title={\"我要评论\"}\r\n                                icon={(\r\n                                  <div className=\"icon\">\r\n                                    <img src={commentIcon} alt=\"\"/>\r\n                                  </div>\r\n                                )}/>}\r\n                            title={\"精彩书评\"}\r\n                            withBorder={true}>\r\n\r\n                            {\r\n                              novelDetailComments.length > 0\r\n                                ? (\r\n                                  <Fragment>\r\n                                    <div className=\"comments\">\r\n                                      {\r\n                                        novelDetailComments.slice(0, 6).map((comment, index) => {\r\n                                          return buildComments(comment, index);\r\n                                        })\r\n                                      }\r\n                                    </div>\r\n                                    {\r\n                                      novelDetailComments.length > 6\r\n                                        ? <div onClick={() => {\r\n                                          goToPageWithCustomerId(props.history, {pathname: `/detail/comments-${novelDetail.id}.html`})\r\n                                        }} className=\"view-more\">\r\n                                          <p>查看更多</p><Icon type={\"right\"}/>\r\n                                        </div>\r\n                                        : null\r\n                                    }\r\n                                  </Fragment>\r\n                                )\r\n                                : <p className={\"no-record\"}>沙发为你留着呦～</p>\r\n                            }\r\n                          </DetailChunk>\r\n                        )\r\n                        : null\r\n                    }\r\n                  </Fragment>\r\n              }\r\n            </div>\r\n            {\r\n              novelDetail === null\r\n                ? null\r\n                : <BottomButtons isIn={novelDetail.is_bookstore} onJoin={handleJoin} onRead={handleRead}/>\r\n            }\r\n          </Fragment>\r\n        )\r\n      }}/>\r\n    </PageWithNavBar>\r\n  )\r\n};\r\n","import * as React from \"react\";\r\nimport {createRef, useCallback, useEffect, useState} from \"react\";\r\nimport {match, RouteChildrenProps} from \"react-router\";\r\nimport {useDocumentTitle, useTheme} from \"../../hooks/Hooks\";\r\nimport {PageWithNavBar} from \"../common/PageWithNavBar\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport {Button, List, Picker, Toast} from \"antd-mobile\";\r\nimport \"./index.less\";\r\nimport {getNovelChapters, INovelChapter} from \"../../api/NovelApi\";\r\nimport {LoadingState} from \"../common/LoadMoreBottomContent\";\r\nimport {FutureElement} from \"../common/FutrueElement\";\r\nimport {AutoMaticSetLoadingState} from \"../../HOC/AutoMaticSetLoadingState\";\r\nimport {IPersistentProps} from \"../../HOC/PersistentComp\";\r\nimport {goToReadPage} from \"../../utils/RouteUtil\";\r\nimport {arabToChinese} from \"../../utils/Utils\";\r\n\r\ninterface IChapterRouteParams {\r\n  id: number;\r\n}\r\n\r\ninterface INextPageProps {\r\n  onPrev: () => void;\r\n  onNext: () => void;\r\n  currentChapter: IChapter | null;\r\n  handleSelect: (item: number[]) => void;\r\n  data: Array<IChapter>;\r\n  hasNext: boolean;\r\n  hasPrev: boolean;\r\n}\r\n\r\nconst NextPage: React.FC<INextPageProps> = (props) => {\r\n  const theme = useTheme()[0];\r\n\r\n  return (\r\n    <div className=\"next-page\">\r\n      {/*<WingBlank>*/}\r\n      <Button className={props.hasPrev ? \"\" : \"disabled\"} onClick={props.onPrev}>上一页</Button>\r\n      <Button className={props.hasNext ? \"\" : \"disabled\"} onClick={props.onNext}>下一页</Button>\r\n      <Picker\r\n        onOk={props.handleSelect}\r\n        cols={1}\r\n        data={props.data}\r\n        value={[props.currentChapter === null ? -1 : (props.currentChapter as IChapter).value]}>\r\n        <div style={{color: theme.mainColor, fontSize: \".15rem\"}}\r\n             className=\"current-chapter\">{props.currentChapter === null ? \"\" : (props.currentChapter as IChapter).label}</div>\r\n      </Picker>\r\n      {/*</WingBlank>*/}\r\n    </div>\r\n  )\r\n};\r\n\r\ninterface IChapter {\r\n  label: string;\r\n  value: number;\r\n}\r\n\r\nexport interface IChapterPersistentData {\r\n  scrollTop: number;\r\n  totalChapters: number;\r\n  order: \"asc\" | \"desc\";\r\n  currentChapter: IChapter | null;\r\n  directoryList: Array<INovelChapter> | null;\r\n  currentPage: number;\r\n  isFinished: boolean;\r\n  chaptersList: Array<IChapter>;\r\n  needBuySection: number;\r\n}\r\n\r\nexport const Chapter: React.FC<RouteChildrenProps & IPersistentProps> = (props) => {\r\n  useDocumentTitle(\"目录\");\r\n  const theme = useTheme()[0] as Theme;\r\n  const [currentChapter, setCurrentChapter] = useState<IChapter | null>(null);\r\n  const [directoryList, setDirectoryList] = useState<Array<INovelChapter> | null>(null);\r\n  const [loadingState, setLoadingState] = useState(LoadingState.LOADING);\r\n  const [currentPage, setCurrentPage] = useState(1);\r\n  const [isFinished, setIsFinished] = useState(false);\r\n  const [totalChapters, setTotalChapters] = useState(0);\r\n  const [sizePerPage, setSizePerPage] = useState<number | null>(null);\r\n  const [needBuySection, setNeedBuySection] = useState<number>(-1);\r\n  const [chaptersList, setChaptersList] = useState<Array<IChapter>>([]);\r\n  const listViewRef: React.RefObject<HTMLDivElement> = createRef();\r\n  const [shouldLoad, setShouldLoad] = useState(props.persistentData === null);\r\n\r\n  // 排序方式 0 正序 1 倒叙\r\n  const [order, setOrder] = useState<\"asc\" | \"desc\">(\"asc\");\r\n\r\n  const id = (((props.match as match).params) as IChapterRouteParams).id;\r\n\r\n  // 初始化操作\r\n  useEffect(() => {\r\n    if (props.persistentData !== null && !shouldLoad) {\r\n      const data = props.persistentData as IChapterPersistentData;\r\n      setLoadingState(LoadingState.LOADED);\r\n      setIsFinished(data.isFinished);\r\n      setChaptersList(data.chaptersList);\r\n      setCurrentChapter(data.currentChapter);\r\n      setDirectoryList(data.directoryList);\r\n      setCurrentPage(data.currentPage);\r\n      setTotalChapters(data.totalChapters);\r\n      setNeedBuySection(data.needBuySection);\r\n      setOrder(data.order);\r\n      return;\r\n    }\r\n\r\n    if (currentPage === 1 && chaptersList.length === 0 && shouldLoad) {\r\n      AutoMaticSetLoadingState(\r\n        getNovelChapters(id, currentPage, order)\r\n          .then((data) => {\r\n            if (data.err_code === 2000) {\r\n              Toast.fail(data.err_msg, 1.5, undefined, false);\r\n              props.history.go(-1);\r\n              return;\r\n            }\r\n\r\n            // 设置是否无下一页\r\n            if (data.data.last_page === null) {\r\n              setIsFinished(true);\r\n            }\r\n\r\n            // 设置每页大小\r\n            if (data.data.data.length > 0) {\r\n              setSizePerPage(data.data.data.length);\r\n            }\r\n\r\n            // 设置收费章节\r\n            setNeedBuySection(data.data.need_buy_section);\r\n\r\n            // 设置弹出菜单内容\r\n            setChaptersList(createChaptersList(data.data.section_count));\r\n\r\n            // 设置总页数\r\n            setTotalChapters(Math.ceil(data.data.section_count / 100));\r\n\r\n            // 设置内容\r\n            setDirectoryList(data.data.data)\r\n          }),\r\n        (state) => setLoadingState(state)\r\n      )\r\n    }\r\n  }, [setLoadingState, setDirectoryList, currentPage, shouldLoad, order]);\r\n\r\n  useEffect(() => {\r\n    if (listViewRef.current !== null && props.persistentData !== null && !shouldLoad) {\r\n      const data = (props.persistentData) as IChapterPersistentData;\r\n      (listViewRef.current as HTMLDivElement).scrollTop = data.scrollTop;\r\n    }\r\n  }, [listViewRef]);\r\n\r\n  // 加载下一页\r\n  useEffect(() => {\r\n    if ((currentPage !== 1 || chaptersList.length > 0) && shouldLoad) {\r\n      getNovelChapters(id, currentPage, order)\r\n        .then((data) => {\r\n          if (data.err_code === 2000) {\r\n            Toast.fail(data.err_msg, 1.5, undefined, false);\r\n            props.history.go(-1);\r\n            return;\r\n          }\r\n          if (listViewRef.current !== null) {\r\n            (listViewRef.current as HTMLDivElement).scrollTop = 0;\r\n          }\r\n\r\n          if (data.data.last_page === null) {\r\n            setIsFinished(true);\r\n          } else {\r\n            setIsFinished(false);\r\n          }\r\n          setDirectoryList([...data.data.data])\r\n        })\r\n    }\r\n  }, [currentPage, setDirectoryList, shouldLoad, order]);\r\n\r\n  useEffect(() => {\r\n    if (directoryList !== null && directoryList.length > 0) {\r\n      modCurrentChapters();\r\n    }\r\n  }, [directoryList]);\r\n\r\n  const persistData = useCallback(() => {\r\n    const data: IChapterPersistentData = {\r\n      needBuySection, totalChapters, order, directoryList, currentChapter, currentPage, chaptersList, isFinished, scrollTop: 0\r\n    };\r\n\r\n    if (directoryList !== null) {\r\n      data.directoryList = directoryList;\r\n    }\r\n\r\n    if (listViewRef.current !== null) {\r\n      data.scrollTop = (listViewRef.current as HTMLDivElement).scrollTop;\r\n    }\r\n\r\n    props.persistData(data);\r\n  }, [needBuySection, listViewRef, directoryList, currentChapter, currentPage, directoryList, chaptersList, order, totalChapters]);\r\n\r\n  useEffect(() => {\r\n    persistData();\r\n  }, [needBuySection, directoryList, currentChapter, currentPage, directoryList, chaptersList, order, totalChapters]);\r\n\r\n  const handleScroll = useCallback(() => {\r\n    if (listViewRef.current !== null) {\r\n      persistData();\r\n    }\r\n  }, [listViewRef, directoryList]);\r\n\r\n\r\n  const modCurrentChapters = () => {\r\n    const list = directoryList as Array<INovelChapter>;\r\n    let val = list[order === \"asc\" ? list.length - 1 : 0].num;\r\n\r\n    if (val % 100 === 0) {\r\n      val -= 99;\r\n    } else {\r\n      val -= val % 100;\r\n      val += 1;\r\n    }\r\n\r\n    const item = chaptersList.find((_item) => _item.value === val);\r\n    if (item !== undefined) {\r\n      setCurrentChapter(item);\r\n    }\r\n  };\r\n\r\n  const createChaptersList = (num: number) => {\r\n    let all = Math.floor(num / 100);\r\n    let left: number = 0;\r\n    if (num % 100 > 0) {\r\n      left = num % 100;\r\n      ++all;\r\n    }\r\n\r\n    const chapters: Array<IChapter> = [];\r\n    for (let i = 0; i < all; i++) {\r\n      const low = i * 100 + 1;\r\n      const high = (i + 1) * 100;\r\n\r\n      if (i === all - 1 && left > 0) {\r\n        chapters.push({label: `${low}-${low + left}章`, value: low});\r\n      } else {\r\n        chapters.push({label: `${low}-${high}章`, value: low});\r\n      }\r\n    }\r\n\r\n    return chapters;\r\n  };\r\n\r\n  const handleSelect = (item: Array<number>) => {\r\n    console.log(totalChapters);\r\n    const val = item[0];\r\n    const page = order === \"asc\" ? Math.floor(val / 100) + 1 : totalChapters - Math.floor(val / 100);\r\n    setShouldLoad(true);\r\n    setCurrentPage(page);\r\n  };\r\n\r\n  return (\r\n    <PageWithNavBar background={\"#f1f1f1\"} {...props} title={\"目录\"} rightContent={\r\n      <div style={{display: \"flex\", alignItems: \"center\"}} className=\"more-touch\">\r\n        <p onClick={() => {\r\n          setShouldLoad(true);\r\n          setCurrentPage(1);\r\n          setOrder(order === \"asc\" ? \"desc\" : \"asc\");\r\n        }} style={{marginRight: \".05rem\", color: \"#333\"}}>{order === \"asc\" ? \"正序\" : \"倒序\"}</p>\r\n        <img style={{width: \".12rem\", height: \".12rem\"}} src=\"http://novelsys.oss-cn-shenzhen.aliyuncs.com/front/sort_icon.png\" alt=\"\"/>\r\n      </div>\r\n    }>\r\n\r\n      <FutureElement\r\n        showBarrier={true}\r\n        showLoadingDelay={0}\r\n        loadingState={loadingState}\r\n        contentBuilder={() => (\r\n          <div onScroll={handleScroll} ref={listViewRef} className=\"chapter-list\">\r\n            <List>\r\n              {\r\n                directoryList !== null\r\n                  ? (directoryList as Array<INovelChapter>).map((dic) => {\r\n                    return (\r\n                      <List.Item onClick={() => goToReadPage(props.history, dic.novel_id, dic.num, true)} key={dic.num}>\r\n                        <p style={{color: \"#333\", fontSize: \".15rem\"}}>第{arabToChinese(dic.num)}章&nbsp;&nbsp;{dic.title}</p>\r\n                        {\r\n                          dic.num >= needBuySection\r\n                            ? <img style={{width: \".15rem\", height: \".15rem\"}}\r\n                                   src=\"https://novelsys.oss-cn-shenzhen.aliyuncs.com/front/myReward_vip_icon.png\"\r\n                                   alt=\"\"/>\r\n                            : null\r\n                        }\r\n                      </List.Item>\r\n                    )\r\n                  })\r\n                  : null\r\n              }\r\n              {/*<List.Item extra={*/}\r\n              {/*  <div className=\"vip-icon\"><img src={vipIcon} alt=\"\"/></div>*/}\r\n              {/*}>*/}\r\n              {/*  <p style={{color: \"#333\"}}>第一章 林氏医馆</p>*/}\r\n              {/*</List.Item>*/}\r\n            </List>\r\n            {\r\n              directoryList === null\r\n                ? null\r\n                : <NextPage\r\n                  hasNext={\r\n                    order === \"asc\"\r\n                      ? currentPage < totalChapters\r\n                      : currentPage !== 1\r\n                  }\r\n                  hasPrev={\r\n                    order === \"asc\"\r\n                      ? currentPage !== 1\r\n                      : currentPage < totalChapters\r\n                  }\r\n                  handleSelect={handleSelect}\r\n                  data={chaptersList}\r\n                  currentChapter={currentChapter}\r\n                  onPrev={() => {\r\n                    if ((order === \"asc\" && currentPage === 1) || (order === \"desc\" && currentPage >= totalChapters)) {\r\n                      return;\r\n                    }\r\n\r\n                    setShouldLoad(true);\r\n                    setCurrentPage(order === \"asc\" ? currentPage - 1 : currentPage + 1);\r\n                  }}\r\n\r\n                  onNext={() => {\r\n                    if ((order === \"asc\" && currentPage >= totalChapters) || (order === \"desc\" && currentPage === 1)) {\r\n                      return;\r\n                    }\r\n\r\n                    setShouldLoad(true);\r\n                    setCurrentPage(order === \"asc\" ? currentPage + 1 : currentPage - 1);\r\n                  }}/>\r\n            }\r\n          </div>\r\n        )}\r\n      />\r\n    </PageWithNavBar>\r\n  );\r\n\r\n};\r\n","import * as React from \"react\";\r\nimport {useCallback, useEffect, useState} from \"react\";\r\nimport {PageWithNavBar} from \"../common/PageWithNavBar\";\r\nimport {match, RouteChildrenProps} from \"react-router\";\r\nimport {useDocumentTitle, useTheme} from \"../../hooks/Hooks\";\r\nimport {Button, TextareaItem, Toast} from \"antd-mobile\";\r\nimport \"./DoComment.less\";\r\nimport {Theme} from \"../../entities/Theme\";\r\nimport {doComment} from \"../../api/NovelApi\";\r\nimport {goToPageWithCustomerId} from \"../../utils/RouteUtil\";\r\nimport {ResponseCode} from \"../../utils/ResponseCode\";\r\n\r\nexport interface IDoCommentParamsType {\r\n  id: number;\r\n}\r\n\r\nexport interface IDoCommentStateType {\r\n  id: number;\r\n  img: string;\r\n  title: string;\r\n  state: number;\r\n}\r\n\r\nconst getStatusText = (status: number) => {\r\n  switch (status) {\r\n    case 1:\r\n      return \"连载中\";\r\n    case 2:\r\n      return \"已完结\";\r\n    case 3:\r\n      return \"限时免费\";\r\n  }\r\n\r\n  return null;\r\n};\r\n\r\nexport const DoComment: React.FC<RouteChildrenProps> = (props) => {\r\n  useDocumentTitle(\"评论\");\r\n  const id = ((props.match as match).params as IDoCommentParamsType).id;\r\n  // const data = props.location.state as IDoCommentStateType;\r\n  const theme = useTheme()[0] as Theme;\r\n  const [currentSize, setCurrentSize] = useState(0);\r\n  const [currentInput, setCurrentInput] = useState(\"\");\r\n\r\n  useEffect(() => {\r\n    setCurrentSize(currentInput.length);\r\n  }, [setCurrentSize, currentInput]);\r\n\r\n  const handleDoComment = useCallback(() => {\r\n    const data = (props.match as match).params as {[propName: string]: any};\r\n    console.log(data);\r\n\r\n    if (currentInput.trim().length <= 0) {\r\n      Toast.fail(\"请填写书评\", 1.5, undefined, false);\r\n      return;\r\n    }\r\n\r\n    doComment(data.id, currentInput)\r\n      .then((data) => {\r\n        if (data.err_code === ResponseCode.SUCCESS) {\r\n          Toast.success(\"书评已发布\", 1.5, undefined, false);\r\n          setTimeout(() => {\r\n            props.history.go(-1);\r\n          }, 1500);\r\n        }\r\n      });\r\n  }, [currentInput]);\r\n\r\n  // if (data === undefined) {\r\n  //   goToPageWithCustomerId(props.history, {pathname: `/detail/novel-${id}.html`});\r\n  //   return null;\r\n  // }\r\n\r\n  return (\r\n    <PageWithNavBar {...props} title={\"评论\"} rightContent={\r\n      <p style={{color: theme.mainColor}}\r\n         onClick={() => {\r\n           handleDoComment();\r\n         }}\r\n         className={\"more-touch\"}>发布</p>\r\n    }>\r\n      <div className=\"do-comment-container\">\r\n        {/*<div className=\"header\">*/}\r\n        {/*  <img src={data.img} alt=\"\"/>*/}\r\n        {/*  <div className=\"middle\">*/}\r\n        {/*    <p className=\"title\">{data.title}</p>*/}\r\n        {/*    <p className={\"status\"}>{getStatusText(data.state)}</p>*/}\r\n        {/*  </div>*/}\r\n        {/*  <div className=\"right\">*/}\r\n        {/*    <Button onClick={handleDoComment} style={{background: theme.mainColor}} type={\"primary\"}>发布书评</Button>*/}\r\n        {/*  </div>*/}\r\n        {/*</div>*/}\r\n        <div className=\"content\">\r\n          <div className=\"title\">\r\n            <span>书评</span>\r\n            <span>{currentSize}/150</span>\r\n          </div>\r\n          <TextareaItem\r\n            rows={0}\r\n            className={\"input-field\"}\r\n            maxLength={150}\r\n            value={currentInput}\r\n            onChange={(str) => {\r\n              if ((str as string).length <= 150) {\r\n                setCurrentInput(str as string)\r\n              } else {\r\n                Toast.fail(\"超过字数限制\", 1.5, undefined, false);\r\n              }\r\n            }}\r\n            placeholde