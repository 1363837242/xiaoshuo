, setCurrentPage, currentPage]);\r\n\r\n  useEffect(() => {\r\n    getNovelRewardList(match.id, currentPage, \"created_at\")\r\n      .then((data) => {\r\n        if (data.data.last_page === null) {\r\n          setLoadingState(LoadingState.FINISHED);\r\n        }\r\n        setList([...list, ...data.data.data]);\r\n      });\r\n\r\n  }, [currentPage, setLoadingState]);\r\n\r\n  return (\r\n    <PageWithNavBar {...props} title={\"打赏记录\"} rightContent={\r\n      <p\r\n        style={{color: theme.mainColor}}\r\n        onClick={() => goToPageWithCustomerId(props.history, {pathname: \"/index.html\", state: {reload: true}})}\r\n        className={\"more-touch\"}\r\n      >\r\n        首页\r\n      </p>\r\n    }>\r\n      <LoadMoreList className={\"comments-list\"} onLoadMore={handleLoadMore} itemCount={list.length} itemBuilder={(index) => (\r\n        <RewardRecord key={index} {...list[index]} />\r\n      )}>\r\n        <LoadMoreBottomContent status={loadingState}/>\r\n      </LoadMoreList>\r\n    </PageWithNavBar>\r\n  );\r\n};\r\n","import * as React from \"react\";\r\nimport {match, Route, RouteChildrenProps, Switch} from \"react-router\";\r\nimport {NovelDetail} from \"./NovelDetail\";\r\nimport {Chapter} from \"../chapter\";\r\nimport {PersistentComp} from \"../../HOC/PersistentComp\";\r\nimport {DoComment} from \"./DoComment\";\r\nimport {Comments} from \"./Comments\";\r\nimport {RewardList} from \"./RewardList\";\r\nimport {NeedLogin} from \"../../HOC/User\";\r\n\r\ninterface IRouteParams {\r\n  type: string;\r\n  id: number;\r\n}\r\n\r\nexport const Detail: React.FC<RouteChildrenProps> = (props) => {\r\n  const params = ((props.match as match).params as IRouteParams);\r\n\r\n  return (\r\n    <Switch>\r\n      <Route path={\"/detail/novel-:id.html\"} children={(router) => (\r\n        <NeedLogin {...router}>\r\n          <PersistentComp {...router} component={NovelDetail}/>\r\n        </NeedLogin>\r\n      )}/>\r\n      <Route path={\"/detail/chapter-:id.html\"} 